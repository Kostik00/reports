    <TfrxERQuery Name="SMZData" UserName="SMZData" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="TabelFrxDS" IgnoreDupParams="False" SQL.Text="DECLARE @DateFrom datetime&#13;&#10;DECLARE @DateTo datetime&#13;&#10;&#13;&#10;SET @DateFrom = :DateFrom  &#13;&#10;SET @DateTo   = :DateTo  &#13;&#10;SELECT     &#13;&#10;	Podr.Name_Sub AS Pord, &#13;&#10;	PodrVer.F_Name_Sub AS PodrName, &#13;&#10;	Pr_Pasp.Cod_Zak, &#13;&#10;	S_Subjects.Name_Sub, &#13;&#10;	S_SubVer.F_Name_Sub, &#13;&#10;	Pr_Tabel_AC.Key_Sub_Ver, &#13;&#10;	SUM(Pr_Tabel_AC.T_AC) AS T_AC, &#13;&#10;	T_Oper.Name_K_Oper&#13;&#10;FROM         &#13;&#10;	Pr_Tabel &#13;&#10;	INNER JOIN S_SubVer AS PodrVer &#13;&#10;		ON Pr_Tabel.Key_Sub_Ver = PodrVer.Key_Sub_Ver &#13;&#10;	INNER JOIN S_Subjects AS Podr &#13;&#10;		ON PodrVer.Key_Sub = Podr.Key_Sub &#13;&#10;	INNER JOIN Pr_VTabel_MK AS Pr_Tabel_MK &#13;&#10;		ON Pr_Tabel.Key_Tabel = Pr_Tabel_MK.Key_Tabel &#13;&#10;	INNER JOIN Pr_Tabel_AC &#13;&#10;		ON Pr_Tabel_MK.Key_Tabel_MK = Pr_Tabel_AC.Key_Tabel_MK &#13;&#10;	INNER JOIN S_SubVer &#13;&#10;		ON S_SubVer.Key_Sub_Ver = Pr_Tabel_AC.Key_Sub_Ver &#13;&#10;	INNER JOIN S_Subjects &#13;&#10;		ON S_SubVer.Key_Sub = S_Subjects.Key_Sub &#13;&#10;	INNER JOIN Pr_Pasp_Rows &#13;&#10;		ON Pr_Tabel_MK.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows &#13;&#10;	INNER JOIN T_Oper &#13;&#10;		ON T_Oper.Cod_Oper = Pr_Pasp_Rows.Cod_Oper &#13;&#10;	INNER JOIN Pr_Pasp &#13;&#10;		ON Pr_Pasp_Rows.Key_Pasport = Pr_Pasp.Key_Pasport &#13;&#10;	INNER JOIN T_VPredm &#13;&#10;		ON Pr_Pasp.Key_Predm = T_VPredm.Key_Predm&#13;&#10;WHERE &#13;&#10;--		Pr_Tabel.Date_Tabel    BETWEEN @DateFrom AND @DateTo AND &#13;&#10;	   Pr_Tabel_AC.DateAc    BETWEEN @DateFrom AND  @DateTo                                           &#13;&#10;	   &#13;&#10;	   &#13;&#10;GROUP BY &#13;&#10;	Podr.Name_Sub, PodrVer.F_Name_Sub, &#13;&#10;	Pr_Pasp.Cod_Zak, &#13;&#10;	S_Subjects.Name_Sub, &#13;&#10;	S_SubVer.F_Name_Sub, &#13;&#10;	Pr_Tabel_AC.Key_Sub_Ver, &#13;&#10;	T_Oper.Name_K_Oper&#13;&#10;ORDER BY &#13;&#10;	PodrName, S_Subjects.Name_Sub, S_SubVer.F_Name_Sub, Pr_Tabel_AC.Key_Sub_Ver, Pr_Pasp.Cod_Zak&#13;&#10;  &#13;&#10;                               " PropData=""/>
