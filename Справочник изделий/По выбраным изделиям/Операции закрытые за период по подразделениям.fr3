    <TfrxERQuery Name="qTempVKZ" UserName="dsTempVKZ" CloseDataSource="False" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;    [Key_VKZ]&#13;&#10;  FROM&#13;&#10;    [#VKZ];" PropData="05704C6566740378010470546F7002280A506172616D65746572730100"/>
    <TfrxERQuery Name="qTOper3" UserName="dsTOper3" CloseDataSource="False" FieldAliases.Text="Cod_Oper=Cod_Oper&#13;&#10;Name_Oper=Name_Oper&#13;&#10;Cod_KLOB=Cod_KLOB" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;    [Cod_Oper],&#13;&#10;    [Name_Oper],&#13;&#10;    [Cod_KLOB]&#13;&#10;  FROM&#13;&#10;    [T_Oper]&#13;&#10;  WHERE&#13;&#10;    [Cod_Oper] = :CodOper                  " PropData="05704C6566740344010470546F7003CC000A506172616D657465727301010C23000000204E616D653D22436F644F706572222044617461547970653D226674537472696E67220000"/>
    <TfrxERQuery Name="qTrud" UserName="dsTrud" CloseDataSource="False" FieldAliases.Text="RowNumber=RowNumber&#13;&#10;KeyVKZ=KeyVKZ&#13;&#10;Cod_Zak=Cod_Zak&#13;&#10;Cod_Predm=Cod_Predm&#13;&#10;Name_Predm=Name_Predm&#13;&#10;Cnt=Cnt&#13;&#10;AcTrud=AcTrud&#13;&#10;AcTrud1=AcTrud1&#13;&#10;AcTrud2=AcTrud2" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="DECLARE @DateFrom datetime;&#13;&#10;DECLARE @DateTo   datetime;    &#13;&#10;  &#13;&#10;SET @DateFrom = :DateFrom;         &#13;&#10;SET @DateTo   = :DateTo;         &#13;&#10;&#13;&#10;WITH PrPaspAc (Key_Pasp_Rows, SumAcKolvo) AS&#13;&#10;   (SELECT Pr_Pasp_Ac.Key_Pasp_Rows, SUM(Pr_Pasp_Ac.AcKolvo) AS SumAcKolvo&#13;&#10;    FROM&#13;&#10;      Pr_Pasp_Ac&#13;&#10;    WHERE Pr_Pasp_Ac.AC_Date &#62;= @DateFrom AND Pr_Pasp_Ac.AC_Date &#60;= @DateTo&#13;&#10;    GROUP BY  Pr_Pasp_Ac.Key_Pasp_Rows)&#13;&#10;&#13;&#10;SELECT&#13;&#10;    [V].RowNumber,      &#13;&#10;    PVKZ2.Key_VKZ AS KeyVKZ,&#13;&#10;    [V].Cod_Zak,&#13;&#10;    [V].Cod_Predm,&#13;&#10;    [V].Name_Predm,&#13;&#10;    COUNT(Pr_Pasp_Rows.Key_Pasp_Rows) AS Cnt, &#13;&#10;    SUM(Pr_Pasp_Rows.Tsm * PrPaspAc.SumAcKolvo/Pr_Pasp.PaspKolvo*Pr_PaspDet.KolvoZap/Pr_Pasp.PaspKolvo)&#13;&#10;           AS AcTrud,&#13;&#10;    SUM(&#13;&#10;      CASE&#13;&#10;      WHEN Pr_Pasp_Rows.Key_TPRows is not null THEN Pr_Pasp_Rows.Tsm * PrPaspAc.SumAcKolvo/Pr_Pasp.PaspKolvo*Pr_PaspDet.KolvoZap/Pr_Pasp.PaspKolvo&#13;&#10;                                               ELSE 0.0&#13;&#10;      END) AS AcTrud1,&#13;&#10;    SUM(&#13;&#10;      CASE&#13;&#10;      WHEN Pr_Pasp_Rows.Key_TPRows is null     THEN Pr_Pasp_Rows.Tsm * PrPaspAc.SumAcKolvo/Pr_Pasp.PaspKolvo*Pr_PaspDet.KolvoZap/Pr_Pasp.PaspKolvo&#13;&#10;                                               ELSE 0.0&#13;&#10;      END) AS AcTrud2&#13;&#10;  FROM  &#13;&#10;    PrPaspAc &#13;&#10;      INNER JOIN Pr_Pasp_Rows   ON Pr_Pasp_Rows.Key_Pasp_Rows = PrPaspAc.Key_Pasp_Rows  &#13;&#10;      INNER JOIN #Oper   AS OPR ON OPR.Cod_Oper = Pr_Pasp_Rows.Cod_Oper AND OPR.Cod_KLOB = 'Y'&#13;&#10;      INNER JOIN Pr_Pasp	       ON Pr_Pasp.Key_Pasport = Pr_Pasp_Rows.Key_Pasport&#13;&#10;      INNER JOIN Pr_PaspDet	    ON Pr_PaspDet.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;      INNER JOIN P_VKZ AS PVKZ1 ON PVKZ1.Key_VKZ = Pr_PaspDet.Key_VKZ	    &#13;&#10;      INNER JOIN P_VKZ AS PVKZ2 ON PVKZ2.Key_VKZ = PVKZ1.Key_VKZ_Izdel AND PVKZ2.Key_VKZ_Master IS NULL&#13;&#10;      INNER JOIN [#VKZ] AS [V]  ON [V].Key_VKZ = PVKZ2.Key_VKZ &#13;&#10;  WHERE&#13;&#10;    Pr_Pasp.PaspKolvo &#62; 0&#13;&#10;  GROUP BY&#13;&#10;    [V].RowNumber,      &#13;&#10;    PVKZ2.Key_VKZ,&#13;&#10;    [V].Cod_Zak,&#13;&#10;    [V].Cod_Predm,&#13;&#10;    [V].Name_Predm  &#13;&#10;  ORDER BY&#13;&#10;    [V].RowNumber ASC;                 " PropData=""/>
