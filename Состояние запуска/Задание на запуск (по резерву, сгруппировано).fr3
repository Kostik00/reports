    <TfrxERQuery Name="OstQRY" UserName="Остаток" CloseDataSource="False" FieldAliases.Text="Name_Pasp_Place=Name_Pasp_Place&#13;&#10;Name_Zagotov=Name_Zagotov&#13;&#10;ProfRazm=ProfRazm&#13;&#10;M_Zag=M_Zag&#13;&#10;Ost=Ost&#13;&#10;KolvoSm=KolvoSm&#13;&#10;Key_Sub_Ver_To=Key_Sub_Ver_To&#13;&#10;Key_ID_Mat=Key_ID_Mat&#13;&#10;Num_Nacl=Num_Nacl&#13;&#10;Data_Nacl=Data_Nacl&#13;&#10;Osnovanie=Osnovanie&#13;&#10;Name_Sub=Name_Sub" BCDToCurrency="False" Master="qMaster" IgnoreDupParams="False" SQL.Text="SELECT     &#13;&#10;       Isnull(Pr_Pasp_Place.Cod_Pasp_Place,'-') Name_Pasp_Place, &#13;&#10;       T_Zagotov.Name_Zagotov, &#13;&#10;       A_Nacl_Str_Sost.ProfRazm, &#13;&#10;       A_Nacl_Str_Sost.M_Zag,&#13;&#10;       A_Nacl_Str_Sost.Kolvo - A_Nacl_Str_Sost.Rashod AS Ost, &#13;&#10;       A_Nacl_Str_Sost.M_Zag * (A_Nacl_Str_Sost.Kolvo - A_Nacl_Str_Sost.Rashod) AS KolvoSm,&#13;&#10;       A_Nacl.Key_Sub_Ver_To,                      &#13;&#10;       A_Nacl_Str.Key_ID_Mat,  &#13;&#10;       A_Nacl.Num_Nacl,&#13;&#10;       A_Nacl.Data_Nacl,&#13;&#10;       A_Nacl.Osnovanie,&#13;&#10;	   S_ALL_Sub.Name_Sub&#13;&#10;FROM&#13;&#10;		A_Ostatok_Mat&#13;&#10;		INNER JOIN S_ALL_Sub&#13;&#10;			ON S_ALL_Sub.Key_Sub_Ver = A_Ostatok_Mat.Key_Sub_Ver&#13;&#10;			&#13;&#10;		INNER JOIN  A_Nacl&#13;&#10;			ON A_Nacl.Key_Sub_Ver_To= A_Ostatok_Mat.Key_Sub_Ver&#13;&#10;		INNER JOIN A_Nacl_Str &#13;&#10;			ON A_Nacl_Str.Key_Nacl = A_Nacl.Key_Nacl&#13;&#10;			AND A_Nacl_Str.Key_ID_Mat= A_Ostatok_Mat.Key_ID_Mat&#13;&#10;		INNER JOIN A_Nacl_Str_Sost&#13;&#10;			ON A_Nacl_Str_Sost.Key_Nacl_Str = A_Nacl_Str.Key_Nacl_Str &#13;&#10;		INNER JOIN T_Zagotov &#13;&#10;			ON A_Nacl_Str_Sost.Cod_Zagotov = T_Zagotov.Cod_Zagotov&#13;&#10;		LEFT JOIN Pr_Pasp_Place &#13;&#10;			ON Pr_Pasp_Place.Key_Pasp_Place = A_Nacl_Str_Sost.Key_Pasp_Place&#13;&#10;&#13;&#10;                 &#13;&#10;WHERE&#13;&#10;       A_Ostatok_Mat.Key_ID_Mat= :Key_ID_Mat                                 &#13;&#10;            