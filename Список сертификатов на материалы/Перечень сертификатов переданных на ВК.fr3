    <TfrxERQuery Name="SertList" UserName="Сертификаты" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT        &#13;&#10;	T_Sert.Key_Sert, &#13;&#10;	T_Sert.VidSert, &#13;&#10;	T_Sert.N_Sert, &#13;&#10;	T_Sert.Date_Sert, &#13;&#10;	T_Sert.RegN_Sert, &#13;&#10;	T_Sert.Key_Sub_Ver, &#13;&#10;	T_Sert.Date_Sert_Prn, &#13;&#10;	T_Sert.Key_SostSert, &#13;&#10;	T_Sert.Ad_Info, &#13;&#10;	T_Sert.Date_Reg, &#13;&#10;	T_Sert.Key_Sub_Ver_Postav, &#13;&#10;	T_Sert.Key_Sert_Razdel, &#13;&#10;	T_Sert.Status_Postav, &#13;&#10;	T_Sert.Date_SostSert, &#13;&#10;	T_Sert.Date_RegN, &#13;&#10;	T_Sert.Date_OTK, &#13;&#10;	T_Sert.Key_Sub_Ver_OTK, &#13;&#10;&#13;&#10;	Proizv.Name_Sub Name_Sub_Proizv, &#13;&#10;	Proizv.F_Name_Sub F_Name_Sub_Proizv, &#13;&#10;&#13;&#10;	Postav.Name_Sub Name_Sub_Postav, &#13;&#10;	Postav.F_Name_Sub F_Name_Sub_Postav,&#13;&#10;&#13;&#10;	TblNacl.*,&#13;&#10;&#13;&#10;	T_SertPos.Key_Sert_Pos,&#13;&#10;	T_SertPos.N_SertPos,&#13;&#10;	T_SertPos.Cod_Plavka,&#13;&#10;	T_SertPos.Party,&#13;&#10;&#13;&#10;	dbo.PrintNamePredm(T_VPredm.Printflag, T_VPredm.Cod_Predm, T_VPredm.Name_Predm,&#13;&#10;						T_VPredm.Sortam, T_VPredm.Type_NTD_Sortam, T_VPredm.Name_NTD_Sortam,&#13;&#10;						T_VPredm.Marka, T_VPredm.Type_NTD_Marka, T_VPredm.Name_NTD_Marka) FNamePredm,&#13;&#10;&#13;&#10;	T_Sert_Razdel.Name_Sert_Razdel&#13;&#10;&#13;&#10;FROM            &#13;&#10;	T_Sert &#13;&#10;&#13;&#10;	INNER JOIN T_Sert_Razdel&#13;&#10;		ON T_Sert_Razdel.Key_Sert_Razdel = T_Sert.Key_Sert_Razdel&#13;&#10;&#13;&#10;	INNER JOIN T_SertPos&#13;&#10;		ON T_SertPos.Key_Sert = T_Sert.Key_Sert&#13;&#10;&#13;&#10;	INNER JOIN T_VPredm&#13;&#10;		ON T_VPredm.Key_Predm = T_SertPos.Key_Predm&#13;&#10;&#13;&#10;	INNER JOIN S_ALL_Sub AS Proizv &#13;&#10;		ON T_Sert.Key_Sub_Ver = Proizv.Key_Sub_Ver &#13;&#10;	INNER JOIN S_ALL_Sub AS Postav &#13;&#10;		ON T_Sert.Key_Sub_Ver_Postav = Postav.Key_Sub_Ver&#13;&#10;&#13;&#10;	LEFT JOIN &#13;&#10;	(&#13;&#10;&#13;&#10;&#13;&#10;SELECT &#13;&#10;	&#13;&#10;	A_Nacl_Str.Key_ID_Mat,&#13;&#10;	ROW_NUMBER()   OVER (PARTITION BY Key_ID_Mat ORDER BY A_Nacl.Key_Nacl) RN,&#13;&#10;	MolFrom.Name_Sub Name_Sub_Mol_From,&#13;&#10;	MolTo.Name_Sub Name_Sub_MOL_To,&#13;&#10;	SubFrom.Name_Sub&#13;&#10;FROM &#13;&#10;	A_Nacl_Str&#13;&#10;	INNER JOIN A_Nacl&#13;&#10;		ON A_Nacl.Key_Nacl = A_Nacl_Str.Key_Nacl&#13;&#10;&#13;&#10;	INNER JOIN S_ALL_Sub SubFrom&#13;&#10;		ON SubFrom.Key_Sub_Ver = A_Nacl.Key_Sub_Ver_From&#13;&#10;		AND SubFrom.Key_Class = 2&#13;&#10;&#13;&#10;	INNER JOIN S_ALL_Sub SubTo&#13;&#10;		ON SubTo.Key_Sub_Ver = A_Nacl.Key_Sub_Ver_To&#13;&#10;		 &#13;&#10;	LEFT JOIN S_ALL_Sub MolFrom&#13;&#10;		ON MolFrom.Key_Sub_Ver = A_Nacl.Key_Sub_Ver_MOL_From&#13;&#10;	LEFT JOIN S_ALL_Sub MOLTo&#13;&#10;		ON MOLTo.Key_Sub_Ver = A_Nacl.Key_Sub_Ver_MOL_To&#13;&#10;&#13;&#10;	) TblNacl&#13;&#10;	ON TblNacl.Key_ID_Mat = T_SertPos.Key_ID_Mat&#13;&#10;	AND TblNacl.RN=1&#13;&#10;&#13;&#10;&#13;&#10;WHERE        &#13;&#10;		(T_Sert.Date_RegN BETWEEN :DateFrom AND :DateTo) &#13;&#10;  	AND  (T_Sert.Key_Sub_Ver_OTK IS NULL)   &#13;&#10;&#13;&#10;AND 	T_Sert.RegN_Sert like '%/21'&#13;&#10;AND T_Sert.Key_SostSert= 0                                                                   &#13;&#10;&#13;&#10;ORDER BY &#13;&#10;	T_Sert.Key_Sert_Razdel,&#13;&#10;	dbo.T_OrderCod_Predm(T_Sert.RegN_Sert), T_Sert.Key_Sert, dbo.T_OrderSortam(T_SertPos.N_SertPos)  " PropData=""/>
