<TfrxReport Version="6.9.6" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="41183,6439494097" ReportOptions.Description.Text="" ReportOptions.Name="Операции закрытые за период по подразделениям.fr3 Справочник изделий" ReportOptions.LastChange="44623,6936274769" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  vYear, vMonth, vDay: Word;&#13;&#10;  vLineDecrement: Integer;&#13;&#10;  sVKZ: String;&#13;&#10;  iRowNumber: Integer = 0;&#13;&#10;&#13;&#10;function MySmPercent(P, F: double): double;&#13;&#10;begin&#13;&#10;  Result := 0;&#13;&#10;  if  P &#62; 0 then&#13;&#10;    Result := F * 100 / P;&#13;&#10;end;&#13;&#10;&#13;&#10;function FnPercent(F, FSm: double): double;&#13;&#10;begin&#13;&#10;  Result := 0;&#13;&#10;  if (FSm&#62;0) then&#13;&#10;       Result:=100*F/FSm;&#13;&#10;end;&#13;&#10;&#13;&#10;function Capitalize(AString: String): String;&#13;&#10;begin&#13;&#10;  Result:=Uppercase(Copy(AString, 1, 1))+Copy(AString, 2, Length(AString)-1);&#13;&#10;end;&#13;&#10;&#13;&#10;function CapitalizeLowercase(AString: String): String;&#13;&#10;var sTemp: String;&#13;&#10;begin&#13;&#10;  sTemp:=Lowercase(Trim(AString));&#13;&#10;  Result:=Uppercase(Copy(sTemp, 1, 1))+Copy(sTemp, 2, Length(sTemp)-1);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure  SetCheckedInList;&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;begin&#13;&#10;{The Operations}&#13;&#10;  for i:=0 to cblOpList.Items.Count-1&#13;&#10;    do begin&#13;&#10;        if qTOper3.Active then qTOper3.Close;&#13;&#10;        qTOper3.ParamByName('CodOper').Value := Copy(cblOpList.Items.Strings(i), 2, 4);&#13;&#10;        qTOper3.Open;&#13;&#10;        case qTOper3.RecordCount of&#13;&#10;          0: ShowMessage('Нет записей в &#34;qTOper3&#34;!');&#13;&#10;          1: if qTOper3.FieldByName('Cod_KLOB').AsString = 'N'&#13;&#10;               then cblOpList.Checked(i):=True&#13;&#10;               else cblOpList.Checked(i):=False;&#13;&#10;          else ShowMessage('Записей в &#34;qTOper3&#34; = '+IntToStr(qTOper3.RecordCount));&#13;&#10;        end;&#13;&#10;        qTOper3.Close;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure FillList;&#13;&#10;var&#13;&#10;  sTemp: String;&#13;&#10;begin&#13;&#10;{The Operations}&#13;&#10;  qTOper.Open;&#13;&#10;  qTOper.First;&#13;&#10;  while not qTOper.Eof&#13;&#10;    do begin&#13;&#10;      sTemp:=qTOper.FieldByName('Cod_Oper').AsString;&#13;&#10;      Case Length(sTemp) of&#13;&#10;        1: sTemp:='   '+sTemp;&#13;&#10;        2: sTemp:='  '+sTemp;&#13;&#10;        3: sTemp:=' '+sTemp;&#13;&#10;        end;&#13;&#10;      cblOpList.Items.Add('('+sTemp+') '+CapitalizeLowercase(qTOper.FieldByName('Name_Oper').AsString));&#13;&#10;      if qTOper.FieldByName('Cod_KLOB').AsString = 'N'&#13;&#10;        then cblOpList.Checked(cblOpList.Items.Count-1):=True&#13;&#10;        else cblOpList.Checked(cblOpList.Items.Count-1):=False;&#13;&#10;      qTOper.Next;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ShowOperGr;&#13;&#10;begin&#13;&#10;  ShowMessage('Begin procedure ShowOperGr!');&#13;&#10;  qTOperGrR2.Close;&#13;&#10;  qTOperGrR2.Open;&#13;&#10;  qTOperGrR2.First;&#13;&#10;  ShowMessage('Begin While! RecordCount = '+IntToStr(qTOperGrR2.RecordCount));&#13;&#10;  while not qTOperGrR2.Eof&#13;&#10;    do begin&#13;&#10;      ShowMessage('('+qTOperGrR2.FieldByName('Cod_Oper_Gr').AsString+') '+qTOperGrR2.FieldByName('Name_GrGr').AsString);&#13;&#10;      qTOperGrR2.Next;&#13;&#10;    end;&#13;&#10;  ShowMessage('End While');&#13;&#10;  ShowMessage('End procedure ShowOperGr!');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SetCheckedInOper;&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;begin&#13;&#10;  for i:=0 to cblOpList.Items.Count-1&#13;&#10;    do begin&#13;&#10;      if cblOpList.Checked(i)&#13;&#10;        then begin&#13;&#10;          qPreUpdate.ParamByName('CodOper').Value:=Trim(Copy(cblOpList.Items.Strings(i), 2, 4));&#13;&#10;          qPreUpdate.Open;&#13;&#10;          qPreUpdate.Close;&#13;&#10;        end;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPageSetSize;&#13;&#10;begin&#13;&#10;  btCancel.Top:=100;&#13;&#10;  btOk.Top:=100;&#13;&#10;  DialogPage.Height:=160;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPageVisibleListSetSize;&#13;&#10;begin&#13;&#10;  btCancel.Top:=344;&#13;&#10;  btOk.Top:=344;&#13;&#10;  DialogPage.Height:=404;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPageOnActivate(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  s: String;&#13;&#10;begin&#13;&#10;  FillList;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPageOnDeactivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if DialogPage.ModalResult = mrOk&#13;&#10;    then begin&#13;&#10;      qPreCreate.Open;&#13;&#10;      qPreCreate.Close;&#13;&#10;      SetCheckedInOper&#13;&#10;    end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btFirstOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cblOpList.ItemIndex:=0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btPreviousOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;begin&#13;&#10;  for i:=cblOpList.ItemIndex-1 downto 0&#13;&#10;    do if cblOpList.Checked(i)&#13;&#10;         then begin&#13;&#10;           cblOpList.ItemIndex:=i;&#13;&#10;           Break;&#13;&#10;         end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btNextOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;begin&#13;&#10;  for i:=cblOpList.ItemIndex+1 to cblOpList.Items.Count-1&#13;&#10;    do if cblOpList.Checked(i)&#13;&#10;         then begin&#13;&#10;           cblOpList.ItemIndex:=i;&#13;&#10;           Break;&#13;&#10;         end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btLastOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cblOpList.ItemIndex:=cblOpList.Items.Count-1;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btClearOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;begin&#13;&#10;  for i:=0 to cblOpList.Items.Count-1 do cblOpList.Checked(i):=False;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btCheckAllOnClick(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;begin&#13;&#10;  for i:=0 to cblOpList.Items.Count-1 do cblOpList.Checked(i):=True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btRestoreOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SetCheckedInList;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SetDoubleColumnCellColor(Sender: TfrxComponent; AColor: TColor);&#13;&#10;begin&#13;&#10;  if ((qTOper2.RecNo+1) mod 4 = 3) or ((qTOper2.RecNo+1) mod 4 = 0)&#13;&#10;    then&#13;&#10;      TfrxMemoView(Sender).Color:=AColor&#13;&#10;    else&#13;&#10;      TfrxMemoView(Sender).Color:=clNone;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SetMasterColor(Sender: TfrxComponent; AColor: TColor);&#13;&#10;var i: Integer;&#13;&#10;begin&#13;&#10;  for i:=0 to TfrxMasterData(Sender).Objects.Count-1&#13;&#10;  do begin&#13;&#10;    if TfrxMasterData(Sender).Objects.Items(i) is TfrxMemoView&#13;&#10;    then TfrxMemoView(TfrxMasterData(Sender).Objects.Items(i)).Color:=AColor;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SetDataBandColor(Sender: TfrxComponent; AColor: TColor);&#13;&#10;var i: Integer;&#13;&#10;begin&#13;&#10;  for i:=0 to TfrxDataBand(Sender).Objects.Count-1&#13;&#10;  do begin&#13;&#10;    if TfrxDataBand(Sender).Objects.Items(i) is TfrxMemoView&#13;&#10;    then TfrxMemoView(TfrxDataBand(Sender).Objects.Items(i)).Color:=AColor;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo36OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SetDoubleColumnCellColor(Sender, $F0F0F0);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo39OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SetDoubleColumnCellColor(Sender, $F0F0F0);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo40OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SetDoubleColumnCellColor(Sender, $F0F0F0);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo41OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SetDoubleColumnCellColor(Sender, $F0F0F0);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbOpListOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Panel1.Visible:=cbOpList.Checked;&#13;&#10;  if cbOpList.Checked&#13;&#10;    then DialogPageVisibleListSetSize&#13;&#10;    else DialogPageSetSize;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if MasterData2.DataSet.RecNo mod 2 = 1&#13;&#10;    then SetDataBandColor(Sender, $F0F0F0)&#13;&#10;    else SetDataBandColor(Sender, clNone);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  qTOper2.Close;&#13;&#10;  qTOper2.Open;&#13;&#10;  if qTOper2.RecordCount = 0&#13;&#10;    then ReportSummary1.Visible:=True&#13;&#10;    else ReportSummary1.Visible:=False;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure mdVKZInsertOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Inc(iRowNumber, 1);&#13;&#10;  qVKZInsert.ParamByName('RowNumber').Value:=iRowNumber;&#13;&#10;  qVKZInsert.ParamByName('KeyVKZ').Value:=&#60;MainDS.&#34;Key_VKZ&#34;&#62;;&#13;&#10;  qVKZInsert.ParamByName('Cod_Zak').Value:=&#60;MainDS.&#34;Cod_Zak&#34;&#62;;&#13;&#10;  qVKZInsert.ParamByName('Cod_Predm').Value:=&#60;MainDS.&#34;Cod_Predm&#34;&#62;;&#13;&#10;  qVKZInsert.ParamByName('Name_Predm').Value:=&#60;MainDS.&#34;Name_Predm&#34;&#62;;&#13;&#10;  qVKZInsert.Open;&#13;&#10;  qVKZInsert.Close;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if MasterData1.DataSet.RecNo mod 2 = 1&#13;&#10;    then SetDataBandColor(Sender, $F0F0F0)&#13;&#10;    else SetDataBandColor(Sender, clNone);&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  sVKZ:='';&#13;&#10;  vLineDecrement:=0;&#13;&#10;  DialogPageSetSize;&#13;&#10;  DecodeDate( Now, vYear, vMonth, vDay);&#13;&#10;  if vMonth &#62; 1&#13;&#10;    then begin {Если месяц позже января}&#13;&#10;      vMonth:=vMonth-1;&#13;&#10;      DateFrom.Date:=EncodeDate( vYear, vMonth, 1);&#13;&#10;      DateTo.Date:=EncodeDate( vYear, vMonth, DaysInMonth(vYear, vMonth))&#13;&#10;    end&#13;&#10;    else begin {Если январь}&#13;&#10;      DateFrom.Date:=EncodeDate( vYear, vMonth, 1);&#13;&#10;      DateTo.Date:=Now&#13;&#10;    end;&#13;&#10;  Panel1.Visible:=False;&#13;&#10;end." StoreInDFM="False">
