    <TfrxERQuery Name="DetailQRY" UserName="DetailQRY" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="GrQRY" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;	Usr_PlnStr.N_Str,     &#13;&#10;	Usr_PlnStr.StrName,&#13;&#10;	SUM (T1.TSmSm) TSm&#13;&#10;&#13;&#10;FROM         &#13;&#10;&#13;&#10;	 Usr_PlnStr&#13;&#10;  &#13;&#10;LEFT JOIN (&#13;&#10;       SELECT     &#13;&#10;               P_VKZ.Cod_Zak, &#13;&#10;               P_VKZ.Key_VKZ_Izdel, &#13;&#10;               T_Oper.Cod_Oper, &#13;&#10;               T_Oper.Name_K_Oper, &#13;&#10;               P_VKZ_Zap.KolvoZap, &#13;&#10;               P_VKZ_Oper.TpzSm, &#13;&#10;               P_VKZ_Oper.TShtSm,                             &#13;&#10;               P_VKZ_Oper.DateZap,&#13;&#10;               (IsNull(T_TPRows.Tpz,0) + isnull(T_TPRows.TSht*P_VKZ_Zap.KolvoZap,0))TSmSm                   &#13;&#10;FROM         &#13;&#10;       P_VKZ_Oper &#13;&#10;       INNER JOIN T_Oper &#13;&#10;               ON P_VKZ_Oper.Cod_Oper = T_Oper.Cod_Oper &#13;&#10;       INNER JOIN P_VKZ_Zap &#13;&#10;               ON P_VKZ_Oper.Key_VKZ_Zap = P_VKZ_Zap.Key_VKZ_Zap &#13;&#10;       INNER JOIN P_VKZ &#13;&#10;               ON P_VKZ_Zap.Key_VKZ = P_VKZ.Key_VKZ&#13;&#10;      	INNER JOIN T_TPRows&#13;&#10;               ON T_TPRows.Key_TPRows=P_VKZ_Oper.Key_TPRows&#13;&#10;                   &#13;&#10;WHERE  T_Oper.Cod_Oper_Gr = :Cod_Oper_Gr                                                             &#13;&#10;) T1&#13;&#10;  ON  T1.DateZap &#62;= Usr_PlnStr.DateFrom&#13;&#10;  AND T1.DateZap &#60;= Usr_PlnStr.DateTo                 &#13;&#10;&#13;&#10;  &#13;&#10;	&#13;&#10;     &#13;&#10;&#13;&#10;GROUP BY &#13;&#10;	Usr_PlnStr.N_Str,     &#13;&#10;	Usr_PlnStr.StrName&#13;&#10;ORDER By Usr_PlnStr.N_Str  " PropData="05704C65667403B4000470546F7002340A506172616D657465727301010C28000000204E616D653D22436F645F4F7065725F4772222044617461547970653D226674556E6B6E6F776E220000"/>
