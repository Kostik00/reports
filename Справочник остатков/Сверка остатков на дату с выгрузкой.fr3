    <TfrxERQuery Name="ErorNomenQRY" UserName="Номенклатура" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="	SELECT Ostatok.*    &#13;&#10;	FROM &#13;&#10;		[1C_Export].dbo.Ostatok Ostatok&#13;&#10;		LEFT JOIN T_VPredm&#13;&#10;			ON  Convert(varchar,T_VPredm.Key_Predm) = Ostatok.ARTICLE&#13;&#10;	WHERE&#13;&#10;		T_VPredm.Key_Predm IS NULL&#13;&#10;  AND  ADTAG  = :ADTag                           " PropData="05704C656674026C0470546F7002500A506172616D657465727301010C3A000000204E616D653D224144546167222044617461547970653D226674496E7465676572222045787072657373696F6E3D2241445461672E54657874220000"/>
    <TfrxERQuery Name="ErrorNamesQRY" UserName="ОтклонениеН" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="	SELECT Ostatok.*, &#13;&#10;Key_Predm, &#13;&#10;Cod_Predm, &#13;&#10;Name_Predm, &#13;&#10;Sortam, &#13;&#10;Type_NTD_Sortam, &#13;&#10;Name_NTD_Sortam, &#13;&#10;Marka, &#13;&#10;Type_NTD_Marka, &#13;&#10;Name_NTD_Marka, &#13;&#10;SKLAD_Ed_Izm, &#13;&#10;Name_K_Ed, &#13;&#10;PrintFlag&#13;&#10;  &#13;&#10;	FROM &#13;&#10;		[1C_Export].dbo.Ostatok Ostatok&#13;&#10;		INNER JOIN T_VPredm&#13;&#10;			ON Convert(varchar, T_VPredm.Key_Predm)=Ostatok.ARTICLE&#13;&#10;&#13;&#10;	WHERE &#13;&#10;		      Ostatok.ARTICLE  IN (Select convert(varchar,Key_Predm) FROM T_Predm)&#13;&#10;			  AND Ostatok.NAME NOT LIKE T_VPredm.Name_Predm+'%'+T_VPredm.Sortam+'%'+T_VPredm.Marka+'%'&#13;&#10;     AND  Ostatok.ADTag = :ADTag                                                                                           " PropData="05704C65667402700470546F70039C000A506172616D657465727301010C3A000000204E616D653D224144546167222044617461547970653D226674496E7465676572222045787072657373696F6E3D2241445461672E54657874220000"/>
    <TfrxERQuery Name="ErrorKolvo" UserName="Отклонения" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="DECLARE @Cod_Sub varchar(50)&#13;&#10;DECLARE @ADTAG INT&#13;&#10;DECLARE @Otkl  float&#13;&#10;&#13;&#10;&#13;&#10;SET @Cod_Sub= :Cod_Sub     &#13;&#10;SET @ADTAG = :ADTAG  &#13;&#10;SET @Otkl = :Otkl  &#13;&#10;&#13;&#10;	if object_id(N'tempdb..#TMP_Ostatok_Mat') is  null&#13;&#10;	BEGIN&#13;&#10;		CREATE TABLE #TMP_Ostatok_Mat(&#13;&#10;			[Key_Sub_Ver] [int] NOT NULL,&#13;&#10;			[Key_ID_Mat] [int] NOT NULL,&#13;&#10;			[Ost] [numeric](18, 4) NOT NULL			DEFAULT (0),&#13;&#10;			[SmOst] [money] NOT NULL				DEFAULT (0),&#13;&#10;			[Rezerv] [numeric](18, 4) NOT NULL      DEFAULT (0),&#13;&#10;			[TmpPrihod] [numeric](18, 4) NOT NULL   DEFAULT (0),&#13;&#10;			[TmpRashod] [numeric](18, 4) NOT NULL   DEFAULT (0),&#13;&#10;			[LastDate] [datetime] NULL,&#13;&#10;			KolvoOnWork [numeric](18, 4) NULL&#13;&#10;		)  &#13;&#10;	END  &#13;&#10;&#13;&#10;SELECT T1.*, T2.* , T_VPredm.Key_Predm   Key_Predm001,&#13;&#10;&#13;&#10;T_VPredm.Cod_Predm,&#13;&#10;T_VPredm.Name_Predm, &#13;&#10;T_VPredm.Sortam, &#13;&#10;T_VPredm.Type_NTD_Sortam, &#13;&#10;T_VPredm.Name_NTD_Sortam, &#13;&#10;T_VPredm.Marka, &#13;&#10;T_VPredm.Type_NTD_Marka, &#13;&#10;T_VPredm.Name_NTD_Marka, &#13;&#10;T_VPredm.SKLAD_Ed_Izm, &#13;&#10;T_VPredm.Name_K_Ed, &#13;&#10;T_VPredm.PrintFlag&#13;&#10;  &#13;&#10;&#13;&#10;FROM&#13;&#10;(&#13;&#10;SELECT   &#13;&#10;	T_VPredm.Key_Predm,&#13;&#10; Ostatok.ITEMNUMBER,               &#13;&#10;	Ostatok.FULLNAME,&#13;&#10;	Ostatok.AMOUNT, &#13;&#10;	Ostatok.UNITCODE, &#13;&#10;	Ostatok.UNITNAME&#13;&#10;FROM &#13;&#10;	[1C_Export].dbo.Ostatok Ostatok&#13;&#10;	INNER JOIN T_VPredm&#13;&#10;		ON Convert(varchar, T_VPredm.Key_Predm)=Ostatok.ARTICLE&#13;&#10;WHERE Ostatok.ADTAG= @ADTAG&#13;&#10;) T1&#13;&#10;&#13;&#10;FULL OUTER JOIN&#13;&#10;(&#13;&#10;SELECT        &#13;&#10;	SubString(S_ALL_Sub.cod_sub,1,1) Cod_Sub, &#13;&#10;	A_ID_Mat.Key_Predm, &#13;&#10;	Round(SUM(A_Ostatok_Mat.Ost),3) AS OST,&#13;&#10;	Round(SUM(A_Ostatok_Mat.KolvoOnWork),3) AS KolvoOnWork,&#13;&#10;	T_VPredm.Sklad_Ed_Izm&#13;&#10;FROM            &#13;&#10;	#TMP_Ostatok_Mat A_Ostatok_Mat &#13;&#10;	INNER JOIN S_ALL_Sub &#13;&#10;		ON A_Ostatok_Mat.Key_Sub_Ver = S_ALL_Sub.Key_Sub_Ver &#13;&#10;	INNER JOIN A_ID_Mat &#13;&#10;		ON A_Ostatok_Mat.Key_ID_Mat = A_ID_Mat.Key_ID_Mat &#13;&#10;	INNER JOIN T_VPredm &#13;&#10;		ON A_ID_Mat.Key_Predm = T_VPredm.Key_Predm&#13;&#10;WHERE&#13;&#10;  S_ALL_Sub.cod_sub like @Cod_Sub                     &#13;&#10;GROUP BY &#13;&#10;	SubString(S_ALL_Sub.cod_sub,1,1), &#13;&#10;	A_ID_Mat.Key_Predm, &#13;&#10;	T_VPredm.Sklad_Ed_Izm&#13;&#10;) T2&#13;&#10;&#13;&#10;ON T1.Key_Predm = T2.Key_Predm&#13;&#10;&#13;&#10;LEFT JOIN T_VPredm&#13;&#10;	ON T_VPredm.Key_Predm=T2.Key_Predm&#13;&#10;&#13;&#10; WHERE ABS(Isnull(T1.AMOUNT,0) - (Isnull( T2.OST  ,0) + Isnull(KolvoOnWork,0) )) &#62; @Otkl                                    &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;   " PropData=""/>
