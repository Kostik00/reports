    <TfrxERQuery Name="OstatokQRY" UserName="Îñòàòêè" CloseDataSource="False" FieldAliases.Text="Name_Sub=Name_Sub&#13;&#10;Key_Sub_Ver=Key_Sub_Ver&#13;&#10;F_Name_Sub=F_Name_Sub&#13;&#10;Key_ID_Mat=Key_ID_Mat&#13;&#10;LZK_Rezerv=LZK_Rezerv&#13;&#10;LZK_Cod_Zak=LZK_Cod_Zak&#13;&#10;VidSert=VidSert&#13;&#10;N_Sert=N_Sert&#13;&#10;Date_Sert=Date_Sert&#13;&#10;Cod_Plavka=Cod_Plavka&#13;&#10;Party=Party&#13;&#10;N_SertPos=N_SertPos&#13;&#10;PrintFlag=PrintFlag&#13;&#10;Key_Predm=Key_Predm&#13;&#10;Cod_Predm=Cod_Predm&#13;&#10;Name_Predm=Name_Predm&#13;&#10;Sortam=Sortam&#13;&#10;Type_NTD_Sortam=Type_NTD_Sortam&#13;&#10;Name_NTD_Sortam=Name_NTD_Sortam&#13;&#10;Marka=Marka&#13;&#10;Type_NTD_Marka=Type_NTD_Marka&#13;&#10;Name_NTD_Marka=Name_NTD_Marka&#13;&#10;OrderSortam=OrderSortam&#13;&#10;Ost=Ost&#13;&#10;Name_K_Ed=Name_K_Ed&#13;&#10;Cod_Gr=Cod_Gr&#13;&#10;FullNameGr=FullNameGr&#13;&#10;PriceMin=PriceMin&#13;&#10;PriceAvg=PriceAvg&#13;&#10;PriceMax=PriceMax&#13;&#10;FirstNacl=FirstNacl&#13;&#10;LastNacl=LastNacl&#13;&#10;YY=YY" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="DECLARE @BeginYear Int;&#13;&#10;DECLARE @EndYear Int;    &#13;&#10;DECLARE @CodZak varchar(50);&#13;&#10;&#13;&#10;SET @BeginYear = :BeginYear;                      &#13;&#10;SET @EndYear = :EndYear;                          &#13;&#10;SET @CodZak = :CodZak;&#13;&#10;  &#13;&#10;SELECT     &#13;&#10;    S_Subjects.Name_Sub,&#13;&#10;    S_SubVer.Key_Sub_Ver,           &#13;&#10;    S_SubVer.F_Name_Sub, &#13;&#10;    A_Ostatok_Mat.Key_ID_Mat,&#13;&#10;    A_LZK.Rezerv AS LZK_Rezerv,&#13;&#10;    A_LZK.Cod_Zak AS LZK_Cod_Zak,           &#13;&#10;    A_VID_Mat.VidSert, &#13;&#10;    A_VID_Mat.N_Sert, &#13;&#10;    A_VID_Mat.Date_Sert, &#13;&#10;    A_VID_Mat.Cod_Plavka,&#13;&#10;    A_VID_Mat.Party, &#13;&#10;    A_VID_Mat.N_SertPos, &#13;&#10;    T_VPredm.PrintFlag,&#13;&#10;    T_VPredm.Key_Predm,                               &#13;&#10;    T_VPredm.Cod_Predm, &#13;&#10;    T_VPredm.Name_Predm, &#13;&#10;    T_VPredm.Sortam, &#13;&#10;    T_VPredm.Type_NTD_Sortam,&#13;&#10;    T_VPredm.Name_NTD_Sortam, &#13;&#10;    T_VPredm.Marka, &#13;&#10;    T_VPredm.Type_NTD_Marka, &#13;&#10;    T_VPredm.Name_NTD_Marka, &#13;&#10;    T_VPredm.OrderSortam, &#13;&#10;    A_Ostatok_Mat.Ost,&#13;&#10;    T_EdIzm.Name_K_Ed,&#13;&#10;    Gr_Groups.Cod_Gr,             &#13;&#10;    Gr_Groups.FullNameGr,&#13;&#10;    T1.PriceMin,&#13;&#10;    T1.PriceAvg,&#13;&#10;    T1.PriceMax,&#13;&#10;    T2.FirstNacl,        &#13;&#10;    T2.LastNacl,&#13;&#10;    DatePart(yyyy, T2.LastNacl) AS YY                                    &#13;&#10;  FROM         &#13;&#10;    A_Ostatok_Mat &#13;&#10;      INNER JOIN A_LZK        ON A_LZK.Key_ID_Mat = A_Ostatok_Mat.Key_ID_Mat AND A_LZK.Key_Sub_Ver = A_Ostatok_Mat.Key_Sub_Ver&#13;&#10;      INNER JOIN A_VID_Mat    ON A_Ostatok_Mat.Key_ID_Mat = A_VID_Mat.Key_ID_Mat&#13;&#10;      INNER JOIN T_VPredm     ON A_VID_Mat.Key_Predm = T_VPredm.Key_Predm&#13;&#10;      INNER JOIN T_EdIzm      ON T_VPredm.Sklad_Ed_Izm = T_EdIzm.Cod_Ed_Izm&#13;&#10;      INNER JOIN S_SubVer     ON A_Ostatok_Mat.Key_Sub_Ver = S_SubVer.Key_Sub_Ver&#13;&#10;      INNER JOIN S_Subjects   ON S_SubVer.Key_Sub = S_Subjects.Key_Sub&#13;&#10;      INNER JOIN Gr_VxodPredm ON A_VID_Mat.Key_Predm = Gr_VxodPredm.Key_Predm&#13;&#10;      INNER JOIN Gr_Groups    ON Gr_VxodPredm.Key_VidGr = Gr_Groups.Key_VidGr AND Gr_VxodPredm.Cod_Gr = Gr_Groups.Cod_Gr &#13;&#10;      LEFT  JOIN Pr_Pasp      ON A_VID_Mat.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;      LEFT  JOIN&#13;&#10;        (SELECT&#13;&#10;            A_ID_Mat.Key_ID_Mat,          &#13;&#10;            Min(A_Nacl_Str.Sm/A_Nacl_Str.KolvoInBaseEd) AS PriceMin,&#13;&#10;            Max(A_Nacl_Str.Sm/A_Nacl_Str.KolvoInBaseEd) AS PriceMax,&#13;&#10;            Avg(A_Nacl_Str.Sm/A_Nacl_Str.KolvoInBaseEd) AS PriceAvg          &#13;&#10;          FROM         &#13;&#10;            A_ID_Mat&#13;&#10;              INNER JOIN A_ID_Mat AS T1 ON T1.Key_Predm            = A_ID_Mat.Key_Predm      &#13;&#10;              INNER JOIN A_Nacl_Str     ON A_Nacl_Str.Key_ID_Mat   = T1.Key_ID_Mat      &#13;&#10;              INNER JOIN A_Nacl         ON A_Nacl.Key_Nacl         = A_Nacl_Str.Key_Nacl      &#13;&#10;              INNER JOIN S_SubVer       ON S_SubVer.Key_Sub_Ver    = A_Nacl.Key_Sub_Ver_From      &#13;&#10;              INNER JOIN S_Subjects     ON S_Subjects.Key_Sub      = S_SubVer.Key_Sub      &#13;&#10;          WHERE&#13;&#10;                (S_Subjects.Key_Class = 2)&#13;&#10;            AND (A_Nacl_Str.KolvoInBaseEd &#62; 0)&#13;&#10;            AND (A_Nacl_Str.Sm&#62; 0)&#13;&#10;          GROUP BY&#13;&#10;            A_ID_Mat.Key_ID_Mat) T1 ON T1.Key_ID_Mat = A_Ostatok_Mat.Key_ID_Mat        &#13;&#10;      INNER JOIN  --  LEFT JOIN &#13;&#10;        (SELECT&#13;&#10;            A_ID_Mat.Key_ID_Mat,&#13;&#10;            MIN(A_Nacl.Data_Nacl) AS FirstNacl,                  &#13;&#10;            MAX(A_Nacl.Data_Nacl) AS LastNacl        &#13;&#10;          FROM&#13;&#10;            A_ID_Mat&#13;&#10;              INNER JOIN A_Nacl_Str ON A_Nacl_Str.Key_ID_Mat = A_ID_Mat.Key_ID_Mat       &#13;&#10;              INNER JOIN A_Nacl     ON A_Nacl.Key_Nacl = A_Nacl_Str.Key_Nacl      &#13;&#10;          GROUP BY&#13;&#10;            A_ID_Mat.Key_ID_Mat) T2 ON T2.Key_ID_Mat = A_Ostatok_Mat.Key_ID_Mat  AND (DatePart(yyyy, T2.LastNacl) BETWEEN @BeginYear AND @EndYear)              &#13;&#10;  WHERE&#13;&#10;        (A_Ostatok_Mat.Ost &#62; 0)&#13;&#10;    AND (A_LZK.Cod_Zak Like @CodZak)&#13;&#10;  ORDER BY        &#13;&#10;    YY ASC,&#13;&#10;    S_Subjects.Name_Sub ASC,&#13;&#10;    Gr_Groups.FullNameGr ASC,&#13;&#10;    (A_Ostatok_Mat.Ost*T1.PriceAvg) DESC;           " PropData=""/>
    <TfrxERQuery Name="qZakaz" UserName="dsZakaz" CloseDataSource="False" FieldAliases.Text="Cod_Zak=Cod_Zak&#13;&#10;Name_Zak=Name_Zak&#13;&#10;DateZakOpen=DateZakOpen&#13;&#10;DateZakClose=DateZakClose&#13;&#10;DateZakSost=DateZakSost&#13;&#10;Cod_SosZ=Cod_SosZ&#13;&#10;Notes=Notes&#13;&#10;Date_PUpdate=Date_PUpdate&#13;&#10;Key_Sub_Ver_Konstr=Key_Sub_Ver_Konstr&#13;&#10;Key_Sub_Ver_Tch=Key_Sub_Ver_Tch&#13;&#10;Key_Sub_Ver_Master=Key_Sub_Ver_Master&#13;&#10;Key_Sub_Ver_Man=Key_Sub_Ver_Man" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="DECLARE @CodZak varchar(50);&#13;&#10;SET @CodZak = :CodZak;          &#13;&#10;&#13;&#10;SELECT&#13;&#10;    [Cod_Zak],&#13;&#10;    '¹ '+[Cod_Zak]+' &#34;'+[Name_Zak]+'&#34;' AS [Name_Zak],&#13;&#10;    [DateZakOpen],&#13;&#10;    [DateZakClose],&#13;&#10;    [DateZakSost],&#13;&#10;    [Cod_SosZ],&#13;&#10;    [Notes],&#13;&#10;    [Date_PUpdate],&#13;&#10;    [Key_Sub_Ver_Konstr],&#13;&#10;    [Key_Sub_Ver_Tch],&#13;&#10;    [Key_Sub_Ver_Master],&#13;&#10;    [Key_Sub_Ver_Man]&#13;&#10;  FROM&#13;&#10;    [D_Zakaz]&#13;&#10;  WHERE&#13;&#10;    [Cod_Zak] Like '%'+@CodZak+'%';" PropData="05704C6566740398000470546F7002140A506172616D657465727301010C49000000204E616D653D22436F645A616B222044617461547970653D226674537472696E67222045787072657373696F6E3D225472696D286564436F645A616B46696C7465722E5465787429220000"/>
    <TfrxERQuery Name="qDetReserv" UserName="dsDetReserv" CloseDataSource="False" FieldAliases.Text="IS_Cod_Zak=IS_Cod_Zak&#13;&#10;Cod_Zak=Cod_Zak&#13;&#10;Name_Sub=Name_Sub&#13;&#10;SUM_KolvoInBaseEd=SUM_KolvoInBaseEd" BCDToCurrency="False" Master="OstatokQRY" IgnoreDupParams="False" SQL.Text="DECLARE @Cod_Zak varchar(50);                         &#13;&#10;DECLARE @Key_Id_Mat  int;&#13;&#10;DECLARE @Key_Sub_Ver int;&#13;&#10;&#13;&#10;SET @Cod_Zak = :Cod_Zak;    &#13;&#10;SET @Key_Id_Mat  = :Key_Id_Mat;&#13;&#10;SET @Key_Sub_Ver = :Key_Sub_Ver;&#13;&#10;&#13;&#10;WITH VMZ(IS_Cod_Zak, KolvoInBaseEd, Name_Sub, Cod_Zak) AS&#13;&#10;(    &#13;&#10;SELECT&#13;&#10;    CASE&#13;&#10;      WHEN ISNULL(P_VKZ_Zap.Cod_Zak, '0') = @Cod_Zak THEN 1     &#13;&#10;      ELSE 0     &#13;&#10;    END AS IS_Cod_Zak,&#13;&#10;    A_Rezerv.KolvoInBaseEd * A_Rezerv.RzActive AS KolvoInBaseEd,&#13;&#10;    S_All_Sub.Name_Sub,                            &#13;&#10;    ISNULL(P_VKZ_Zap.Cod_Zak, '0') AS Cod_Zak               &#13;&#10;  FROM&#13;&#10;    P_VMZ&#13;&#10;      INNER JOIN T_VPredm     AS DSE      ON P_VMZ.Key_PredmDSE = DSE.Key_Predm&#13;&#10;      INNER JOIN dbo.A_Rezerv AS A_Rezerv ON A_Rezerv.Key_VMZ = P_VMZ.Key_VMZ&#13;&#10;      LEFT JOIN&#13;&#10;        (SELECT&#13;&#10;          A_Rezerv.Key_Rezerv,&#13;&#10;          SUM(Pr_CZ_DT.Kolvo_Part) AS Kolvo_Part,&#13;&#10;          SUM(Pr_CZ_DT.Kolvo)      AS KolvoZ&#13;&#10;          FROM&#13;&#10;            A_Rezerv&#13;&#10;              INNER JOIN Pr_CZ_DT         ON A_Rezerv.Key_Rezerv = Pr_CZ_DT.Key_Rezerv&#13;&#10;          GROUP BY A_Rezerv.Key_Rezerv)&#13;&#10;                              AS CZSm     ON CZSm.Key_Rezerv = A_Rezerv.Key_Rezerv&#13;&#10;      INNER JOIN A_VID_Mat    AS A_ID_Mat ON A_ID_Mat.Key_Id_Mat = A_Rezerv.Key_Id_Mat&#13;&#10;      INNER JOIN T_VPredm     AS PredmM   ON A_ID_Mat.Key_Predm = PredmM.Key_Predm&#13;&#10;      INNER JOIN T_Edizm                  ON T_Edizm.Cod_Ed_Izm = A_Rezerv.Cod_Ed_Izm&#13;&#10;      INNER JOIN T_Edizm      AS T_EdizmM ON T_EdizmM.Cod_Ed_Izm = PredmM.Sklad_Ed_Izm&#13;&#10;      LEFT  JOIN T_Zagotov                ON A_Rezerv.Cod_Zagotov = T_Zagotov.Cod_Zagotov&#13;&#10;      LEFT  JOIN P_VKZ_Zap                ON P_VKZ_Zap.Key_Vkz_Zap = P_VMZ.Key_Vkz_Zap                        &#13;&#10;      LEFT  JOIN S_All_Sub                ON S_All_Sub.cod_sub=A_Rezerv.Usr AND S_All_Sub.Key_Class = 5&#13;&#10;  WHERE&#13;&#10;        A_Rezerv.Key_Id_Mat  = @Key_Id_Mat          &#13;&#10;    AND A_Rezerv.Key_Sub_Ver = @Key_Sub_Ver)&#13;&#10;&#13;&#10;SELECT IS_Cod_Zak, Cod_Zak, Name_Sub, SUM(KolvoInBaseEd) AS SUM_KolvoInBaseEd                      &#13;&#10;FROM VMZ&#13;&#10;GROUP BY                      &#13;&#10;  IS_Cod_Zak,&#13;&#10;  Cod_Zak,&#13;&#10;  Name_Sub      &#13;&#10;HAVING&#13;&#10;  SUM(KolvoInBaseEd) &#62; 0.00                        &#13;&#10;ORDER BY                      &#13;&#10;  IS_Cod_Zak DESC,&#13;&#10;  SUM_KolvoInBaseEd DESC,&#13;&#10;  Name_Sub ASC;" PropData=""/>
    <TfrxERQuery Name="qLimitReserv" UserName="dsLimitReserv" CloseDataSource="False" FieldAliases.Text="IS_Cod_Zak=IS_Cod_Zak&#13;&#10;Cod_Zak=Cod_Zak&#13;&#10;SUM_Rezerv=SUM_Rezerv" BCDToCurrency="False" Master="OstatokQRY" IgnoreDupParams="False" SQL.Text="DECLARE @Cod_Zak varchar(50);                         &#13;&#10;DECLARE @Key_Id_Mat  int;&#13;&#10;DECLARE @Key_Sub_Ver int;&#13;&#10;&#13;&#10;SET @Cod_Zak = :Cod_Zak;    &#13;&#10;SET @Key_Id_Mat  = :Key_Id_Mat;&#13;&#10;SET @Key_Sub_Ver = :Key_Sub_Ver;  &#13;&#10;&#13;&#10;WITH&#13;&#10;  LZK(IS_Cod_Zak, Cod_Zak, Rezerv) AS&#13;&#10;    (SELECT&#13;&#10;        CASE&#13;&#10;          WHEN Cod_Zak = @Cod_Zak THEN 1     &#13;&#10;          ELSE 0     &#13;&#10;        END AS IS_Cod_Zak,&#13;&#10;        [Cod_Zak],&#13;&#10;        [Rezerv]&#13;&#10;      FROM [A_LZK]       &#13;&#10;      WHERE [Key_ID_Mat] = @Key_ID_Mat AND [Key_Sub_Ver] = @Key_Sub_Ver AND [Cod_Zak] = @Cod_Zak&#13;&#10;&#13;&#10;      UNION&#13;&#10;&#13;&#10;     SELECT&#13;&#10;        0 AS IS_Cod_Zak, [Cod_Zak], [Rezerv]                 &#13;&#10;      FROM [A_LZK]       &#13;&#10;      WHERE [Key_ID_Mat] = @Key_ID_Mat AND [Key_Sub_Ver] = @Key_Sub_Ver AND [Cod_Zak] &#60;&#62; @Cod_Zak)&#13;&#10;        &#13;&#10;SELECT&#13;&#10;  IS_Cod_Zak, Cod_Zak, SUM(Rezerv) AS SUM_Rezerv                 &#13;&#10;FROM LZK               &#13;&#10;GROUP BY&#13;&#10;  IS_Cod_Zak,&#13;&#10;  Cod_Zak&#13;&#10;HAVING&#13;&#10;  SUM(Rezerv) &#62; 0.00                      &#13;&#10;ORDER BY&#13;&#10;  IS_Cod_Zak DESC,&#13;&#10;  SUM_Rezerv DESC;                   " PropData=""/>
