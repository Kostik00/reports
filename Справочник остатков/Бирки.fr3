#10;  Osnovanie,&#13;&#10;  Sost)&#13;&#10;SELECT DISTINCT&#13;&#10;  A_Ostatok_Mat.Key_Sub_Ver,&#13;&#10;  A_Ostatok_Mat.Key_ID_Mat,&#13;&#10;  A_Nacl.Key_Nacl,&#13;&#10;  A_Nacl.Num_Nacl,&#13;&#10;  A_Nacl.Data_Nacl,&#13;&#10;  A_Nacl.Osnovanie,&#13;&#10;  A_Nacl.Sost&#13;&#10;FROM&#13;&#10;  A_Ostatok_Mat&#13;&#10;  INNER JOIN A_Nacl&#13;&#10;          ON ((A_Ostatok_Mat.Key_Sub_Ver = A_Nacl.Key_Sub_Ver_From) OR (A_Ostatok_Mat.Key_Sub_Ver = A_Nacl.Key_Sub_Ver_To))&#13;&#10;  INNER JOIN A_Nacl_Str&#13;&#10;          ON A_Nacl.Key_Nacl = A_Nacl_Str.Key_Nacl AND A_Ostatok_Mat.Key_ID_Mat = A_Nacl_Str.Key_ID_Mat&#13;&#10;WHERE&#13;&#10;      A_Ostatok_Mat.Key_Sub_Ver = :KeySubVer AND A_Ostatok_Mat.Key_ID_Mat = :KeyIDMat;&#13;&#10;&#13;&#10;SET NOCOUNT OFF;                                  &#13;&#10;  &#13;&#10;SELECT DISTINCT  --  OstDocQRY&#13;&#10;  &#13;&#10;  A_VID_Mat.VidSert,&#13;&#10;  A_VID_Mat.N_Sert,&#13;&#10;  A_VID_Mat.Date_Sert,&#13;&#10;  A_VID_Mat.Cod_Plavka,&#13;&#10;  A_VID_Mat.Party,&#13;&#10;  A_VID_Mat.SrokGodn,&#13;&#10;  A_VID_Mat.RegN_Sert, &#13;&#10;  A_VID_Mat.RegN,&#13;&#10;  A_VID_Mat.Cod_Pasp,&#13;&#10;&#13;&#10;  TVN.Key_Nacl,&#13;&#10;  TVN.Num_Nacl,&#13;&#10;  TVN.Data_Nacl,&#13;&#10;  TVN.Osnovanie,&#13;&#10;  TVN.Sost,&#13;&#10;&#13;&#10;  A_Nacl_Str.Key_Nacl_Str,&#13;&#10;  A_Nacl_Str.Key_Nacl,&#13;&#10;  A_Nacl_Str.Key_ID_Mat,&#13;&#10;&#13;&#10;  A_Nacl_Str.Kolvo,&#13;&#10;  A_Nacl_Str.Cod_Ed_Izm,&#13;&#10;&#13;&#10;  A_Nacl_Str.KolvoInBaseEd,&#13;&#10;  A_Nacl_Str.Price,&#13;&#10;  A_Nacl_Str.PercNds,&#13;&#10;  A_Nacl_Str.Sm,&#13;&#10;  A_Nacl_Str.SmNDS,&#13;&#10;  A_Nacl_Str.SrokKons,&#13;&#10;  A_Nacl_Str.NumStr_By_Docum,              &#13;&#10;&#13;&#10;  A_Nacl_Str_Sost.Key_Nacl_Str_Sost,                                            &#13;&#10;  A_Nacl_Str_Sost.ProfRazm,&#13;&#10;  A_Nacl_Str_Sost.Kolvo KolvoZag,                           &#13;&#10;&#13;&#10;  dbo.A_FnGetNaclStrRezerv(A_Nacl_Str.Key_Nacl_Str) Primen,                           &#13;&#10;    &#13;&#10;  T_VPredm.Key_Predm,&#13;&#10;&#13;&#10;  T_VPredm.PrintFlag,&#13;&#10;  T_VPredm.Cod_Predm,                            &#13;&#10;  T_VPredm.Name_Predm,&#13;&#10;  T_VPredm.Sortam,&#13;&#10;  T_VPredm.Type_NTD_Sortam,&#13;&#10;  T_VPredm.Name_NTD_Sortam,&#13;&#10;  T_VPredm.Marka,&#13;&#10;  T_VPredm.Type_NTD_Marka,&#13;&#10;  T_VPredm.Name_NTD_Marka,&#13;&#10;  T_VPredm.Cod_Ed_Izm AS BaseCodEd,&#13;&#10;  T_VPredm.Name_K_Ed AS BaseEdizm,&#13;&#10;&#13;&#10;  T_EdIzm.Name_K_Ed,&#13;&#10;&#13;&#10;  T_Zagotov.Name_K_Zagotov   Name_Zagotov&#13;&#10;&#13;&#10;FROM&#13;&#10;  @tvNacl AS TVN  &#13;&#10;    INNER JOIN A_Nacl_Str&#13;&#10;            ON A_Nacl_Str.Key_Nacl = TVN.Key_Nacl     &#13;&#10;--           AND A_Nacl_Str.Key_ID_Mat = TVN.Key_ID_Mat     &#13;&#10;    INNER JOIN A_VID_Mat&#13;&#10;            ON A_VID_Mat.Key_ID_Mat = A_Nacl_Str.Key_ID_Mat&#13;&#10;    INNER JOIN T_VPredmSkl T_VPredm&#13;&#10;            ON T_VPredm.Key_Predm = A_VID_Mat.Key_Predm&#13;&#10;    INNER JOIN T_EdIzm&#13;&#10;            ON A_Nacl_Str.Cod_Ed_Izm = T_EdIzm.Cod_Ed_Izm&#13;&#10;    LEFT  JOIN A_Nacl_Str_Sost&#13;&#10;            ON A_Nacl_Str_Sost.Key_Nacl_Str = A_Nacl_Str.Key_Nacl_Str&#13;&#10;    LEFT  JOIN T_Zagotov&#13;&#10;  	         ON T_Zagotov.Cod_Zagotov = A_Nacl_Str_Sost.Cod_Zagotov&#13;&#10;    &#13;&#10;ORDER BY&#13;&#10;  TVN.Key_Nacl, A_Nacl_Str.NumStr_By_Docum;  " PropData=""/>
