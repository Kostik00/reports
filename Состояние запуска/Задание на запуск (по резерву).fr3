<?xml version="1.0" encoding="utf-8"?>
<TfrxReport Version="6.9.6" DotMatrixReport="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="42215,0909024074" ReportOptions.Description.Text="" ReportOptions.Name="Задание на запуск (по резерву, fix. 89 - 88).fr3 Состояние запуска" ReportOptions.LastChange="44625,6335357755" ReportOptions.VersionBuild="22" ReportOptions.VersionMajor="15" ReportOptions.VersionMinor="8" ReportOptions.VersionRelease="2" ScriptLanguage="PascalScript" ScriptText.Text="uses 'CommonRU.pas';&#13;&#10;&#13;&#10;var&#13;&#10;  sTemp:          String = '';&#13;&#10;  sProfile:       String = '';&#13;&#10;  sCount:         String = '';&#13;&#10;  sProbe:         String = '';&#13;&#10;  sReportTitle:   String = '';&#13;&#10;  IniFile:        TIniFile;&#13;&#10;&#13;&#10;procedure DialogPageOnActivate(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;begin&#13;&#10;  qPVZList.Open;&#13;&#10;  qPVZList.First;&#13;&#10;  while not qPVZList.Eof&#13;&#10;    do begin&#13;&#10;      CheckListBox1.Items.Add(FormatFloat('00000#', qPVZList.FieldByName('Key_VKZ_Zap').AsFloat)+' '+qPVZList.FieldByName('Cod_Pasp').AsString);&#13;&#10;      qPVZList.Next&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPageOnDeactivate(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;  sValues: String = '';&#13;&#10;begin&#13;&#10;  if DialogPage.ModalResult = mrOk&#13;&#10;    then begin&#13;&#10;      gfIdMat.Visible:=PrintSpravka.Checked;&#13;&#10;      chgfIdMat.Visible:=PrintAllProductions.Checked;&#13;&#10;      chgfIdMat.Visible:=PrintProductionList.Checked;&#13;&#10;      TIniFile(IniFile).WriteBool('DialogPage', 'PrintAll', PrintAll.Checked);&#13;&#10;      TIniFile(IniFile).WriteBool('DialogPage', 'PrintSpravka', PrintSpravka.Checked);&#13;&#10;      TIniFile(IniFile).WriteBool('DialogPage', 'PrintTPOper', PrintTPOper.Checked);&#13;&#10;      TIniFile(IniFile).WriteBool('DialogPage', 'PrintAllProductions', PrintAllProductions.Checked);&#13;&#10;      TIniFile(IniFile).WriteBool('DialogPage', 'PrintProductionList', PrintProductionList.Checked);&#13;&#10;      TIniFile(IniFile).WriteBool('DialogPage', 'PrintOnlyCode', PrintOnlyCode.Checked);&#13;&#10;      for i:=0 to CheckListBox1.Items.Count-1&#13;&#10;        do begin&#13;&#10;          if CheckListBox1.Checked[i]&#13;&#10;            then begin&#13;&#10;              if sValues = ''&#13;&#10;                then sValues:=Copy(CheckListBox1.Items.Strings[i], 1, 6)&#13;&#10;                else sValues:=sValues+', '+Copy(CheckListBox1.Items.Strings[i], 1, 6);&#13;&#10;            end;&#13;&#10;        end;&#13;&#10;      ShowMessage(sValues);&#13;&#10;      qDSE.SQL.Strings[50]:='        INNER JOIN P_VKZ_Zap       ON P_VKZ_Zap.Key_VKZ_Zap = P_VKZ_Zap_Det.Key_VKZ_Zap AND P_VKZ_Zap.Key_VKZ_Zap IN ('+sValues+')';&#13;&#10;    end;&#13;&#10;  if PrintOnlyCode.Checked&#13;&#10;    then sReportTitle:='Задание на запуск (по резерву) № '+IntToStr(&#60;MainDS.&#34;Key_GrZap&#34;&#62;)+' от '+ DateToStr(&#60;MainDS.&#34;DateGrZap&#34;&#62;)+#10+'(только код)'&#13;&#10;    else sReportTitle:='Задание на запуск (по резерву) № '+IntToStr(&#60;MainDS.&#34;Key_GrZap&#34;&#62;)+' от '+ DateToStr(&#60;MainDS.&#34;DateGrZap&#34;&#62;);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ghCodPredmDSEOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  gfCodPredmDSE.Visible:=(not (Trim(&#60;dsDSE.&#34;Ad_Info&#34;&#62;) = '')) and PrintTPOper.Checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  sQueryProbeProfileTemp: String  = '';&#13;&#10;  sQueryProbeTemp:        String  = '';&#13;&#10;  sProfileProbeTemp:      String  = '';&#13;&#10;  iFirst, iSecond:        Integer =  0;&#13;&#10;&#13;&#10;  function fnReplaceSymbolsInProfile(AInputProfile: String): String;&#13;&#10;  begin&#13;&#10;    Result:=StringReplace(AInputProfile, 'х', 'x', 1);&#13;&#10;    Result:=StringReplace(Result, 'Х', 'x', 1);&#13;&#10;    Result:=StringReplace(Result, '.', ',', 1);&#13;&#10;    Result:=StringReplace(Result, '*', 'x', 3);&#13;&#10;  end;&#13;&#10;&#13;&#10;  procedure prFindProfileAndProbe(AInputDetailProfile, AInputProbeProfile: String = ''; var AOutputProfile, AOutputProbe: String = '');&#13;&#10;  const&#13;&#10;    iDetailProfile = 1;&#13;&#10;    iDetailProbe   = 2;&#13;&#10;    iProbeProfile  = 4;&#13;&#10;    iProbeProbe    = 8;&#13;&#10;  var&#13;&#10;    sTempDetaileProbe01: String;&#13;&#10;    sTempDetaileProbe02: String;&#13;&#10;&#13;&#10;    sDetailProfile: String = '';&#13;&#10;    sDetailProbe:   String = '';&#13;&#10;    sProbeProfile:  String = '';&#13;&#10;    sProbeProbe:    String = '';&#13;&#10;    sDetailBlank:   String = '';&#13;&#10;&#13;&#10;    iCase: Integer = 0;&#13;&#10;  begin&#13;&#10;&#13;&#10;    if Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(\w.*((заг\.)|(заготовк(а|и))).*[\w\.])\s*\)$', AInputDetailProfile, 0)) &#60;&#62; ''&#13;&#10;      then begin&#13;&#10;        sDetailProfile:= Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(\w.*((заг\.)|(заготовк(а|и))).*[\w\.])\s*\)$', AInputDetailProfile, 1));&#13;&#10;        sDetailBlank:=   Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(\w.*((заг\.)|(заготовк(а|и))).*[\w\.])\s*\)$', AInputDetailProfile, 2));&#13;&#10;        AOutputProfile:=sDetailProfile+' ('+sDetailBlank+')'&#13;&#10;      end&#13;&#10;      else begin&#13;&#10;        sDetailProfile:= Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(проб(а|ы)\s.*)\)$', AInputDetailProfile, 1));&#13;&#10;        sDetailProbe:=   Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(проб(а|ы)\s.*)\)$', AInputDetailProfile, 2));&#13;&#10;        sProbeProfile:=  Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(проб(а|ы)\s.*)\)$', AInputProbeProfile,  1));&#13;&#10;        sProbeProbe:=    Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(проб(а|ы)\s.*)\)$', AInputProbeProfile,  2));&#13;&#10;        if sDetailProfile &#60;&#62; '' then iCase:=iCase+iDetailProfile;&#13;&#10;        if sDetailProbe   &#60;&#62; '' then iCase:=iCase+iDetailProbe;&#13;&#10;        if sProbeProfile  &#60;&#62; '' then iCase:=iCase+iProbeProfile;&#13;&#10;        if sProbeProbe    &#60;&#62; '' then iCase:=iCase+iProbeProbe;&#13;&#10;        case iCase of&#13;&#10;          1: AOutputProfile:=sDetailProfile;&#13;&#10;          2: AOutputProbe:=sDetailProbe;&#13;&#10;          3:  begin&#13;&#10;                AOutputProfile:=sDetailProfile;&#13;&#10;                AOutputProbe:=sDetailProbe;&#13;&#10;              end;&#13;&#10;        end;&#13;&#10;        AInputDetailProfile:= Trim(AInputDetailProfile);&#13;&#10;        AInputProbeProfile:=  Trim(AInputProbeProfile);&#13;&#10;        if (AInputDetailProfile &#60;&#62; '') or (AInputProbeProfile &#60;&#62; '')&#13;&#10;          then begin&#13;&#10;            AOutputProfile:=Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(проб(а|ы)\s.*)\)$', AInputDetailProfile, 1));&#13;&#10;            sTempDetaileProbe01:=Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(проб(а|ы)\s.*)\)$', AInputDetailProfile, 2));&#13;&#10;            sTempDetaileProbe02:=Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(проб(а|ы)\s.*)\)$', AInputProbeProfile,  2));&#13;&#10;            if sTempDetaileProbe01 = sTempDetaileProbe02&#13;&#10;              then AOutputProbe:=Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(проб(а|ы)\s.*)\)$', AInputProbeProfile, 1))+' ('+sTempDetaileProbe01+')'&#13;&#10;              else AOutputProbe:=Trim(FindRegExprMatch('^\s*(L\s?=\s?[0-9]{1,3}\s?[+-±]\s?[0-9]{1,2})\s+\(\s*(проб(а|ы)\s.*)\)$', AInputProbeProfile, 1))+' ('+sTempDetaileProbe01+', '+sTempDetaileProbe02+')'&#13;&#10;          end&#13;&#10;      end;&#13;&#10;&#13;&#10;  AOutputProfile:=sDetailProfile;&#13;&#10;  AOutputProbe:=AInputProbeProfile;&#13;&#10;&#13;&#10;//  AOutputProbe:=Trim(qProbe.FieldByName('Prof_Razm').AsString);&#13;&#10;{   AOutputProfile:='AOutputProfile';&#13;&#10;    AOutputProbe:='AOutputProbe';  }&#13;&#10;  end;&#13;&#10;&#13;&#10;begin&#13;&#10;  sTemp:=    '';&#13;&#10;  sProfile:= '';&#13;&#10;  sProbe:=   '';&#13;&#10;  if qProbe.Active then qProbe.Close;&#13;&#10;  qProbe.Open;&#13;&#10;  sTemp:=fnReplaceSymbolsInProfile(Trim(qDSE.FieldByName('Prof_Razm').AsString));&#13;&#10;  if PrintOnlyCode.Checked then iSecond:=2;  //  Если указано &#34;печатать только код&#34;, то iSecond:=2&#13;&#10;  if ((&#60;dsDSE.&#34;EdNorm&#34;&#62; &#62; 1) or (&#60;dsDSE.&#34;KolZag&#34;&#62; &#62; 1))  //  Количество деталей на заготовку или количество заготовок больше еденицы&#13;&#10;    then begin&#13;&#10;      if Trim(FindRegExpr('^.*(\(.*[0-9]\s*заг\.\s+на\s+[0-9]+\s*дет\.\s*\)).*$', sTemp)) = '' then iFirst:=1  //  необходимо пояснение в профиле&#13;&#10;    end;&#13;&#10;  case iFirst+iSecond of&#13;&#10;    0: begin  //  Нет необходимости пояснения в профиле, и откл. - &#34;печатать только код&#34;&#13;&#10;      sProfile:=Trim(sTemp);&#13;&#10;      sCount:=IntToStr(&#60;dsDSE.&#34;KolDT&#34;&#62;)+' ('+IntToStr(&#60;dsDSE.&#34;KolvoZapPln&#34;&#62;)+')';&#13;&#10;    end;&#13;&#10;    1: begin  //  Несколько деталей на одну заготовку (необходимо пояснение в профиле), и откл. - &#34;печатать только код&#34;&#13;&#10;      sProfile:=Trim(sTemp+' ('+IntToStr(&#60;dsDSE.&#34;KolZag&#34;&#62;)+' заг. на '+IntToStr(&#60;dsDSE.&#34;EdNorm&#34;&#62;)+' дет.)');&#13;&#10;      sCount:=IntToStr(&#60;dsDSE.&#34;KolDT&#34;&#62;)+' ('+IntToStr(&#60;dsDSE.&#34;KolvoZapPln&#34;&#62;)+')';&#13;&#10;    end;&#13;&#10;    2: begin  //  Нет необходимости пояснения в профиле, и вкл. - &#34;печатать только код&#34;&#13;&#10;      sProfile:=Trim(sTemp);&#13;&#10;      sCount:=IntToStr(&#60;dsDSE.&#34;KolvoZapPln&#34;&#62;);&#13;&#10;    end;&#13;&#10;    3: begin  //  Несколько деталей на одну заготовку (необходимо пояснение в профиле), и вкл. - &#34;печатать только код&#34;&#13;&#10;      sProfile:=Trim(sTemp+' ('+IntToStr(&#60;dsDSE.&#34;KolZag&#34;&#62;)+' заг. на '+IntToStr(&#60;dsDSE.&#34;EdNorm&#34;&#62;)+' дет.)');&#13;&#10;      sCount:=IntToStr(&#60;dsDSE.&#34;KolvoZapPln&#34;&#62;);&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;  prFindProfileAndProbe(sTemp, fnReplaceSymbolsInProfile(Trim(qProbe.FieldByName('Prof_Razm').AsString)), sProfile, sProbe);&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  IniFile:=TIniFile.Create('.\Отчеты\Состояние запуска\Задание на запуск (по резерву).ini');&#13;&#10;  PrintAll.Checked:=TIniFile(IniFile).ReadBool('DialogPage', 'PrintAll', False);&#13;&#10;  PrintSpravka.Checked:=TIniFile(IniFile).ReadBool('DialogPage', 'PrintSpravka', False);&#13;&#10;  PrintTPOper.Checked:=TIniFile(IniFile).ReadBool('DialogPage', 'PrintTPOper', False);&#13;&#10;  PrintAllProductions.Checked:=TIniFile(IniFile).ReadBool('DialogPage', 'PrintAllProductions', False);&#13;&#10;  PrintProductionList.Checked:=TIniFile(IniFile).ReadBool('DialogPage', 'PrintProductionList', False);&#13;&#10;  PrintOnlyCode.Checked:=TIniFile(IniFile).ReadBool('DialogPage', 'PrintOnlyCode', False);&#13;&#10;end." StoreInDFM="False">
