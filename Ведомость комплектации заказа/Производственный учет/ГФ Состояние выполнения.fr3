    <TfrxERQuery Name="WSIQry" UserName="ВСИ" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="VKZfrxDS" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;       :Key_VKZ as Key_VKZ_Master,                                                     &#13;&#10;       P_VKZ.Key_VKZ_Izdel, &#13;&#10;       P_VKZ.Key_Predm, &#13;&#10;       SUM(P_VKZ.Kolvo) AS Kolvo, &#13;&#10;       T_VPredm.Cod_Predm, &#13;&#10;       T_VPredm.Name_Predm, &#13;&#10;       T_VPredm.Cod_Prin,&#13;&#10;       T_VPredm.OrderCod_Predm&#13;&#10;FROM         &#13;&#10;       P_VKZ &#13;&#10;       INNER JOIN T_VPredm&#13;&#10;               ON P_VKZ.Key_Predm = T_VPredm.Key_Predm&#13;&#10;WHERE              &#13;&#10;       (P_VKZ.Key_VKZ_Izdel = :Key_VKZ_Izdel)&#13;&#10;       AND CodStrSv like (select CodStrSv+'%' from P_VKZ T1 where T1.Key_VKZ= :Key_VKZ  )                                                                            &#13;&#10;                   &#13;&#10;GROUP BY &#13;&#10;       P_VKZ.Key_VKZ_Izdel, &#13;&#10;       P_VKZ.Key_Predm, &#13;&#10;       T_VPredm.Cod_Predm, &#13;&#10;       T_VPredm.Name_Predm, &#13;&#10;       T_VPredm.Cod_Prin, &#13;&#10;       T_VPredm.OrderCod_Predm&#13;&#10;ORDER BY &#13;&#10;       T_VPredm.Cod_Prin, &#13;&#10;       T_VPredm.OrderCod_Predm, &#13;&#10;       T_VPredm.Name_Predm" PropData="05704C65667403D4000470546F70024C0A506172616D657465727301010C24000000204E616D653D224B65795F564B5A222044617461547970653D226674556E6B6E6F776E2200010C2A000000204E616D653D224B65795F564B5A5F497A64656C222044617461547970653D226674556E6B6E6F776E220000"/>
    <TfrxERQuery Name="PrepareQRY" UserName="PrepareQRY" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="VKZfrxDS" IgnoreDupParams="False" SQL.Text="SET NOCOUNT ON                              &#13;&#10;&#13;&#10;  -- В эту таблицу мы складываем список используемых операций        &#13;&#10;if object_id(N'tempdb..#Tmp_OperList') is  not null&#13;&#10;    TRUNCATE TABLE  #Tmp_OperList&#13;&#10;ELSE&#13;&#10;CREATE TABLE #Tmp_OperList (&#13;&#10;   Cod_Oper varchar(4)-- Коды операций&#13;&#10;   )  &#13;&#10;  &#13;&#10;SET NOCOUNT OFF&#13;&#10;  &#13;&#10;SELECT GetDate() as DT                                                     &#13;&#10;  &#13;&#10;         " PropData="05704C65667402500470546F70024C0A506172616D65746572730100"/>
    <TfrxERQuery Name="TrudQRY" UserName="Трудоемкость" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="WSIQry" IgnoreDupParams="False" SQL.Text="if object_id(N'tempdb..#Tmp_OperList') is   null&#13;&#10;CREATE TABLE #Tmp_OperList (&#13;&#10;   Cod_Oper varchar(4)-- Коды операций&#13;&#10;   )&#13;&#10;  &#13;&#10;&#13;&#10;&#13;&#10;SELECT&#13;&#10;       T_Oper_Gr.Name_GrGr Name_Oper_Gr,&#13;&#10;       #Tmp_OperList.Cod_Oper,&#13;&#10;       T_Oper.Name_K_Oper,&#13;&#10;       T1.TSmByPasp,&#13;&#10;       T2.TSmByZ,&#13;&#10;       T2.KolvoZapByZ                 &#13;&#10;FROM&#13;&#10;  #Tmp_OperList&#13;&#10;  INNER JOIN T_Oper&#13;&#10;       ON T_Oper.Cod_Oper=#Tmp_OperList.Cod_Oper&#13;&#10;  INNER JOIN T_Oper_Gr&#13;&#10;       ON T_Oper_Gr.Cod_Oper_Gr=T_Oper.Cod_Oper_Gr&#13;&#10;&#13;&#10;  LEFT JOIN&#13;&#10;(&#13;&#10;SELECT     &#13;&#10;	P_VKZ.Key_Predm, &#13;&#10;	Pr_Pasp_Rows.Cod_Oper, &#13;&#10;	Sum(Pr_Pasp_Rows.Tsm * Pr_PaspDet.KolvoZap / Pr_Pasp.PaspKolvo) TSmByPasp&#13;&#10;&#13;&#10;FROM         &#13;&#10;	P_VKZ &#13;&#10;	INNER JOIN Pr_PaspDet &#13;&#10;		ON P_VKZ.Key_VKZ = Pr_PaspDet.Key_VKZ &#13;&#10;	INNER JOIN Pr_Pasp &#13;&#10;		ON Pr_PaspDet.Key_Pasport = Pr_Pasp.Key_Pasport &#13;&#10;	INNER JOIN Pr_Pasp_Rows &#13;&#10;		ON Pr_Pasp.Key_Pasport = Pr_Pasp_Rows.Key_Pasport&#13;&#10;WHERE     &#13;&#10;       (P_VKZ.Key_VKZ_Izdel = :Key_VKZ_Izdel )&#13;&#10;       AND P_VKZ.CodStrSv like (select CodStrSv+'%' from P_VKZ T1 where T1.Key_VKZ= :Key_VKZ_Master )&#13;&#10;       AND P_VKZ.Key_Predm = :Key_Predm                                                                    &#13;&#10;           &#13;&#10;&#13;&#10;GROUP BY 	&#13;&#10;	P_VKZ.Key_Predm, &#13;&#10;	Pr_Pasp_Rows.Cod_Oper   &#13;&#10;)  T1&#13;&#10;  ON T1.Cod_Oper =   #Tmp_OperList.Cod_Oper&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;LEFT JOIN&#13;&#10;(&#13;&#10;SELECT     &#13;&#10;	P_VKZ.Key_Predm,&#13;&#10;	Pr_Pasp_Rows.Cod_Oper, &#13;&#10; Sum(Pr_Tabel_AC.T_AC * Pr_PaspDet.KolvoZap / Pr_Pasp.PaspKolvo) TSmByZ,  &#13;&#10; Sum(Pr_Tabel_AC.Kolvo * Pr_PaspDet.KolvoZap / Pr_Pasp.PaspKolvo) KolvoZapByZ&#13;&#10;&#13;&#10;&#13;&#10;FROM         &#13;&#10;	P_VKZ &#13;&#10;	INNER JOIN Pr_PaspDet &#13;&#10;		ON P_VKZ.Key_VKZ = Pr_PaspDet.Key_VKZ &#13;&#10;	INNER JOIN Pr_Pasp &#13;&#10;		ON Pr_PaspDet.Key_Pasport = Pr_Pasp.Key_Pasport &#13;&#10;	INNER JOIN Pr_Pasp_Rows &#13;&#10;		ON Pr_Pasp.Key_Pasport = Pr_Pasp_Rows.Key_Pasport &#13;&#10;	INNER JOIN Pr_Tabel_MK &#13;&#10;		ON Pr_Pasp_Rows.Key_Pasp_Rows = Pr_Tabel_MK.Key_Pasp_Rows&#13;&#10; INNER JOIN Pr_Tabel_AC &#13;&#10;  ON Pr_Tabel_MK.Key_Tabel_MK = Pr_Tabel_AC.Key_Tabel_MK&#13;&#10;       &#13;&#10;  &#13;&#10;WHERE     &#13;&#10;       (P_VKZ.Key_VKZ_Izdel = :Key_VKZ_Izdel )&#13;&#10;       AND P_VKZ.CodStrSv like (select CodStrSv+'%' from P_VKZ T1 where T1.Key_VKZ= :Key_VKZ_Master )&#13;&#10;       AND P_VKZ.Key_Predm = :Key_Predm                                                                    &#13;&#10;GROUP BY 	&#13;&#10;	P_VKZ.Key_Predm, &#13;&#10;	Pr_Pasp_Rows.Cod_Oper  &#13;&#10;  &#13;&#10;) T2&#13;&#10;  ON T2.Cod_Oper =  #Tmp_OperList.Cod_Oper                                                               &#13;&#10;&#13;&#10;         &#13;&#10;ORDER BY&#13;&#10;       Name_Oper_Gr,&#13;&#10;       #Tmp_OperList.Cod_Oper  " PropData=""/>
    <TfrxERQuery Name="PrepareQRY1" UserName="ERQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="VKZfrxDS" IgnoreDupParams="False" SQL.Text="SET NOCOUNT ON                              &#13;&#10;&#13;&#10;  -- В эту таблицу мы складываем список используемых операций        &#13;&#10;if object_id(N'tempdb..#Tmp_OperList') is  not null&#13;&#10;    TRUNCATE TABLE  #Tmp_OperList&#13;&#10;ELSE&#13;&#10;CREATE TABLE #Tmp_OperList (&#13;&#10;   Cod_Oper varchar(4)-- Коды операций&#13;&#10;   )&#13;&#10;&#13;&#10;INSERT INTO  #Tmp_OperList&#13;&#10;  &#13;&#10;SELECT DISTINCT Pr_Pasp_Rows.Cod_Oper&#13;&#10;FROM         P_VKZ INNER JOIN&#13;&#10;                      Pr_PaspDet ON P_VKZ.Key_VKZ = Pr_PaspDet.Key_VKZ INNER JOIN&#13;&#10;                      Pr_Pasp_Rows ON Pr_PaspDet.Key_Pasport = Pr_Pasp_Rows.Key_Pasport&#13;&#10;WHERE              &#13;&#10;       (P_VKZ.Key_VKZ_Izdel = :Key_VKZ_Izdel)&#13;&#10;       AND CodStrSv like (select CodStrSv+'%' from P_VKZ T1 where T1.Key_VKZ= :Key_VKZ  )                                                                            &#13;&#10;  &#13;&#10;  &#13;&#10;SET NOCOUNT OFF&#13;&#10;  &#13;&#10;SELECT GetDate() as DT                                                     &#13;&#10;  &#13;&#10;         " PropData="05704C65667402500470546F70038C000A506172616D657465727301010C2A000000204E616D653D224B65795F564B5A5F497A64656C222044617461547970653D226674556E6B6E6F776E2200010C24000000204E616D653D224B65795F564B5A222044617461547970653D226674556E6B6E6F776E220000"/>
