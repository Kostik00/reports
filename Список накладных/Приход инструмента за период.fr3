    <TfrxERQuery Name="InstrumQRY" UserName="Инструменты" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="/****** Script for SelectTopNRows command from SSMS  ******/&#13;&#10;SELECT        &#13;&#10;  Gr_Groups.Cod_Gr,&#13;&#10;  Gr_Groups.FullNameGr,&#13;&#10;  &#13;&#10;  A_Nacl.Data_Nacl, &#13;&#10;  T_VPredm.PrintFlag, &#13;&#10;  T_VPredm.Cod_Predm, &#13;&#10;  T_VPredm.Name_Predm, &#13;&#10;  T_VPredm.Sortam, &#13;&#10;  T_VPredm.Type_NTD_Sortam, &#13;&#10;  T_VPredm.Name_NTD_Sortam,&#13;&#10;  T_VPredm.Marka,      &#13;&#10;  T_VPredm.Type_NTD_Marka,&#13;&#10;  T_VPredm.Name_NTD_Marka,&#13;&#10;  A_Nacl_Str.KolvoInBaseEd,        &#13;&#10;  A_Nacl_Str.Price, &#13;&#10;  A_Nacl_Str.Sm, &#13;&#10;  T_EdIzm.Name_K_Ed, &#13;&#10;  SubFrom.F_Name_Sub AS SubFrom, &#13;&#10;  SubTo.F_Name_Sub AS SubTo,&#13;&#10;  T_VPredm.Name_K_Ed    SkladEdIzm                                                      &#13;&#10;FROM            &#13;&#10;	A_Nacl &#13;&#10;	INNER JOIN A_Nacl_Str &#13;&#10;		ON A_Nacl.Key_Nacl = A_Nacl_Str.Key_Nacl &#13;&#10;	INNER JOIN A_VID_Mat &#13;&#10;		ON A_Nacl_Str.Key_ID_Mat = A_VID_Mat.Key_ID_Mat &#13;&#10;	INNER JOIN T_VPREDMSKL T_VPredm &#13;&#10;		ON A_VID_Mat.Key_Predm = T_VPredm.Key_Predm &#13;&#10;	INNER JOIN T_EdIzm &#13;&#10;		ON A_Nacl_Str.Cod_Ed_Izm = T_EdIzm.Cod_Ed_Izm &#13;&#10;	INNER JOIN S_ALL_Sub AS SubFrom &#13;&#10;		ON A_Nacl.Key_Sub_Ver_From = SubFrom.Key_Sub_Ver &#13;&#10;	INNER JOIN S_ALL_Sub AS SubTo &#13;&#10;		ON A_Nacl.Key_Sub_Ver_To = SubTo.Key_Sub_Ver&#13;&#10;&#13;&#10;	INNER JOIN  Gr_VxodPredm&#13;&#10;		ON Gr_VxodPredm.Key_Predm=T_VPredm.Key_Predm&#13;&#10;		AND Gr_VxodPredm.Key_VidGr = 1&#13;&#10;	INNER JOIN Gr_Groups&#13;&#10;		ON Gr_Groups.Cod_Gr=Gr_VxodPredm.Cod_Gr&#13;&#10;		AND Gr_Groups.Key_VidGr = Gr_VxodPredm.Key_VidGr&#13;&#10;&#13;&#10;       &#13;&#10;		&#13;&#10;&#13;&#10;WHERE        &#13;&#10;       (SubTo.F_Name_Sub LIKE 'Склад инструм%')&#13;&#10;  