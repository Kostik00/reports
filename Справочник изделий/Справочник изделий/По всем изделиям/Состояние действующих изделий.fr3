    <TfrxERQuery Name="IzdelQRY" UserName="Èçäåëèÿ" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT     &#13;&#10;       VKZ_IZDEL.Cod_Zak, &#13;&#10;       VKZ_IZDEL.Key_VKZ AS Key_VKZ_Izdel, &#13;&#10;       IZDEL.Cod_Predm, &#13;&#10;       IZDEL.Name_Predm, &#13;&#10;       T0.CntPredm CntDetail,&#13;&#10;       T1.CntPredm CntND,&#13;&#10;	   T2.PredmPasp PredmPasp,&#13;&#10;	   T3.PredmPasp PredmPaspZap,                    &#13;&#10;	   T4.PredmPasp PredmPaspKontr,                    &#13;&#10;	   T5.PredmPasp PredmNZ                                          &#13;&#10;         &#13;&#10;FROM         &#13;&#10;       P_VKZ AS VKZ_IZDEL &#13;&#10;       INNER JOIN  T_VPredm AS IZDEL &#13;&#10;               ON VKZ_IZDEL.Key_Predm = IZDEL.Key_Predm&#13;&#10;                 &#13;&#10;        LEFT JOIN                        &#13;&#10;        (  &#13;&#10;        SELECT Key_VKZ_Izdel, COUNT(Key_Predm) CntPredm&#13;&#10;        FROM&#13;&#10;               FN_VKZ_UQ_Predm('10') &#13;&#10;        GROUP BY Key_VKZ_Izdel&#13;&#10;        ) T0&#13;&#10;        ON T0.Key_VKZ_Izdel =   VKZ_IZDEL.Key_VKZ                                               &#13;&#10;          &#13;&#10;  &#13;&#10;        LEFT JOIN                        &#13;&#10;        (  &#13;&#10;        SELECT Key_VKZ_Izdel, COUNT(Key_Predm) CntPredm&#13;&#10;        FROM&#13;&#10;        (&#13;&#10;            SELECT     &#13;&#10;            	P_VKZ.Key_VKZ_Izdel, &#13;&#10;            	P_VKZ.Key_Predm, &#13;&#10;            	SUM(Kolvo) AS Kolvo, &#13;&#10;            	SUM(Kolvo_On_Izdel) AS KolvoOnIzdel&#13;&#10;            FROM&#13;&#10;            	P_VKZ&#13;&#10;               INNER JOIN T_Predm&#13;&#10;               ON T_Predm.Key_Predm = P_VKZ.Key_Predm&#13;&#10;            WHERE T_Predm.Cod_Prin &#60;&#62; '10'                                                                            &#13;&#10;            GROUP BY &#13;&#10;            	Key_VKZ_Izdel, &#13;&#10;            	P_VKZ.Key_Predm&#13;&#10;        ) T1&#13;&#10;        GROUP BY Key_VKZ_Izdel&#13;&#10;        ) T1&#13;&#10;        ON T1.Key_VKZ_Izdel =   VKZ_IZDEL.Key_VKZ&#13;&#10;&#13;&#10;&#13;&#10;        LEFT JOIN&#13;&#10;        (&#13;&#10;              select T1.Key_VKZ_Izdel, COUNT(T1.Key_Predm) as PredmPasp                                &#13;&#10;               &#13;&#10;              FROM &#13;&#10;              	FN_VKZ_UQ_Predm('10') T1&#13;&#10;              	LEFT JOIN FN_VKZ_UQ_Predm_PASP('10') T2&#13;&#10;              		ON T1.Key_VKZ_Izdel= T2.Key_VKZ_Izdel&#13;&#10;              		AND T1.Key_Predm=T2.Key_Predm&#13;&#10;              WHERE&#13;&#10;              	T1.KolvoPredm &#60;=T2.PaspKolvo&#13;&#10;              &#13;&#10;              GROUP BY T1.Key_VKZ_Izdel            &#13;&#10;        ) T2                   &#13;&#10;        ON T2.Key_VKZ_Izdel =   VKZ_IZDEL.Key_VKZ&#13;&#10;&#13;&#10;&#13;&#10;        LEFT JOIN&#13;&#10;        (&#13;&#10;              select T1.Key_VKZ_Izdel, COUNT(T1.Key_Predm) as PredmPasp                                &#13;&#10;               &#13;&#10;              FROM &#13;&#10;              	FN_VKZ_UQ_Predm('10') T1&#13;&#10;              	LEFT JOIN FN_VKZ_UQ_Predm_Pasp_S('10',1,10) T2&#13;&#10;              		ON T1.Key_VKZ_Izdel= T2.Key_VKZ_Izdel&#13;&#10;              		AND T1.Key_Predm=T2.Key_Predm&#13;&#10;              WHERE&#13;&#10;              	T1.KolvoPredm &#60;=T2.PaspKolvo&#13;&#10;              &#13;&#10;              GROUP BY T1.Key_VKZ_Izdel            &#13;&#10;        ) T3                   &#13;&#10;        ON T3.Key_VKZ_Izdel =   VKZ_IZDEL.Key_VKZ&#13;&#10;            &#13;&#10;&#13;&#10;        LEFT JOIN&#13;&#10;        (&#13;&#10;              select T1.Key_VKZ_Izdel, COUNT(T1.Key_Predm) as PredmPasp                                &#13;&#10;               &#13;&#10;              FROM &#13;&#10;              	FN_VKZ_UQ_Predm('10') T1&#13;&#10;              	LEFT JOIN FN_VKZ_UQ_Predm_Pasp_S('10',-10,-1) T2&#13;&#10;              		ON T1.Key_VKZ_Izdel= T2.Key_VKZ_Izdel&#13;&#10;              		AND T1.Key_Predm=T2.Key_Predm&#13;&#10;              WHERE&#13;&#10;              	T1.KolvoPredm &#60;=T2.PaspKolvo&#13;&#10;              &#13;&#10;              GROUP BY T1.Key_VKZ_Izdel            &#13;&#10;        ) T4                   &#13;&#10;        ON T4.Key_VKZ_Izdel =   VKZ_IZDEL.Key_VKZ&#13;&#10;&#13;&#10;&#13;&#10;        LEFT JOIN&#13;&#10;        (&#13;&#10;              select T1.Key_VKZ_Izdel, COUNT(T1.Key_Predm) as PredmPasp                                &#13;&#10;               &#13;&#10;              FROM &#13;&#10;              	FN_VKZ_UQ_Predm('10') T1&#13;&#10;              	LEFT JOIN FN_VKZ_UQ_Predm_Pasp_S('10',1,10) T2&#13;&#10;              		ON  T1.Key_VKZ_Izdel = T2.Key_VKZ_Izdel&#13;&#10;              		AND T1.Key_Predm     = T2.Key_Predm&#13;&#10;              WHERE&#13;&#10;                  	T1.KolvoPredm &#62; isnull(T2.PaspKolvo,0)&#13;&#10;				           AND T1.DataZapUtv &#60;= GetDate()                          &#13;&#10;              &#13;&#10;              GROUP BY T1.Key_VKZ_Izdel            &#13;&#10;        ) T5                   &#13;&#10;        ON T5.Key_VKZ_Izdel =   VKZ_IZDEL.Key_VKZ    &#13;&#10;                 &#13;&#10;WHERE     &#13;&#10;       (VKZ_IZDEL.Key_VKZ_Master IS NULL) &#13;&#10;       AND (VKZ_IZDEL.Key_VKZ_Type = 1) &#13;&#10;       AND (VKZ_IZDEL.Cod_SosZ = 'Ä')&#13;&#10;--AND 	   T2.PredmPasp&#60;&#62;T3.PredmPasp                    &#13;&#10;&#13;&#10;ORDER BY VKZ_IZDEL.Cod_Zak, IZDEL.Cod_Predm  " PropData="05704C65667403A0000470546F7002480A506172616D65746572730100"/>
