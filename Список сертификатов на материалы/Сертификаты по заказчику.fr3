    <TfrxERQuery Name="qCertificate" UserName="dsCertificate" CloseDataSource="False" FieldAliases.Text="Cod_Zak=Cod_Zak&#13;&#10;Key_VKZIZD=Key_VKZIZD&#13;&#10;Cod_SosZ=Cod_SosZ&#13;&#10;Key_VKZ_Sost=Key_VKZ_Sost&#13;&#10;Name_VKZ_Sost=Name_VKZ_Sost&#13;&#10;Cod_PredmIZD=Cod_PredmIZD&#13;&#10;Name_PredmIZD=Name_PredmIZD&#13;&#10;OrderCod_Predm=OrderCod_Predm&#13;&#10;Cod_PredmDT=Cod_PredmDT&#13;&#10;Name_PredmDT=Name_PredmDT&#13;&#10;Cod_Pasp=Cod_Pasp&#13;&#10;PaspKolvo=PaspKolvo&#13;&#10;ZavNumber=ZavNumber&#13;&#10;Key_ID_Mat=Key_ID_Mat&#13;&#10;Name_PredmPM=Name_PredmPM&#13;&#10;SortamPM=SortamPM&#13;&#10;Type_NTD_SortamPM=Type_NTD_SortamPM&#13;&#10;Name_NTD_SortamPM=Name_NTD_SortamPM&#13;&#10;MarkaPM=MarkaPM&#13;&#10;Type_NTD_MarkaPM=Type_NTD_MarkaPM&#13;&#10;Name_NTD_MarkaPM=Name_NTD_MarkaPM&#13;&#10;Name_PredmFM=Name_PredmFM&#13;&#10;SortamFM=SortamFM&#13;&#10;Type_NTD_SortamFM=Type_NTD_SortamFM&#13;&#10;Name_NTD_SortamFM=Name_NTD_SortamFM&#13;&#10;MarkaFM=MarkaFM&#13;&#10;Type_NTD_MarkaFM=Type_NTD_MarkaFM&#13;&#10;Name_NTD_MarkaFM=Name_NTD_MarkaFM&#13;&#10;N_Sert=N_Sert&#13;&#10;Date_Sert=Date_Sert&#13;&#10;Cod_Plavka=Cod_Plavka&#13;&#10;Party=Party&#13;&#10;RegN_Sert=RegN_Sert&#13;&#10;RegN=RegN&#13;&#10;Key_Sert=Key_Sert&#13;&#10;Key_Sert_Pos=Key_Sert_Pos&#13;&#10;Key_PredmPM=Key_PredmPM&#13;&#10;Key_PredmFM=Key_PredmFM&#13;&#10;Zamena_Docum=Zamena_Docum" BCDToCurrency="False" Master="qClient" IgnoreDupParams="False" SQL.Text="DECLARE @Key_Sub_Ver_AES Int;&#13;&#10;DECLARE @Date_Sert Date;&#13;&#10;DECLARE @Key_VKZ_Sost Int;               &#13;&#10;DECLARE @SkipTB1 Int;&#13;&#10;&#13;&#10;SET @Key_Sub_Ver_AES = :Key_Sub_Ver_AES;&#13;&#10;SET @Date_Sert =       :Date_Sert;&#13;&#10;SET @Key_VKZ_Sost =    :Key_VKZ_Sost;    &#13;&#10;SET @SkipTB1 =         :SkipTB1;&#13;&#10;  &#13;&#10;/*&#13;&#10;[dsCertificate.&#34;Key_Sert&#34;]  &#13;&#10;[dsCertificate.&#34;N_Sert&#34;]&#13;&#10;[dsCertificate.&#34;Date_Sert&#34;]&#13;&#10;[dsCertificate.&#34;RegN_Sert&#34;]    &#13;&#10;*/      &#13;&#10;  &#13;&#10;SELECT DISTINCT                     &#13;&#10;    P_VKZ.Cod_Zak,&#13;&#10;    [IZD].[Key_VKZ] AS [Key_VKZIZD],          &#13;&#10;    [IZD].Cod_SosZ,&#13;&#10;    [IZD].Key_VKZ_Sost,&#13;&#10;    [IZDS].Name_VKZ_Sost,&#13;&#10;    [IZDPR].Cod_Predm  AS Cod_PredmIZD,&#13;&#10;    [IZDPR].Name_Predm AS Name_PredmIZD,&#13;&#10;    T_VPredm.OrderCod_Predm,                &#13;&#10;    T_VPredm.Cod_Predm  AS Cod_PredmDT, &#13;&#10;    T_VPredm.Name_Predm AS Name_PredmDT, &#13;&#10;    Pr_Pasp.Cod_Pasp,&#13;&#10;    Pr_Pasp.PaspKolvo,&#13;&#10;    CASE&#13;&#10;      WHEN [Pr_Pasp].[ZavNumFrom] = [Pr_Pasp].[ZavNumTo]&#13;&#10;        THEN CAST([Pr_Pasp].[ZavNumFrom] AS varchar(3))&#13;&#10;        ELSE CONCAT(CAST([Pr_Pasp].[ZavNumFrom] AS varchar(3)), ' – ', CAST([Pr_Pasp].[ZavNumTo] AS varchar(3)))&#13;&#10;    END                         AS ZavNumber,  &#13;&#10;    Pr_SP.Key_ID_Mat, &#13;&#10;    T_VPredm_1.Name_Predm       AS Name_PredmPM, &#13;&#10;    T_VPredm_1.Sortam SortamPM,&#13;&#10;    T_VPredm_1.Type_NTD_Sortam Type_NTD_SortamPM, &#13;&#10;    T_VPredm_1.Name_NTD_Sortam Name_NTD_SortamPM, &#13;&#10;    T_VPredm_1.Marka MarkaPM, &#13;&#10;    T_VPredm_1.Type_NTD_Marka Type_NTD_MarkaPM, &#13;&#10;    T_VPredm_1.Name_NTD_Marka Name_NTD_MarkaPM,&#13;&#10;    T_VPredm_2.Name_Predm       AS Name_PredmFM, &#13;&#10;    T_VPredm_2.Sortam           AS SortamFM, &#13;&#10;    T_VPredm_2.Type_NTD_Sortam  AS Type_NTD_SortamFM, &#13;&#10;    T_VPredm_2.Name_NTD_Sortam  AS Name_NTD_SortamFM,&#13;&#10;    T_VPredm_2.Marka            AS MarkaFM, &#13;&#10;    T_VPredm_2.Type_NTD_Marka   AS Type_NTD_MarkaFM, &#13;&#10;    T_VPredm_2.Name_NTD_Marka   AS Name_NTD_MarkaFM, &#13;&#10;    A_VID_Mat.N_Sert N_Sert, &#13;&#10;    A_VID_Mat.Date_Sert,&#13;&#10;    A_VID_Mat.Cod_Plavka, &#13;&#10;    A_VID_Mat.Party,    &#13;&#10;    A_VID_Mat.RegN_Sert,&#13;&#10;    A_VID_Mat.RegN,&#13;&#10;    A_VID_Mat.Key_Sert          AS Key_Sert,        &#13;&#10;    A_VID_Mat.Key_Sert_Pos      AS Key_Sert_Pos,&#13;&#10;    T_VPredm_1.Key_Predm Key_PredmPM,&#13;&#10;    T_VPredm_2.Key_Predm Key_PredmFM,&#13;&#10;    Pr_SP.Zamena_Docum             &#13;&#10;  FROM&#13;&#10;    P_VKZ&#13;&#10;    INNER JOIN P_VKZ AS [IZD]&#13;&#10;            ON [IZD].[Key_VKZ] = [P_VKZ].[Key_VKZ_Izdel]&#13;&#10;    INNER JOIN T_VPredm AS [IZDPR]&#13;&#10;            ON [IZDPR].[Key_Predm] = [IZD].[Key_Predm]&#13;&#10;    INNER JOIN P_VKZ_Sost AS [IZDS]&#13;&#10;            ON [IZDS].[Key_VKZ_Sost] = [IZD].[Key_VKZ_Sost]&#13;&#10;    INNER JOIN T_VPredm &#13;&#10;            ON T_VPredm.Key_Predm = P_VKZ.Key_Predm       &#13;&#10;    INNER JOIN Pr_PaspDet &#13;&#10;            ON Pr_PaspDet.Key_VKZ = P_VKZ.Key_VKZ       &#13;&#10;    INNER JOIN Pr_Pasp &#13;&#10;            ON Pr_Pasp.Key_Pasport = Pr_PaspDet.Key_Pasport       &#13;&#10;    INNER JOIN Pr_SP &#13;&#10;            ON Pr_SP.Key_Pasport = Pr_Pasp.Key_Pasport      &#13;&#10;    INNER JOIN T_SPRows &#13;&#10;            ON T_SPRows.Key_SPRow = Pr_SP.Key_SPRow   &#13;&#10;    INNER JOIN T_SP&#13;&#10;            ON T_SP.Key_SP =  T_SPRows.Key_SP                                                                  &#13;&#10;    INNER JOIN A_VID_Mat &#13;&#10;            ON A_VID_Mat.Key_ID_Mat = Pr_SP.Key_ID_Mat    &#13;&#10;    LEFT  JOIN T_VPredm AS T_VPredm_2 &#13;&#10;            ON T_VPredm_2.Key_Predm = A_VID_Mat.Key_Predm    &#13;&#10;    LEFT  JOIN T_VPredm AS T_VPredm_1 &#13;&#10;            ON T_VPredm_1.Key_Predm = T_SPRows.Key_Predm&#13;&#10;&#13;&#10;  WHERE&#13;&#10;   (P_VKZ.Key_VKZ_Izdel in (SELECT Key_VKZ_Izdel FROM P_VKZ WHERE P_VKZ.Key_VKZ = P_VKZ.Key_VKZ_Izdel AND P_VKZ.Key_Sub_Ver_AES = @Key_Sub_Ver_AES))&#13;&#10;    AND (EXISTS(SELECT [T_SP].[Key_SP] FROM [T_SP] WHERE [T_SP].[Key_Predm] = [T_VPredm].[Key_Predm] AND [T_SP].[InTB1] = 1) OR (1 = @SkipTB1))&#13;&#10;    AND A_VID_Mat.Date_Sert &#60; @Date_Sert&#13;&#10;    AND [IZD].Key_VKZ_Sost = @Key_VKZ_Sost                &#13;&#10;    AND T_SPRows.SP_Position = 0&#13;&#10;&#13;&#10;  ORDER BY&#13;&#10;    [A_VID_Mat].[Key_Sert] ASC,&#13;&#10;    [IZD].[Key_VKZ] ASC,          &#13;&#10;    [T_VPredm].[OrderCod_Predm] ASC;  " PropData=""/>
