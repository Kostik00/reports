    <TfrxERQuery Name="qRS" UserName="dsRS" CloseDataSource="False" FieldAliases.Text="Cod_Zak=Cod_Zak&#13;&#10;Cod_Predm_Izdel=Cod_Predm_Izdel" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="WITH FM AS&#13;&#10;  (SELECT DISTINCT&#13;&#10;      P_VKZ.Key_GrZap,&#13;&#10;      A_VID_Mat.Key_Id_Mat, &#13;&#10;      A_VID_Mat.VidSert, &#13;&#10;      A_VID_Mat.N_Sert, &#13;&#10;      A_VID_Mat.Date_Sert,&#13;&#10;      A_VID_Mat.Cod_Plavka,&#13;&#10;      A_VID_Mat.Party,&#13;&#10;      A_VID_Mat.RegN_Sert,&#13;&#10;      T_VPredm.PrintFlag,                           &#13;&#10;      T_VPredm.Cod_Predm, &#13;&#10;      T_VPredm.Name_Predm, &#13;&#10;      T_VPredm.Sortam, &#13;&#10;      T_VPredm.OrderSortam, &#13;&#10;      T_VPredm.Marka, &#13;&#10;      T_VPredm.Type_NTD_Sortam, &#13;&#10;      T_VPredm.Name_NTD_Sortam, &#13;&#10;      T_VPredm.Type_NTD_Marka, &#13;&#10;      T_VPredm.Name_NTD_Marka,  &#13;&#10;      dbo.A_IdMat_TOInfo(A_VID_Mat.Key_Id_Mat) AS TOInfo,&#13;&#10;      P_VKZ_Zap.Key_VKZ_Zap,&#13;&#10;      DSE.Cod_Predm AS Cod_Predm_DSE   &#13;&#10;    FROM            &#13;&#10;      P_VKZ&#13;&#10;        INNER JOIN P_VKZ_Zap_Det   ON P_VKZ_Zap_Det.Key_VKZ = P_VKZ.Key_VKZ&#13;&#10;        INNER JOIN P_VKZ_Zap       ON P_VKZ_Zap.Key_VKZ_Zap = P_VKZ_Zap_Det.Key_VKZ_Zap&#13;&#10;        INNER JOIN P_VMZ 		        ON P_VMZ.Key_VKZ_Zap     = P_VKZ_Zap.Key_VKZ_Zap&#13;&#10;        INNER JOIN T_SPRows		      ON P_VMZ.Key_SPRow       = T_SPRows.Key_SPRow &#13;&#10;        INNER JOIN A_Rezerv		      ON A_Rezerv.Key_VMZ      = P_VMZ.Key_VMZ AND A_Rezerv.Key_Id_Mat = :Key_Id_Mat            &#13;&#10;        INNER JOIN A_VID_Mat		     ON A_VID_Mat.Key_ID_Mat  = A_Rezerv.Key_Id_Mat       &#13;&#10;        INNER JOIN T_VPredm		      ON T_VPredm.Key_Predm    = A_VID_Mat.Key_Predm        &#13;&#10;        INNER JOIN T_VPredm AS DSE	ON DSE.Key_Predm         = P_VKZ_Zap.Key_Predm  &#13;&#10;    WHERE        &#13;&#10;      (P_VKZ.Key_GrZap &#60;&#62; :Key_GrZap ) AND&#13;&#10;      (T_SPRows.SP_position = 0)       AND&#13;&#10;      ((P_VKZ_Zap.Key_Pasport IS NULL) OR (:PrintAll = 1))),&#13;&#10;GZ AS&#13;&#10;  (SELECT&#13;&#10;      P_VKZ.Cod_Zak,&#13;&#10;      T_VPredm_Izdel.Cod_Predm&#13;&#10;    FROM&#13;&#10;  	  P_VKZ&#13;&#10;        INNER JOIN D_Zakaz                    ON D_Zakaz.Cod_Zak = P_VKZ.Cod_Zak&#13;&#10;        INNER JOIN T_VPredm                   ON T_VPredm.Key_Predm = P_VKZ.Key_Predm&#13;&#10;        INNER JOIN P_VKZ AS P_VKZ_Izdel       ON P_VKZ_Izdel.Key_VKZ = P_VKZ.Key_VKZ_Izdel&#13;&#10;        INNER JOIN T_VPredm AS T_VPredm_Izdel ON T_VPredm_Izdel.Key_Predm = P_VKZ_Izdel.Key_Predm&#13;&#10;        INNER JOIN P_VKZ_Sost                 ON P_VKZ_Sost.Key_VKZ_Sost = P_VKZ.Key_VKZ_Sost&#13;&#10;    WHERE&#13;&#10;      (P_VKZ.Key_GrZap = :Key_GrZap)&#13;&#10;    GROUP BY&#13;&#10;        P_VKZ.Cod_Zak,&#13;&#10;        T_VPredm_Izdel.Cod_Predm)&#13;&#10;&#13;&#10;SELECT&#13;&#10;    P_VKZ_Izdel.Cod_Zak,      &#13;&#10;    T_VPredm_Izdel.Cod_Predm     AS Cod_Predm_Izdel&#13;&#10;  FROM            &#13;&#10;    P_VKZ_Zap&#13;&#10;      INNER JOIN &#13;&#10;        (SELECT&#13;&#10;            P_VKZ_Zap_Det.Key_VKZ_Zap,&#13;&#10;            MIN(P_VKZ.DataZapUtv) AS DataZapUtv,&#13;&#10;            P_VKZ.Cod_SosZ AS Cod_SosPos                                         &#13;&#10;          FROM&#13;&#10;            P_VKZ_Zap_Det&#13;&#10;              INNER JOIN P_VKZ ON P_VKZ.Key_VKZ = P_VKZ_Zap_Det.Key_VKZ AND P_VKZ.Cod_SosZ = 'Д'                                        &#13;&#10;          GROUP BY&#13;&#10;            P_VKZ.Cod_SosZ, P_VKZ_Zap_Det.Key_VKZ_Zap) AS TZapUtv&#13;&#10;                                            ON TZapUtv.Key_VKZ_Zap      = P_VKZ_Zap.Key_VKZ_Zap&#13;&#10;      INNER JOIN P_VKZ    AS P_VKZ_Izdel    ON P_VKZ_Izdel.Key_VKZ      = P_VKZ_Zap.Key_VKZ_Izdel AND P_VKZ_Izdel.Cod_SosZ = 'Д'        &#13;&#10;      INNER JOIN T_VPredm AS T_VPredm_Izdel ON T_VPredm_Izdel.Key_Predm = P_