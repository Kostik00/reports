    <TfrxERQuery Name="TrudQRY" UserName="TrudQRY" CloseDataSource="False" FieldAliases.Text="Key_Sub_Ver=Key_Sub_Ver&#13;&#10;Name_Sub=Name_Sub&#13;&#10;F_Name_Sub=F_Name_Sub&#13;&#10;Cnt=Cnt&#13;&#10;AcTrud=AcTrud&#13;&#10;Cnt1=Cnt1&#13;&#10;AcTrud1=AcTrud1&#13;&#10;Cnt2=Cnt2&#13;&#10;AcTrud2=AcTrud2" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SET NOCOUNT ON;&#13;&#10;&#13;&#10;DECLARE @DateFrom datetime;&#13;&#10;DECLARE @DateTo   datetime;    &#13;&#10;SET @DateFrom = :DateFrom;&#13;&#10;SET @DateTo   = :DateTo;&#13;&#10;  &#13;&#10;SET NOCOUNT OFF;&#13;&#10;&#13;&#10;SELECT&#13;&#10;    T0.Key_Sub_Ver,&#13;&#10;    MAX(LTRIM(T0.Name_Sub)) AS Name_Sub,&#13;&#10;    MAX(LTRIM(T0.F_Name_Sub)) AS F_Name_Sub,&#13;&#10;    SUM(T2.Cnt) AS Cnt,&#13;&#10;    SUM(T2.AcTrud) AS AcTrud,&#13;&#10;    SUM(T3.Cnt) AS Cnt1,&#13;&#10;    SUM(T3.AcTrud) AS AcTrud1,&#13;&#10;    SUM(T4.Cnt) AS Cnt2,&#13;&#10;    SUM(T4.AcTrud) AS AcTrud2&#13;&#10;FROM&#13;&#10;(&#13;&#10;SELECT  DISTINCT   Pr_Pasp_Ac.Key_Sub_Ver, S_Subjects.Name_Sub, S_SubVer.F_Name_Sub&#13;&#10;FROM         Pr_Pasp_Ac INNER JOIN&#13;&#10;                      S_SubVer ON Pr_Pasp_Ac.Key_Sub_Ver = S_SubVer.Key_Sub_Ver INNER JOIN&#13;&#10;                      S_Subjects ON S_SubVer.Key_Sub = S_Subjects.Key_Sub&#13;&#10;		WHERE &#13;&#10;		  Pr_Pasp_Ac.AC_Date &#62;= @DateFrom&#13;&#10;		  AND Pr_Pasp_Ac.AC_Date &#60;= @DateTo&#13;&#10;&#13;&#10;) T0&#13;&#10;CROSS JOIN #Oper  T1 &#13;&#10;&#13;&#10;INNER JOIN&#13;&#10;( &#13;&#10;&#13;&#10;SELECT     &#13;&#10;	Pr_Pasp_Ac.Key_Sub_Ver,&#13;&#10;	T_Oper.Cod_Oper,&#13;&#10;	count(Pr_Pasp_Rows.Key_Pasp_Rows) AS Cnt, &#13;&#10;	SUM(Pr_Pasp_Rows.Tsm * Pr_Pasp_Ac.AcKolvo/Pr_Pasp.PaspKolvo*Pr_PaspDet.KolvoZap/Pr_Pasp.PaspKolvo) AS AcTrud&#13;&#10;FROM  &#13;&#10;	(&#13;&#10;		SELECT&#13;&#10;			Pr_Pasp_Ac.Key_Sub_Ver,     &#13;&#10;			Pr_Pasp_Ac.Key_Pasp_Rows, &#13;&#10;			SUM(Pr_Pasp_Ac.AcKolvo) AS AcKolvo&#13;&#10;		FROM         &#13;&#10;			Pr_Pasp_Ac &#13;&#10;		WHERE &#13;&#10;		  Pr_Pasp_Ac.AC_Date &#62;= @DateFrom&#13;&#10;		  AND Pr_Pasp_Ac.AC_Date &#60;= @DateTo&#13;&#10;		    &#13;&#10;		GROUP BY  Pr_Pasp_Ac.Key_Sub_Ver, Pr_Pasp_Ac.Key_Pasp_Rows&#13;&#10;	)       Pr_Pasp_Ac &#13;&#10;&#13;&#10;	INNER JOIN Pr_Pasp_Rows &#13;&#10;		ON Pr_Pasp_Ac.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows &#13;&#10;	INNER JOIN Pr_Pasp&#13;&#10;		ON Pr_Pasp.Key_Pasport = Pr_Pasp_Rows.Key_Pasport&#13;&#10;	INNER JOIN T_Oper &#13;&#10;		ON Pr_Pasp_Rows.Cod_Oper = T_Oper.Cod_Oper&#13;&#10;	INNER JOIN Pr_PaspDet&#13;&#10;	    ON Pr_PaspDet.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;	INNER JOIN P_VKZ PVKZ1&#13;&#10;	    ON PVKZ1.Key_VKZ = Pr_PaspDet.Key_VKZ	    &#13;&#10;	INNER JOIN P_VKZ PVKZ2&#13;&#10;	    ON PVKZ1.Key_VKZ_Izdel = PVKZ2.Key_VKZ AND PVKZ2.Key_VKZ_Master IS NULL AND PVKZ2.Key_VKZ IN (SELECT Key_VKZ FROM #VKZ)	    	      &#13;&#10;	    &#13;&#10;WHERE Pr_Pasp.PaspKolvo &#62; 0&#13;&#10;&#13;&#10;GROUP BY Pr_Pasp_Ac.Key_Sub_Ver, T_Oper.Cod_Oper&#13;&#10;) T2&#13;&#10;ON&#13;&#10;  T1.Cod_Oper = T2.Cod_Oper AND&#13;&#10;  T1.Cod_KLOB = 'Y' AND&#13;&#10;  T2.Key_Sub_Ver = T0.Key_Sub_Ver&#13;&#10;&#13;&#10;&#13;&#10;LEFT JOIN&#13;&#10;( &#13;&#10;SELECT     &#13;&#10;	Pr_Pasp_Ac.Key_Sub_Ver,&#13;&#10;	T_Oper.Cod_Oper,	&#13;&#10;	count(Pr_Pasp_Rows.Key_Pasp_Rows) AS Cnt, &#13;&#10;	SUM(Pr_Pasp_Rows.Tsm * Pr_Pasp_Ac.AcKolvo/Pr_Pasp.PaspKolvo*Pr_PaspDet.KolvoZap/Pr_Pasp.PaspKolvo) AS AcTrud&#13;&#10;FROM  &#13;&#10;	(&#13;&#10;		SELECT     &#13;&#10;			Pr_Pasp_Ac.Key_Sub_Ver,&#13;&#10;			Pr_Pasp_Ac.Key_Pasp_Rows, &#13;&#10;			SUM(Pr_Pasp_Ac.AcKolvo) AS AcKolvo&#13;&#10;		FROM         &#13;&#10;			Pr_Pasp_Ac &#13;&#10;		WHERE &#13;&#10;		  Pr_Pasp_Ac.AC_Date &#62;= @DateFrom&#13;&#10;		  AND Pr_Pasp_Ac.AC_Date &#60;= @DateTo&#13;&#10;		GROUP BY  Pr_Pasp_Ac.Key_Sub_Ver, Pr_Pasp_Ac.Key_Pasp_Rows&#13;&#10;	)       Pr_Pasp_Ac &#13;&#10;&#13;&#10;	INNER JOIN Pr_Pasp_Rows &#13;&#10;		ON Pr_Pasp_Ac.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows &#13;&#10;  AND Pr_Pasp_Rows.Key_TPRows is not  null                                                                                   &#13;&#10;	INNER JOIN Pr_Pasp&#13;&#10;		ON Pr_Pasp.Key_Pasport = Pr_Pasp_Rows.Key_Pasport&#13;&#10;	INNER JOIN T_Oper &#13;&#10;		ON Pr_Pasp_Rows.Cod_Oper = T_Oper.Cod_Oper &#13;&#10;	INNER JOIN Pr_PaspDet&#13;&#10;	    ON Pr_PaspDet.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;	INNER JOIN P_VKZ PVKZ1&#13;&#10;	    ON PVKZ1.Key_VKZ = Pr_PaspDet.Key_VKZ	    &#13;&#10;	INNER JOIN P_VKZ PVKZ2&#13;&#10;	    ON PVKZ1.Key_VKZ_Izdel = PVKZ2.Key_VKZ AND PVKZ2.Key_VKZ_Master IS NULL AND PVKZ2.Key_VKZ IN (SELECT Key_VKZ FROM #VKZ)	    	    &#13;&#10;&#13;&#10;WHERE Pr_Pasp.PaspKolvo &#62; 0&#13;&#10;&#13;&#10;GROUP BY Pr_Pasp_Ac.Key_Sub_Ver, T_Oper.Cod_Oper&#13;&#10;) T3&#13;&#10;ON&#13;&#10;  T1.Cod_Oper = T3.Cod_Oper AND&#13;&#10;  T1.Cod_KLOB = 'Y' AND&#13;&#10;  T3.Key_Sub_Ver = T0.Key_Sub_Ver&#13;&#10;&#13;&#10;LEFT JOIN&#13;&#10;( &#13;&#10;SELECT&#13;&#10;	Pr_Pasp_Ac.Key_Sub_Ver,     &#13;&#10;	T_Oper.Cod_Oper,	&#13;&#10;	count(Pr_Pasp_Rows.Key_Pasp_Rows) AS Cnt, &#13;&#10;	SUM(Pr_Pasp_Rows.Tsm * Pr_Pasp_Ac.AcKolvo/Pr_Pasp.PaspKolvo*Pr_PaspDet.KolvoZap/Pr_Pasp.PaspKolvo) AS AcTrud&#13;&#10;FROM  &#13;&#10;	(&#13;&#10;		SELECT     &#13;&#10;			Pr_Pasp_Ac.Key_Sub_Ver, &#13;&#10;			Pr_Pasp_Ac.Key_Pasp_Rows, &#13;&#10;			SUM(Pr_Pasp_Ac.AcKolvo) AS AcKolvo&#13;&#10;		FROM         &#13;&#10;			Pr_Pasp_Ac &#13;&#10;		WHERE &#13;&#10;		  Pr_Pasp_Ac.AC_Date &#62;= @DateFrom&#13;&#10;		  AND Pr_Pasp_Ac.AC_Date &#60;= @DateTo&#13;&#10;		GROUP BY  Pr_Pasp_Ac.Key_Sub_Ver, Pr_Pasp_Ac.Key_Pasp_Rows&#13;&#10;	)       Pr_Pasp_Ac &#13;&#10;&#13;&#10;	INNER JOIN Pr_Pasp_Rows &#13;&#10;		ON Pr_Pasp_Ac.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows &#13;&#10;  AND Pr_Pasp_Rows.Key_TPRows is null                                                                       &#13;&#10;	INNER JOIN Pr_Pasp&#13;&#10;		ON Pr_Pasp.Key_Pasport = Pr_Pasp_Rows.Key_Pasport&#13;&#10;	INNER JOIN T_Oper &#13;&#10;		ON Pr_Pasp_Rows.Cod_Oper = T_Oper.Cod_Oper&#13;&#10;	INNER JOIN Pr_PaspDet&#13;&#10;	    ON Pr_PaspDet.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;	INNER JOIN P_VKZ PVKZ1&#13;&#10;	    ON PVKZ1.Key_VKZ = Pr_PaspDet.Key_VKZ	    &#13;&#10;	INNER JOIN P_VKZ PVKZ2&#13;&#10;	    ON PVKZ1.Key_VKZ_Izdel = PVKZ2.Key_VKZ AND PVKZ2.Key_VKZ_Master IS NULL AND PVKZ2.Key_VKZ IN (SELECT Key_VKZ FROM #VKZ)	    	    &#13;&#10;&#13;&#10;WHERE Pr_Pasp.PaspKolvo &#62; 0&#13;&#10;&#13;&#10;GROUP BY Pr_Pasp_Ac.Key_Sub_Ver, T_Oper.Cod_Oper&#13;&#10;) T4&#13;&#10;ON&#13;&#10;  T1.Cod_Oper = T4.Cod_Oper AND&#13;&#10;  T1.Cod_KLOB = 'Y' AND&#13;&#10;  T4.Key_Sub_Ver = T0.Key_Sub_Ver&#13;&#10;&#13;&#10;GROUP BY T0.Key_Sub_Ver&#13;&#10;ORDER BY F_Name_Sub;&#13;&#10;&#13;&#10;DROP TABLE #VKZ;                       " PropData=""/>
    <TfrxERQuery Name="qPreCreate" UserName="dsPreCreate" CloseDataSource="False" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SET NOCOUNT ON;  &#13;&#10;&#13;&#10;IF object_id(N'tempdb..#VKZ') is not null&#13;&#10;    TRUNCATE TABLE #VKZ    &#13;&#10;  ELSE          &#13;&#10;    CREATE TABLE #VKZ       &#13;&#10;      (&#13;&#10;      RowNumber Int not null,                                     &#13;&#10;      Key_VKZ Integer not null,&#13;&#10;      Cod_Zak varchar(30) not null,          &#13;&#10;      Cod_Predm varchar(100) not null,&#13;&#10;      Name_Predm varchar(500) null          &#13;&#10;      );  &#13;&#10;  &#13;&#10;IF object_id(N'tempdb..#Oper') is not null&#13;&#10;    TRUNCATE TABLE #Oper&#13;&#10;  ELSE          &#13;&#10;    CREATE TABLE #Oper   &#13;&#10;      (&#13;&#10;      Cod_Oper varchar(4) not null,&#13;&#10;      Name_Oper varchar(70) not null,          &#13;&#10;      Cod_KLOB varchar(11) not null DEFAULT 'Y'                               &#13;&#10;      );  &#13;&#10;  &#13;&#10;INSERT INTO&#13;&#10;    #Oper (Cod_Oper, Name_Oper, Cod_KLOB)&#13;&#10;  SELECT DISTINCT &#13;&#10;    Cod_Oper, Name_Oper, 'Y'           &#13;&#10;  FROM T_Oper;&#13;&#10;  &#13;&#10;SET NOCOUNT OFF;  &#13;&#10;&#13;&#10;SELECT TOP 1 Cod_Oper, Name_Oper, Cod_KLOB FROM #Oper;      " PropData="05704C656674038C000470546F7002280A506172616D65746572730100"/>
    <TfrxERQuery Name="qTOperGr" UserName="dsTOperGr" CloseDataSource="False" FieldAliases.Text="Cod_Oper_Gr=Cod_Oper_Gr&#13;&#10;Name_Oper_Gr=Name_Oper_Gr&#13;&#10;Name_GrGr=Name_GrGr" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT&#13;&#10;    [T].[Cod_Oper_Gr],  &#13;&#10;    [T].[Name_Oper_Gr],  &#13;&#10;    [T].[Name_GrGr]&#13;&#10;  FROM&#13;&#10;    [T_Oper_Gr] AS [T]              " PropData="05704C656674023C0470546F7002780A506172616D65746572730100"/>
