VKZ_Izdel&#13;&#10;         &#13;&#10;       INNER JOIN P_VKZ&#13;&#10;               ON   P_VKZ.Key_VKZ_Izdel = P_VKZ_Izdel.Key_VKZ_Izdel&#13;&#10;               AND  P_VKZ.CodStrSv like P_VKZ_Izdel.CodStrSv + '%'                                                            &#13;&#10;--               AND  P_VKZ.Key_VKZ = P_VKZ_Izdel.Key_VKZ                                                            &#13;&#10;                   &#13;&#10;       INNER JOIN T_VPredm AS DSE&#13;&#10;               ON DSE.Key_Predm = P_VKZ.Key_Predm  &#13;&#10;       INNER JOIN T_SPRows &#13;&#10;               ON T_SPRows.Key_SP = P_VKZ.Key_SP&#13;&#10;               AND T_SPRows.SP_Position &#62;0&#13;&#10;                   &#13;&#10;       INNER JOIN T_VPredm &#13;&#10;              ON T_SPRows.Key_Predm = T_VPredm.Key_Predm&#13;&#10;              AND T_VPredm.Cod_Postav = '8'&#13;&#10;              AND ((T_VPredm.Cod_Prin &#60;&#62; '50') or :PrintM = 1)&#13;&#10;              AND ((T_VPredm.Cod_Prin &#60;&#62; '51') or :PrintSvrm = 1)                 &#13;&#10;  &#13;&#10;                 &#13;&#10;       INNER JOIN Gr_VxodPredm&#13;&#10;               ON Gr_VxodPredm.Key_Predm   = T_VPredm.Key_Predm&#13;&#10;               AND Gr_VxodPredm.Key_VidGr  =   1&#13;&#10;       INNER JOIN Gr_Groups&#13;&#10;               ON Gr_Groups.Cod_Gr = Gr_VxodPredm.Cod_Gr&#13;&#10;               AND Gr_Groups.Key_VidGr = Gr_VxodPredm.Key_VidGr                                          &#13;&#10;                 &#13;&#10;WHERE     &#13;&#10;    (P_VKZ_Izdel.Key_VKZ = :Key_VKZ)&#13;&#10;                             &#13;&#10;        &#13;&#10;ORDER BY&#13;&#10;&#13;&#10;	T_VPredm.Cod_Postav, &#13;&#10;	T_VPredm.Cod_Prin, &#13;&#10;	Gr_Groups.Cod_Gr,   &#13;&#10;	T_VPredm.Name_Predm, &#13;&#10;	T_VPredm.OrderSortam       " PropData=""/>
