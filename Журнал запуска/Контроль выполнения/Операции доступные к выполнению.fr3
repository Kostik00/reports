    <TfrxERQuery Name="SMQry" UserName="Стат" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="WITH PR as&#13;&#10;(&#13;&#10;	SELECT  &#13;&#10;			pr.Key_Pasport,&#13;&#10;			pr.Key_Pasp_Rows,&#13;&#10;			AcDate,&#13;&#10;			LAG(AcDate)  OVER (PARTITION BY pr.Key_Pasport ORDER BY  N_Oper_SM) DateAV&#13;&#10;		FROM &#13;&#10;			Pr_Pasp_Rows  pr&#13;&#10;	-- 	WHERE pr.Key_Pasport = 159028&#13;&#10;) ,&#13;&#10;MOVETBL AS (&#13;&#10;&#13;&#10;	SELECT &#13;&#10;		PR.Key_Pasport ,&#13;&#10;		PR.Key_Pasp_Rows,&#13;&#10;		pr.AcDate, &#13;&#10;		Isnull(pr.DateAV, pr.AcDate) avDate ,&#13;&#10;		Row_Number() OVER (PARTITION BY pr.Key_Pasp_Rows ORDER BY FirstEv DESC) RN1,&#13;&#10;		SUBSTRING( pe.AdData,22,500) FIO,&#13;&#10;		pe.FirstEv&#13;&#10;&#13;&#10;	FROM  PR PR&#13;&#10;		LEFT JOIN  Pr_Pasp_Ev pe&#13;&#10;				ON  pe.Key_Pasport = pr.Key_Pasport&#13;&#10;				AND pe.FirstEv &#60; Isnull( pr.AcDate , pr.DateAV  )&#13;&#10;				and pe.AdData like 'Передача паспорта -&#62;%'&#13;&#10;&#13;&#10;)&#13;&#10;&#13;&#10;&#13;&#10;--SELECT &#13;&#10;--	MOVETBL.*,&#13;&#10;--	MOVETBL.FIO,&#13;&#10;--	PR.N_Oper_Sm,&#13;&#10;--	T_Oper.Name_K_Oper,&#13;&#10;--	DatePart(YY, MOVETBL.avDate) Y,&#13;&#10;--	DatePart(MM, MOVETBL.avDate) M&#13;&#10;----	,SUM(PR.TSm) TSm&#13;&#10;--FROM &#13;&#10;--	MOVETBL&#13;&#10;--	INNER JOIN Pr_Pasp_Rows PR&#13;&#10;--		ON PR.Key_Pasp_Rows = MOVETBL.Key_Pasp_Rows&#13;&#10;--	INNER JOIN T_Oper&#13;&#10;--		ON T_Oper.Cod_Oper = PR.Cod_Oper&#13;&#10;&#13;&#10;--WHERE RN1 = 1&#13;&#10;----AND FIO LIKE 'горя%'&#13;&#10;----AND T_Oper.Name_K_Oper like 'Слес%'&#13;&#10;--AND MOVETBL.Key_Pasport = 158167 -- 158423 -- 159028&#13;&#10;--ORDER BY N_Oper_Sm&#13;&#10; &#13;&#10;&#13;&#10;&#13;&#10;SELECT &#13;&#10; &#13;&#10;	MOVETBL.FIO,&#13;&#10;	T_Oper.Name_K_Oper,&#13;&#10;	DatePart(YY, MOVETBL.FirstEv) Y,&#13;&#10;	DatePart(MM, MOVETBL.FirstEv) M,&#13;&#10;	SUM(PR.TSm) TSm&#13;&#10;FROM &#13;&#10;	MOVETBL&#13;&#10;	INNER JOIN Pr_Pasp_Rows PR&#13;&#10;		ON PR.Key_Pasp_Rows = MOVETBL.Key_Pasp_Rows&#13;&#10;	INNER JOIN T_Oper&#13;&#10;		ON T_Oper.Cod_Oper = PR.Cod_Oper&#13;&#10;&#13;&#10;WHERE RN1 = 1&#13;&#10;AND FIO LIKE :FIO         &#13;&#10;AND T_Oper.Name_K_Oper like :Oper        &#13;&#10;&#13;&#10;GROUP BY&#13;&#10;	MOVETBL.FIO,&#13;&#10;	T_Oper.Name_K_Oper,&#13;&#10;	DatePart(YY, MOVETBL.FirstEv),&#13;&#10;	DatePart(MM, MOVETBL.FirstEv)&#13;&#10;ORDER BY FIO, T_Oper.Name_K_Oper, Y,M    " PropData=""/>
