    <TfrxERQuery Name="SostPK" UserName="ÑîñòÏÊ" CloseDataSource="True" FieldAliases.Text="Key_VKZ_Izdel=Key_VKZ_Izdel&#13;&#10;Key_Pr_PK=Key_Pr_PK&#13;&#10;Cod_PK=Cod_PK&#13;&#10;Key_Pk_Str=Key_Pk_Str&#13;&#10;Cod_Pk_Str=Cod_Pk_Str&#13;&#10;Cod_Pk_Str_Sort=Cod_Pk_Str_Sort&#13;&#10;Pk_Str_Txt=Pk_Str_Txt&#13;&#10;Cod_Predm=Cod_Predm&#13;&#10;OrderCod_Predm=OrderCod_Predm&#13;&#10;Name_Predm=Name_Predm&#13;&#10;CntByVKZ=CntByVKZ&#13;&#10;Key_TP=Key_TP&#13;&#10;Cod_TP=Cod_TP&#13;&#10;Key_TPRows=Key_TPRows&#13;&#10;N_OPER_MK=N_OPER_MK&#13;&#10;N_OPER_TP=N_OPER_TP&#13;&#10;Name_K_OperByPK=Name_K_OperByPK&#13;&#10;Key_VKZ_Zap=Key_VKZ_Zap&#13;&#10;KolvoZap=KolvoZap&#13;&#10;Key_Pasport=Key_Pasport&#13;&#10;Cod_Pasp=Cod_Pasp&#13;&#10;PaspKolvo=PaspKolvo&#13;&#10;Name_Pasp_Status=Name_Pasp_Status&#13;&#10;Date_Pasp_Status=Date_Pasp_Status&#13;&#10;Key_Pasp_Rows=Key_Pasp_Rows&#13;&#10;N_Oper_Sm=N_Oper_Sm&#13;&#10;Name_K_Oper=Name_K_Oper&#13;&#10;Name_Status_Oper=Name_Status_Oper&#13;&#10;AcDate=AcDate&#13;&#10;PrevCnt=PrevCnt&#13;&#10;PrevTrud=PrevTrud&#13;&#10;SMZ=SMZ&#13;&#10;Notes=Notes&#13;&#10;KontrCntAll=KontrCntAll&#13;&#10;KontrCntAc=KontrCntAc&#13;&#10;IsAc=IsAc&#13;&#10;AcNote=AcNote&#13;&#10;SubAc=SubAc" BCDToCurrency="False" Master="MainFrxDS" IgnoreDupParams="False" SQL.Text="&#13;&#10;&#13;&#10;DECLARE @Key_Pr_Pk int&#13;&#10;DECLARE @HideAc int&#13;&#10;DECLARE @ShowSZ int&#13;&#10;&#13;&#10;DECLARE @DateFrom DateTime&#13;&#10;DECLARE @DateTo DateTime&#13;&#10;&#13;&#10;&#13;&#10;SET @Key_Pr_Pk = :Key_Pr_Pk  &#13;&#10;SET @HideAc = :HideAc     &#13;&#10;SET @ShowSZ = :ShowSZ   &#13;&#10;&#13;&#10;SET @DateFrom = :DateFrom&#13;&#10;SET @DateTo = :DateTo&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;SELECT &#13;&#10;       Pr_Pk_SmData.*,  &#13;&#10;       dbo.Pr_PK_OperSZ (Pr_Pk_SmData.Key_Pasp_Rows  , '20010101'  , '29990101'  ) SMZ ,   &#13;&#10;       dbo.Pr_Pasp_GetNotes(Pr_Pk_SmData.Key_Pasport) Notes,&#13;&#10;       TCntKontr.CntAll KontrCntAll,&#13;&#