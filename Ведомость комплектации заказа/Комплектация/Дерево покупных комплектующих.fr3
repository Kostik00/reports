String = '';)&#13;&#10;var slTemp: TStringList;&#13;&#10;begin&#13;&#10;slTemp:=TStringList(Create);&#13;&#10;end;&#13;&#10;}&#13;&#10;&#13;&#10;function CertificateGrouping(AString: String): String;&#13;&#10;var&#13;&#10;  sSubString: String;&#13;&#10;  sTemp:      String;&#13;&#10;  i: Integer;&#13;&#10;  slStringList: TStringList;&#13;&#10;begin {поиск последовательностей крирллическая &#34;С&#34;, 4-е цифры, символ &#34;/&#34;, 2-е цифры}&#13;&#10;  slStringList:=TStringList.Create;&#13;&#10;  slStringList.Clear;&#13;&#10;  slStringList.Duplicates:=dupIgnore;&#13;&#10;  sSubString:=UpperCase(Trim(AString));{ Перевод символов в верхний регистр и удаление концевых пробелов }&#13;&#10;  while Pos('C', sSubString) &#62; 0{ Замена латинской 'C' на кирилическую 'С' }&#13;&#10;    do begin&#13;&#10;      i:=Pos('C', sSubString);&#13;&#10;      Delete(sSubString, i, 1);&#13;&#10;      Insert(sSubString, 'С', i)&#13;&#10;    end;&#13;&#10;  while Pos(',', sSubString) &#60;&#62; 0{ Удаление символа &#34;,&#34; }&#13;&#10;    do begin&#13;&#10;      i:=Pos(',', sSubString);&#13;&#10;      Delete(sSubString, i, 1)&#13;&#10;    end;&#13;&#10;  while Pos(' ', sSubString) &#60;&#62; 0{ Устранение пробелов }&#13;&#10;    do begin&#13;&#10;      i:=Pos(' ', sSubString);&#13;&#10;      Delete(sSubString, i, 1)&#13;&#10;    end;&#13;&#10;  sTemp:=FindRegExpr('С\d{4}/\d{2}', sSubString);&#13;&#10;  while ((Length(sSubString) &#62;= 8) AND (Length(sTemp) = 8))&#13;&#10;    do begin&#13;&#10;      slStringList.Sort;&#13;&#10;      slStringList.Duplicates:=dupIgnore;&#13;&#10;      if slStringList.IndexOf(sTemp) = -1 then slStringList.Add(sTemp);&#13;&#10;      Delete(sSubString, Pos(sTemp, sSubString), 8);&#13;&#10;      sTemp:=FindRegExpr('С\d{4}/\d{2}', sSubString)&#13;&#10;    end;&#13;&#10;  slStringList.Sort;&#13;&#10;  sTemp:='';&#13;&#10;  for i:=0 to slStringList.Count-1&#13;&#10;    do begin&#13;&#10;      if i = 0&#13;&#10;        then sTemp:=slStringList.Strings[i]&#13;&#1