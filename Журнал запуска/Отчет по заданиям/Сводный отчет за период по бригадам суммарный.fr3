    <TfrxERQuery Name="SwZ" UserName="ÑâÇàäàíèÿ" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="DECLARE @DateFrom datetime&#13;&#10;DECLARE @DateTo datetime&#13;&#10;&#13;&#10;SET @DateFrom = :DateFrom     &#13;&#10;SET @DateTo   = :DateTo  &#13;&#10;&#13;&#10;&#13;&#10;SELECT T1.*, T2.TSmz&#13;&#10;FROM&#13;&#10;(&#13;&#10;SELECT     &#13;&#10;	S_Subjects.Key_Sub, &#13;&#10;	S_Subjects.Name_Sub, &#13;&#10;	S_SubVer.Key_Sub_Ver, &#13;&#10;	S_SubVer.F_Name_Sub, &#13;&#10;  &#13;&#10; dbo.Pr_GetTabelSm1(S_SubVer.Key_Sub_Ver, @DateFrom , @DateTo )   AS SmTabel,&#13;&#10; &#13;&#10; SUM( Isnull(Pr_Pasp_Ac.AcKolvo*Pr_Pasp_Rows.TSm/Pr_Pasp.PaspKolvo,0)) SmTrud,&#13;&#10; SUM( Isnull(T002.AcKolvo*      Pr_Pasp_Rows.TSm/Pr_Pasp.PaspKolvo,0)) SmTrudDP&#13;&#10;     &#13;&#10;FROM&#13;&#10; S_Subjects  &#13;&#10; INNER JOIN S_SubVer&#13;&#10;		ON S_Subjects.Key_Sub = S_SubVer.Key_Sub   &#13;&#10;-- AND S_SubVer.Fl &#60;&#62; 1&#13;&#10;-- AND S_SubVer.Fl =0                                   &#13;&#10;	INNER JOIN Pr_Pasp_Ac &#13;&#10;		ON Pr_Pasp_Ac.Key_Sub_Ver = S_SubVer.Key_Sub_Ver&#13;&#10;      AND Pr_Pasp_Ac.AC_Date &#62;=  @DateFrom&#13;&#10;      AND Pr_Pasp_Ac.AC_Date &#60;=  @DateTo&#13;&#10;           &#13;&#10;	INNER JOIN Pr_Pasp_Rows &#13;&#10;		ON Pr_Pasp_Rows.Key_Pasp_Rows = Pr_Pasp_Ac.Key_Pasp_Rows&#13;&#10;&#13;&#10; INNER JOIN Pr_Pasp&#13;&#10;       ON  Pr_Pasp.Key_Pasport =   Pr_Pasp_Rows.Key_Pasport&#13;&#10;&#13;&#10; LEFT JOIN Pr_Pasp_Rows T001    &#13;&#10;  ON   T001.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows&#13;&#10;               &#13;&#10; AND  ((T001.Row_Src &#60;&#62; 1) OR (T001.Key_TPRows is null))&#13;&#10; AND T001.DateDopN IS NOT NULL                                                   &#13;&#10;   &#13;&#10;	LEFT JOIN Pr_Pasp_Ac  T002             &#13;&#10;		ON      T002.Key_Sub_Ver     = Pr_Pasp_Ac.Key_Sub_Ver&#13;&#10;      AND T002.AC_Date         = Pr_Pasp_Ac.AC_Date&#13;&#10;      AND T002.Key_Pasp_Rows   = Pr_Pasp_Ac.Key_Pasp_Rows                                         &#13;&#10;      AND T002.Key_Pasp_Rows   = T001.Key_Pasp_Rows                                        &#13;&#10;	   &#13;&#10;	  &#13;&#10;  &#13;&#10;                                &#13;&#10;GROUP BY&#13;&#10;	S_Subjects.Key_Sub, &#13;&#10;	S_Subjects.Name_Sub, &#13;&#10;	S_SubVer.Key_Sub_Ver, &#13;&#10;	S_SubVer.F_Name_Sub&#13;&#10;) T1&#13;&#10;&#13;&#10;&#13;&#10;LEFT JOIN &#13;&#10;(&#13;&#10;SELECT     &#13;&#10;     S_SubVer.Key_Sub_Ver, &#13;&#10;&#13;&#10;     SUM(Pr_Tabel_MK.Tzap*Pr_PaspDet.KolvoZap/Pr_Pasp.PaspKolvo) AS TSmz&#13;&#10;       &#13;&#10;FROM         &#13;&#10;       Pr_Tabel &#13;&#10;       INNER JOIN S_SubVer ON Pr_Tabel.Key_Sub_Ver = S_SubVer.Key_Sub_Ver &#13;&#10;       INNER JOIN S_Subjects ON S_SubVer.Key_Sub = S_Subjects.Key_Sub &#13;&#10;       INNER JOIN Pr_Tabel_MK ON Pr_Tabel.Key_Tabel = Pr_Tabel_MK.Key_Tabel &#13;&#10;       INNER JOIN Pr_Pasp_Rows ON Pr_Tabel_MK.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows &#13;&#10;       INNER JOIN Pr_Pasp ON Pr_Pasp_Rows.Key_Pasport = Pr_Pasp.Key_Pasport &#13;&#10;       INNER JOIN Pr_PaspDet ON Pr_Pasp.Key_Pasport = Pr_PaspDet.Key_Pasport &#13;&#10;       INNER JOIN P_VKZ ON Pr_PaspDet.Key_VKZ = P_VKZ.Key_VKZ &#13;&#10;       INNER JOIN P_VKZ AS P_VKZ_Izdel ON P_VKZ.Key_VKZ_Izdel = P_VKZ_Izdel.Key_VKZ &#13;&#10;       INNER JOIN T_VPredm ON P_VKZ_Izdel.Key_Predm = T_VPredm.Key_Predm&#13;&#10;WHERE     &#13;&#10;       (S_Subjects.Key_Class = 1)&#13;&#10;       AND Pr_Tabel.Date_Tabel &#62;= @DateFrom                                                &#13;&#10;       AND Pr_Tabel.Date_Tabel &#60;= @DateTo                                                  &#13;&#10;    &#13;&#10;GROUP BY &#13;&#10;       S_SubVer.Key_Sub_Ver&#13;&#10;&#13;&#10;&#13;&#10;) T2&#13;&#10;ON T2.Key_Sub_Ver = T1.Key_Sub_Ver&#13;&#10;&#13;&#10;                         &#13;&#10;ORDER BY 	&#13;&#10;	Name_Sub, &#13;&#10;	F_Name_Sub&#13;&#10;" PropData=""/>
