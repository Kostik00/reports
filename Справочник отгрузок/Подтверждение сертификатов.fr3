    <TfrxERQuery Name="SertQry" UserName="Сертификаты" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="MainFrxDS" IgnoreDupParams="False" SQL.Text="SELECT DISTINCT&#13;&#10;&#13;&#10;  S_SubVer.F_Name_Sub,&#13;&#10;&#13;&#10;  FMat.Name_Predm      Name_FMat,&#13;&#10;&#13;&#10;  FMat.OrderSortam,&#13;&#10;  FMat.Sortam          Sortam_FMat,&#13;&#10;  FMat.Type_NTD_Sortam Type_NTD_Sortam_Mat,&#13;&#10;  FMat.Name_NTD_Sortam Name_NTD_Sortam_Mat,&#13;&#10;&#13;&#10;  FMat.MarkaPrn        Marka_FMat,&#13;&#10;  FMat.MarkaPrn        MarkaPrn_FMat,&#13;&#10;  FMat.Type_NTD_Marka  Type_NTD_Marka_Mat,&#13;&#10;  FMat.Name_NTD_Marka  Name_NTD_Marka_Mat,&#13;&#10;&#13;&#10;  A_VId_Mat.Key_ID_Mat,&#13;&#10;  A_VId_Mat.VidSert,&#13;&#10;  A_VId_Mat.N_Sert,&#13;&#10;  A_VId_Mat.Date_Sert,&#13;&#10;  A_VId_Mat.Cod_Plavka,&#13;&#10;  A_VId_Mat.Party,&#13;&#10;  A_VId_Mat.Key_Sert,                          &#13;&#10;--  A_VId_Mat.Key_Pasport PaspMat,&#13;&#10;    &#13;&#10;  dbo.Pr_SertSv(A_VId_Mat.Key_Sert_Pos,'000' ,'Документ') Docum                     &#13;&#10;&#13;&#10;FROM&#13;&#10;  Pr_Otgr&#13;&#10;  INNER JOIN P_VKZ P_VKZ                       -- берем  всю ВКЗ на изделие&#13;&#10;    ON P_VKZ.Key_VKZ_Izdel=Pr_Otgr.Key_Vkz&#13;&#10;&#13;&#10;  INNER JOIN PR_PaspDet             -- смотрим для нее какие есть паспорта&#13;&#10;    ON PR_PaspDet.Key_VKZ=P_VKZ.Key_VKZ&#13;&#10;  INNER JOIN PR_Pasp                -- берем все паспорта&#13;&#10;    ON  PR_Pasp.Key_Pasport=PR_PaspDet.Key_Pasport&#13;&#10;&#13;&#10;  INNER JOIN Pr_SP                   -- Ну и смотрим только то, на что есть фактический материал&#13;&#10;    ON Pr_SP.Key_Pasport=PR_Pasp.Key_Pasport&#13;&#10;  INNER JOIN T_SP&#13;&#10;    ON T_SP.Key_SP=PR_Pasp.Key_SP&#13;&#10;&#13;&#10;  INNER JOIN A_VId_Mat                -- Это мы смотрим материал в идентефикаторе&#13;&#10;    ON A_VId_Mat.Key_ID_Mat=Pr_SP.Key_ID_Mat&#13;&#10;&#13;&#10;  INNER JOIN T_VPredm FMat            -- А это собственно говоря и есть сам материал&#13;&#10;    ON FMat.Key_Predm=A_VId_Mat.Key_Predm&#13;&#10;  INNER JOIN Gr_VxodPredm&#13;&#10;    ON Gr_VxodPredm.Key_Predm=FMat.Key_Predm&#13;&#10;    AND Gr_VxodPredm.Key_VidGr=1&#13;&#10;  INNER JOIN Gr_Groups&#13;&#10;    ON Gr_Groups.Cod_Gr=Gr_VxodPredm.Cod_Gr&#13;&#10;    AND Gr_VxodPredm.Key_VidGr= Gr_VxodPredm.Key_VidGr&#13;&#10;  LEFT JOIN S_SubVer&#13;&#10;    ON S_SubVer.Key_Sub_Ver=A_VId_Mat.Key_Sub_Ver_Postav&#13;&#10;&#13;&#10;WHERE&#13;&#10;  Pr_Otgr.NumOtgr=:NumOtgr&#13;&#10;  AND ((T_SP.InTB1=1)OR(:NoTB = 1))                    &#13;&#10;  AND FMat.Cod_Prin=50&#13;&#10;&#13;&#10;&#13;&#10;UNION&#13;&#10;SELECT     &#13;&#10;       S_SubVer.F_Name_Sub, &#13;&#10;       T_VPredm.Name_Predm, &#13;&#10;       T_VPredm.OrderSortam, &#13;&#10;       T_VPredm.Sortam, &#13;&#10;       T_VPredm.Type_NTD_Sortam, &#13;&#10;       T_VPredm.Name_NTD_Sortam,&#13;&#10;       T_VPredm.Marka, &#13;&#10;       T_VPredm.MarkaPrn, &#13;&#10;       T_VPredm.Type_NTD_Marka, &#13;&#10;       T_VPredm.Name_NTD_Marka, &#13;&#10;       A_VId_Mat.Key_ID_Mat,&#13;&#10;       A_VID_Mat.VidSert, &#13;&#10;       A_VID_Mat.N_Sert,&#13;&#10;       A_VID_Mat.Date_Sert, &#13;&#10;  A_VId_Mat.Cod_Plavka,&#13;&#10;  A_VId_Mat.Party,&#13;&#10;  A_VId_Mat.Key_Sert,                          &#13;&#10;       dbo.Pr_SertSv(A_VId_Mat.Key_Sert_Pos,'000' ,'Документ') Docum&#13;&#10;FROM         P_SvrM INNER JOIN&#13;&#10;                      P_SvrFM ON P_SvrM.Key_P_SvrM = P_SvrFM.Key_P_SvrM INNER JOIN&#13;&#10;                      P_SvrM_TT ON P_SvrM.Key_P_SvrM = P_SvrM_TT.Key_P_SvrM INNER JOIN&#13;&#10;                      A_VID_Mat ON P_SvrFM.Key_Id_Mat = A_VID_Mat.Key_ID_Mat INNER JOIN&#13;&#10;                      T_VPredm ON A_VID_Mat.Key_Predm = T_VPredm.Key_Predm INNER JOIN&#13;&#10;                      T_Sert ON A_VID_Mat.Key_Sert = T_Sert.Key_Sert INNER JOIN&#13;&#10;                      S_SubVer ON T_Sert.Key_Sub_Ver_Postav = S_SubVer.Key_Sub_Ver INNER JOIN&#13;&#10;                      S_Subjects ON S_SubVer.Key_Sub = S_Subjects.Key_Sub INNER JOIN&#13;&#10;                      Pr_Otgr ON P_SvrM.Key_VKZ_Izdel = Pr_Otgr.Key_VKZ&#13;&#10;WHERE     (P_SvrM_TT.Cod_TT = '000') AND (Pr_Otgr.NumOtgr =:NumOtgr)    &#13;&#10;                                                             &#13;&#10;ORDER BY   S_SubVer.F_Name_Sub, Name_FMat, FMat.OrderSortam" PropData=""/>
    <TfrxERQuery Name="SmSert" UserName="SmSert" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="MainFrxDS" IgnoreDupParams="False" SQL.Text="SELECT DISTINCT&#13;&#10;  A_VId_Mat.Key_ID_Mat,                                   &#13;&#10;  S_SubVer.F_Name_Sub,&#13;&#10;  A_VId_Mat.VidSert,&#13;&#10;  A_VId_Mat.N_Sert,&#13;&#10;  A_VId_Mat.Date_Sert,&#13;&#10;    &#13;&#10;  dbo.Pr_SertSv(A_VId_Mat.Key_Sert_Pos,'000' ,'Документ') Docum                     &#13;&#10;&#13;&#10;FROM&#13;&#10;  Pr_Otgr&#13;&#10;  INNER JOIN P_VKZ P_VKZ                       -- берем  всю ВКЗ на изделие&#13;&#10;    ON P_VKZ.Key_VKZ_Izdel=Pr_Otgr.Key_Vkz&#13;&#10;&#13;&#10;  INNER JOIN PR_PaspDet             -- смотрим для нее какие есть паспорта&#13;&#10;    ON PR_PaspDet.Key_VKZ=P_VKZ.Key_VKZ&#13;&#10;  INNER JOIN PR_Pasp                -- берем все паспорта&#13;&#10;    ON  PR_Pasp.Key_Pasport=PR_PaspDet.Key_Pasport&#13;&#10;&#13;&#10;  INNER JOIN Pr_SP                   -- Ну и смотрим только то, на что есть фактический материал&#13;&#10;    ON Pr_SP.Key_Pasport=PR_Pasp.Key_Pasport&#13;&#10;  INNER JOIN T_SP&#13;&#10;    ON T_SP.Key_SP=PR_Pasp.Key_SP&#13;&#10;&#13;&#10;  INNER JOIN A_VId_Mat                -- Это мы смотрим материал в идентефикаторе&#13;&#10;    ON A_VId_Mat.Key_ID_Mat=Pr_SP.Key_ID_Mat&#13;&#10;&#13;&#10;  INNER JOIN T_VPredm FMat            -- А это собственно говоря и есть сам материал&#13;&#10;    ON FMat.Key_Predm=A_VId_Mat.Key_Predm&#13;&#10;  INNER JOIN Gr_VxodPredm&#13;&#10;    ON Gr_VxodPredm.Key_Predm=FMat.Key_Predm&#13;&#10;    AND Gr_VxodPredm.Key_VidGr=1&#13;&#10;  INNER JOIN Gr_Groups&#13;&#10;    ON Gr_Groups.Cod_Gr=Gr_VxodPredm.Cod_Gr&#13;&#10;    AND Gr_VxodPredm.Key_VidGr= Gr_VxodPredm.Key_VidGr&#13;&#10;  LEFT JOIN S_SubVer&#13;&#10;    ON S_SubVer.Key_Sub_Ver=A_VId_Mat.Key_Sub_Ver_Postav&#13;&#10;&#13;&#10;WHERE&#13;&#10;  Pr_Otgr.NumOtgr=:NumOtgr&#13;&#10;  AND ((T_SP.InTB1=1)OR(:NoTB = 1))                    &#13;&#10;  AND FMat.Cod_Prin=50&#13;&#10;  AND dbo.Pr_SertSv(A_VId_Mat.Key_Sert_Pos,'000' ,'Документ') not in ('-')        &#13;&#10;&#13;&#10;UNION&#13;&#10;SELECT     &#13;&#10;       A_VId_Mat.Key_ID_Mat,&#13;&#10;       S_SubVer.F_Name_Sub,&#13;&#10;       A_VID_Mat.VidSert, &#13;&#10;       A_VID_Mat.N_Sert,&#13;&#10;       A_VID_Mat.Date_Sert, &#13;&#10;       dbo.Pr_SertSv(A_VId_Mat.Key_Sert_Pos,'000' ,'Документ') Docum&#13;&#10;FROM         P_SvrM INNER JOIN&#13;&#10;                      P_SvrFM ON P_SvrM.Key_P_SvrM = P_SvrFM.Key_P_SvrM INNER JOIN&#13;&#10;                      P_SvrM_TT ON P_SvrM.Key_P_SvrM = P_SvrM_TT.Key_P_SvrM INNER JOIN&#13;&#10;                      A_VID_Mat ON P_SvrFM.Key_Id_Mat = A_VID_Mat.Key_ID_Mat INNER JOIN&#13;&#10;                      T_VPredm ON A_VID_Mat.Key_Predm = T_VPredm.Key_Predm INNER JOIN&#13;&#10;                      T_Sert ON A_VID_Mat.Key_Sert = T_Sert.Key_Sert INNER JOIN&#13;&#10;                      S_SubVer ON T_Sert.Key_Sub_Ver_Postav = S_SubVer.Key_Sub_Ver INNER JOIN&#13;&#10;                      S_Subjects ON S_SubVer.Key_Sub = S_Subjects.Key_Sub INNER JOIN&#13;&#10;                      Pr_Otgr ON P_SvrM.Key_VKZ_Izdel = Pr_Otgr.Key_VKZ&#13;&#10;WHERE     (P_SvrM_TT.Cod_TT = '000') AND (Pr_Otgr.NumOtgr =:NumOtgr)          &#13;&#10;  AND dbo.Pr_SertSv(A_VId_Mat.Key_Sert_Pos,'000' ,'Документ') not in ('-')        &#13;&#10;    &#13;&#10;&#13;&#10;    &#13;&#10;ORDER BY   &#13;&#10;  A_VId_Mat.VidSert,&#13;&#10;  A_VId_Mat.N_Sert,&#13;&#10;  A_VId_Mat.Date_Sert" PropData=""/>
