    <TfrxERQuery Name="qNaclStrSost" UserName="dsNaclStrSost" CloseDataSource="False" FieldAliases.Text="NSS_Key_Nacl_Str_Sost=NSS_Key_Nacl_Str_Sost&#13;&#10;NSS_Key_Nacl_Str=NSS_Key_Nacl_Str&#13;&#10;NSS_Kolvo_InBaseEd=NSS_Kolvo_InBaseEd&#13;&#10;NSS_Mass=NSS_Mass" BCDToCurrency="False" Master="qNaclStr" IgnoreDupParams="False" SQL.Text="DECLARE @Key_Nacl_Str Int;&#13;&#10;SET     @Key_Nacl_Str = :Key_Nacl_Str;&#13;&#10;&#13;&#10;DECLARE @Koef Float;&#13;&#10;SET     @Koef = :Koef;    &#13;&#10;  &#13;&#10;SELECT&#13;&#10;    [NSS].[Key_Nacl_Str_Sost]         AS [NSS_Key_Nacl_Str_Sost],&#13;&#10;    [NSS].[Key_Nacl_Str]              AS [NSS_Key_Nacl_Str],&#13;&#10;    [NSS].[Kolvo_InBaseEd]            AS [NSS_Kolvo_InBaseEd],&#13;&#10;    CAST([NSS].[Kolvo_InBaseEd]*@Koef AS FLOAT)&#13;&#10;                                      AS [NSS_Mass]      &#13;&#10;  FROM&#13;&#10;    [A_Nacl_Str_Sost] AS [NSS]  &#13;&#10;  WHERE&#13;&#10;    [NSS].[Key_Nacl_Str] = @Key_Nacl_Str&#13;&#10;  ORDER BY&#13;&#10;    [NSS].[Key_Nacl_Str_Sost] ASC;    " PropData=""/>
    <TfrxERQuery Name="qNaclStr" UserName="dsNaclStr" CloseDataSource="False" FieldAliases.Text="NS_Key_Nacl=NS_Key_Nacl&#13;&#10;NS_Key_Nacl_Str=NS_Key_Nacl_Str&#13;&#10;NS_KolvoInBaseEd=NS_KolvoInBaseEd&#13;&#10;Koef=Koef&#13;&#10;NS_Mass=NS_Mass&#13;&#10;NSS_COUNT=NSS_COUNT&#13;&#10;NSS_Kolvo_InBaseEd=NSS_Kolvo_InBaseEd&#13;&#10;NSS_Mass=NSS_Mass" BCDToCurrency="False" Master="MainFrxDS" IgnoreDupParams="False" SQL.Text="DECLARE @Key_Nacl Int;&#13;&#10;SET     @Key_Nacl = :Key_Nacl;&#13;&#10;&#13;&#10;DECLARE @NSS_COUNT Int;&#13;&#10;SET     @NSS_COUNT = :NSS_COUNT;&#13;&#10;&#13;&#10;DECLARE @DeltaMass Float;&#13;&#10;SET     @DeltaMass = :DeltaMass;    &#13;&#10;  &#13;&#10;SELECT&#13;&#10;    [NS].[Key_Nacl]                   AS [NS_Key_Nacl],&#13;&#10;    [NS].[Key_Nacl_Str]               AS [NS_Key_Nacl_Str],                         &#13;&#10;    MAX([NS].[KolvoInBaseEd])         AS [NS_KolvoInBaseEd],&#13;&#10;    [MU].[Koef]                       AS [Koef],                      &#13;&#10;    CAST(MAX([NS].[KolvoInBaseEd]*[MU].[Koef]) AS FLOAT)&#13;&#10;                                      AS [NS_Mass],&#13;&#10;    COUNT([NSS].[Key_Nacl_Str_Sost])  AS [NSS_COUNT],                                          &#13;&#10;    SUM([NSS].[Kolvo_InBaseEd])       AS [NSS_Kolvo_InBaseEd],&#13;&#10;    CAST(SUM([NSS].[Kolvo_InBaseEd]*[MU].[Koef]) AS FLOAT)&#13;&#10;                                      AS [NSS_Mass]                                                           &#13;&#10;  FROM&#13;&#10;    [A_Nacl_Str] AS [NS]  &#13;&#10;      INNER JOIN [T_EdIzm] AS [MU]&#13;&#10;              ON [MU].[Cod_Ed_Izm] = [NS].[Cod_Ed_Izm]  &#13;&#10;      INNER JOIN [A_Nacl_Str_Sost] AS [NSS]&#13;&#10;              ON [NSS].[Key_Nacl_Str] = [NS].[Key_Nacl_Str]                  &#13;&#10;  WHERE&#13;&#10;    [NS].[Key_Nacl] = @Key_Nacl&#13;&#10;  GROUP BY&#13;&#10;    [NS].[Key_Nacl],&#13;&#10;    [NS].[Key_Nacl_Str],&#13;&#10;    [MU].[Koef]        &#13;&#10;  HAVING&#13;&#10;    COUNT([NSS].[Key_Nacl_Str_Sost]) &#62;= @NSS_COUNT&#13;&#10;    AND ABS(MAX([NS].[KolvoInBaseEd]*[MU].[Koef]) - SUM([NSS].[Kolvo_InBaseEd]*[MU].[Koef])) &#62;= @DeltaMass                                        &#13;&#10;  ORDER BY&#13;&#10;    [NS].[Key_Nacl_Str] ASC;  " PropData=""/>
