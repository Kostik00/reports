  <TfrxDataPage Name="Data" HGuides.Text="" VGuides.Text="" Height="1000" Left="0" Top="0" Width="1000">
    <TfrxERQuery Name="qDSE" UserName="dsDSE" CloseDataSource="False" FieldAliases.Text="FM_Key_GrZap=FM_Key_GrZap&#13;&#10;FM_Key_Id_Mat=FM_Key_Id_Mat&#13;&#10;FM_VidSert=FM_VidSert&#13;&#10;FM_N_Sert=FM_N_Sert&#13;&#10;FM_Date_Sert=FM_Date_Sert&#13;&#10;FM_Cod_Plavka=FM_Cod_Plavka&#13;&#10;FM_Party=FM_Party&#13;&#10;FM_RegN_Sert=FM_RegN_Sert&#13;&#10;FM_PrintFlag=FM_PrintFlag&#13;&#10;FM_Cod_Predm=FM_Cod_Predm&#13;&#10;FM_Name_Predm=FM_Name_Predm&#13;&#10;FM_Sortam=FM_Sortam&#13;&#10;FM_OrderSortam=FM_OrderSortam&#13;&#10;FM_Marka=FM_Marka&#13;&#10;FM_Type_NTD_Sortam=FM_Type_NTD_Sortam&#13;&#10;FM_Name_NTD_Sortam=FM_Name_NTD_Sortam&#13;&#10;FM_Type_NTD_Marka=FM_Type_NTD_Marka&#13;&#10;FM_Name_NTD_Marka=FM_Name_NTD_Marka&#13;&#10;FM_TOInfo=FM_TOInfo&#13;&#10;FM_Key_VKZ_Zap=FM_Key_VKZ_Zap&#13;&#10;FM_Cod_Predm_DSE=FM_Cod_Predm_DSE&#13;&#10;DataZapUtv=DataZapUtv&#13;&#10;Cod_SosPos=Cod_SosPos&#13;&#10;Cod_Predm_Izdel=Cod_Predm_Izdel&#13;&#10;Name_Predm_Izdel=Name_Predm_Izdel&#13;&#10;Cod_SosIzd=Cod_SosIzd&#13;&#10;Cod_Zak=Cod_Zak&#13;&#10;Key_Predm=Key_Predm&#13;&#10;Cod_Predm=Cod_Predm&#13;&#10;Name_Predm=Name_Predm&#13;&#10;Sortam=Sortam&#13;&#10;PrintFlag_Mat=PrintFlag_Mat&#13;&#10;Cod_Predm_Mat=Cod_Predm_Mat&#13;&#10;Name_Predm_Mat=Name_Predm_Mat&#13;&#10;Sortam_Mat=Sortam_Mat&#13;&#10;Type_NTD_Sortam_Mat=Type_NTD_Sortam_Mat&#13;&#10;Name_NTD_Sortam_Mat=Name_NTD_Sortam_Mat&#13;&#10;Marka_Mat=Marka_Mat&#13;&#10;Type_NTD_Marka_Mat=Type_NTD_Marka_Mat&#13;&#10;Name_NTD_Marka_Mat=Name_NTD_Marka_Mat&#13;&#10;KolvoZapPln=KolvoZapPln&#13;&#10;KolDT=KolDT&#13;&#10;Cod_DocumZ=Cod_DocumZ&#13;&#10;Date_DocumZ=Date_DocumZ&#13;&#10;Prof_Razm=Prof_Razm&#13;&#10;EdNorm=EdNorm&#13;&#10;KolZag=KolZag&#13;&#10;Ad_Info=Ad_Info&#13;&#10;Name_K_Oper=Name_K_Oper&#13;&#10;Cod_TP=Cod_TP&#13;&#10;Name_K_T_Status=Name_K_T_Status" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="DECLARE @KeyGrZap Integer;&#13;&#10;SET     @KeyGrZap = :Key_GrZap;  &#13;&#10;&#13;&#10;WITH FM(&#13;&#10;    Key_GrZap,&#13;&#10;    Key_Id_Mat, &#13;&#10;    VidSert, &#13;&#10;    N_Sert, &#13;&#10;    Date_Sert,&#13;&#10;    Cod_Plavka,&#13;&#10;    Party,&#13;&#10;    RegN_Sert,&#13;&#10;    PrintFlag,                           &#13;&#10;    Cod_Predm, &#13;&#10;    Name_Predm, &#13;&#10;    Sortam, &#13;&#10;    OrderSortam, &#13;&#10;    Marka, &#13;&#10;    Type_NTD_Sortam, &#13;&#10;    Name_NTD_Sortam, &#13;&#10;    Type_NTD_Marka, &#13;&#10;    Name_NTD_Marka,  &#13;&#10;    TOInfo,&#13;&#10;    Key_VKZ_Zap,&#13;&#10;    Cod_Predm_DSE) AS&#13;&#10;  (SELECT DISTINCT&#13;&#10;      P_VKZ.Key_GrZap,&#13;&#10;      A_VID_Mat.Key_Id_Mat, &#13;&#10;      A_VID_Mat.VidSert, &#13;&#10;      A_VID_Mat.N_Sert, &#13;&#10;      A_VID_Mat.Date_Sert,&#13;&#10;      A_VID_Mat.Cod_Plavka,&#13;&#10;      A_VID_Mat.Party,&#13;&#10;      A_VID_Mat.RegN_Sert,&#13;&#10;      T_VPredm.PrintFlag,                           &#13;&#10;      T_VPredm.Cod_Predm, &#13;&#10;      T_VPredm.Name_Predm, &#13;&#10;      T_VPredm.Sortam, &#13;&#10;      T_VPredm.OrderSortam, &#13;&#10;      T_VPredm.Marka, &#13;&#10;      T_VPredm.Type_NTD_Sortam, &#13;&#10;      T_VPredm.Name_NTD_Sortam, &#13;&#10;      T_VPredm.Type_NTD_Marka, &#13;&#10;      T_VPredm.Name_NTD_Marka,  &#13;&#10;      dbo.A_IdMat_TOInfo(A_VID_Mat.Key_Id_Mat) AS TOInfo,&#13;&#10;      P_VKZ_Zap.Key_VKZ_Zap,&#13;&#10;      DSE.Cod_Predm AS Cod_Predm_DSE   &#13;&#10;    FROM            &#13;&#10;      P_VKZ&#13;&#10;        INNER JOIN P_VKZ_Zap_Det   ON P_VKZ_Zap_Det.Key_VKZ = P_VKZ.Key_VKZ&#13;&#10;        INNER JOIN P_VKZ_Zap       ON P_VKZ_Zap.Key_VKZ_Zap = P_VKZ_Zap_Det.Key_VKZ_Zap                                    &#13;&#10;        INNER JOIN P_VMZ 	 	       ON P_VMZ.Key_VKZ_Zap = P_VKZ_Zap.Key_VKZ_Zap&#13;&#10;        INNER JOIN T_SPRows		      ON P_VMZ.Key_SPRow = T_SPRows.Key_SPRow &#13;&#10;        INNER JOIN A_Rezerv		      ON A_Rezerv.Key_VMZ  = P_VMZ.Key_VMZ&#13;&#10;        INNER JOIN A_VID_Mat		     ON A_Rezerv.Key_Id_Mat = A_VID_Mat.Key_ID_Mat &#13;&#10;        INNER JOIN T_VPredm		      ON A_VID_Mat.Key_Predm = T_VPredm.Key_Predm  &#13;&#10;        INNER JOIN T_VPredm AS DSE	ON DSE.Key_Predm = P_VKZ_Zap.Key_Predm  &#13;&#10;    WHERE        &#13;&#10;      (P_VKZ.Key_GrZap = @KeyGrZap)    AND&#13;&#10;      (T_SPRows.SP_position = 0)       AND&#13;&#10;      ((P_VKZ_Zap.Key_Pasport IS NULL) OR (1 = 1)))&#13;&#10;&#13;&#10;SELECT&#13;&#10;    FM.Key_GrZap                 AS FM_Key_GrZap,&#13;&#10;    FM.Key_Id_Mat                AS FM_Key_Id_Mat, &#13;&#10;    FM.VidSert                   AS FM_VidSert, &#13;&#10;    FM.N_Sert                    AS FM_N_Sert, &#13;&#10;    FM.Date_Sert                 AS FM_Date_Sert,&#13;&#10;    FM.Cod_Plavka                AS FM_Cod_Plavka,&#13;&#10;    FM.Party                     AS FM_Party,&#13;&#10;    FM.RegN_Sert                 AS FM_RegN_Sert,&#13;&#10;    FM.PrintFlag                 AS FM_PrintFlag,                           &#13;&#10;    FM.Cod_Predm                 AS FM_Cod_Predm, &#13;&#10;    FM.Name_Predm                AS FM_Name_Predm, &#13;&#10;    FM.Sortam                    AS FM_Sortam, &#13;&#10;    FM.OrderSortam               AS FM_OrderSortam, &#13;&#10;    FM.Marka                     AS FM_Marka, &#13;&#10;    FM.Type_NTD_Sortam           AS FM_Type_NTD_Sortam, &#13;&#10;    FM.Name_NTD_Sortam           AS FM_Name_NTD_Sortam, &#13;&#10;    FM.Type_NTD_Marka            AS FM_Type_NTD_Marka, &#13;&#10;    FM.Name_NTD_Marka            AS FM_Name_NTD_Marka,  &#13;&#10;    FM.TOInfo                    AS FM_TOInfo,&#13;&#10;    FM.Key_VKZ_Zap               AS FM_Key_VKZ_Zap,&#13;&#10;    FM.Cod_Predm_DSE             AS FM_Cod_Predm_DSE,  &#13;&#10;    TZapUtv.DataZapUtv           AS DataZapUtv,&#13;&#10;    TZapUtv.Cod_SosPos           AS Cod_SosPos,      &#13;&#10;    T_VPredm_Izdel.Cod_Predm     AS Cod_Predm_Izdel, &#13;&#10;    T_VPredm_Izdel.Name_Predm    AS Name_Predm_Izdel,&#13;&#10;    P_VKZ_Izdel.Cod_So