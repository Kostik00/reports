<TfrxReport Version="6.8.6" DotMatrixReport="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="44260,5634666435" ReportOptions.Description.Text="" ReportOptions.Name="Контроль количества (по списку, ver. 14).fr3 Список накладных" ReportOptions.LastChange="44264,9616884144" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  iCount: Integer = 1;&#13;&#10;  fDeltaMass: Extended = 1.0;&#13;&#10;  Style: TfrxStyleItem;&#13;&#10;  Styles: TfrxStyles;&#13;&#10;//  StyleSheet: TfrxStyleSheet;&#13;&#10;&#13;&#10;procedure sbCountUpOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Inc(iCount);&#13;&#10;  meCount.Text:=IntToStr(iCount);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure sbCountDownOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Dec(iCount);&#13;&#10;  meCount.Text:=IntToStr(iCount);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  meCount.Text:=IntToStr(iCount);&#13;&#10;  meDeltaMass.Text:=FormatFloat('##0.00', fDeltaMass);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnDeactivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if DialogPage1.ModalResult = mrOk&#13;&#10;    then begin&#13;&#10;      qNaclStr.ParamByName('NSS_Count').Value:=iCount;&#13;&#10;      qNaclStr.ParamByName('DeltaMass').Value:=fDeltaMass;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure meCountOnChange(Sender: TfrxComponent);&#13;&#10;var i: Integer;&#13;&#10;begin&#13;&#10;  try&#13;&#10;    i:=StrToInt(meCount.Text);&#13;&#10;    if i &#60;&#62; iCount then iCount:=i;&#13;&#10;  except&#13;&#10;    ShowMessage('ExceptionMessage (meCountOnChange): '+#10+ExceptionMessage);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure sbDeltaMassUpOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  fDeltaMass:=fDeltaMass+1.0;&#13;&#10;  if fDeltaMass &#60; 0 then fDeltaMass:=0.0;&#13;&#10;  meDeltaMass.Text:=FormatFloat('##0.00', fDeltaMass)&#13;&#10;end;&#13;&#10;&#13;&#10;procedure sbDeltaMassDownOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  fDeltaMass:=fDeltaMass-1.0;&#13;&#10;  if fDeltaMass &#60; 0 then fDeltaMass:=0.0;&#13;&#10;  meDeltaMass.Text:=FormatFloat('##0.00', fDeltaMass)&#13;&#10;end;&#13;&#10;&#13;&#10;procedure meDeltaMassOnChange(Sender: TfrxComponent);&#13;&#10;var f: Extended;&#13;&#10;begin&#13;&#10;  try&#13;&#10;    f:=StrToFloat(meCount.Text);&#13;&#10;    if f &#60;&#62; fDeltaMass then fDeltaMass:=f;&#13;&#10;  except&#13;&#10;    ShowMessage('ExceptionMessage (meDeltaMassOnChange): '+#10+ExceptionMessage);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  cbxDesign.Items.Add('Обычный документ (тонкие линии)');&#13;&#10;  cbxDesign.Items.Add('Обычный документ (широкие поля)');&#13;&#10;  cbxDesign.Items.Add('Контрастный цвет (разметка)');&#13;&#10;  cbxDesign.ItemIndex:=0;&#13;&#10;end." StoreInDFM="False">
