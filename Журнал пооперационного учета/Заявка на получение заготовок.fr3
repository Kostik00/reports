    <TfrxERQuery Name="SMZData" UserName="SMZData" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="TabelFrxDS" IgnoreDupParams="False" SQL.Text="SELECT DISTINCT                                        &#13;&#10;       Pr_Tabel.Key_Tabel, &#13;&#10;       Pr_Tabel.Date_Tabel, &#13;&#10;       Pr_Pasp.Key_Pasport, &#13;&#10;       Pr_Pasp.Cod_Pasp, &#13;&#10;       Pr_Pasp.PaspKolvo,&#13;&#10;       Pr_Pasp.Cod_Zak,                                           &#13;&#10;       DT.Cod_Predm AS Cod_Predm,&#13;&#10;       DT.Name_Predm AS Name_Predm, &#13;&#10;       Pr_Tabel_MK.KolvoZap,&#13;&#10;         &#13;&#10;       A_VID_Mat.Key_ID_Mat, &#13;&#10;       A_VID_Mat.Key_Sert,&#13;&#10;       A_VID_Mat.Key_Sert_Pos,                                       &#13;&#10;       A_VID_Mat.VidSert, &#13;&#10;       A_VID_Mat.N_Sert,&#13;&#10;       A_VID_Mat.Date_Sert, &#13;&#10;       A_VID_Mat.N_SertPos, &#13;&#10;       A_VID_Mat.Cod_Plavka, &#13;&#10;       A_VID_Mat.Party,&#13;&#10;       [dbo].[A_IdMatTTDetail]( A_VID_Mat.Key_ID_Mat, '411')  TTSadka,&#13;&#10;       A_VID_Mat.Cod_ID_Mat,             &#13;&#10;           &#13;&#10; 			      T_Predm_M.PrintFlag		     AS PrintFlagM,                                           &#13;&#10;&#13;&#10;       	 	T_Predm_M.Key_Predm       AS Key_PredmM,&#13;&#10;        		T_Predm_M.Cod_Predm       AS Cod_PredmM, &#13;&#10;        		T_Predm_M.Name_Predm      AS Name_PredmM,&#13;&#10;                                           &#13;&#10;        		T_Predm_M.Sortam          AS SortamM, &#13;&#10;        		T_Predm_M.OrderSortam     AS OrderSortamM,&#13;&#10;        		T_Predm_M.SortamFn        AS SortamFnM,&#13;&#10;        		T_Predm_M.Type_NTD_Sortam AS Type_NTD_SortamM, &#13;&#10;        		T_Predm_M.Name_NTD_Sortam AS Name_NTD_SortamM, &#13;&#10;        		T_Predm_M.Marka           AS MarkaM, &#13;&#10;        		T_Predm_M.Type_NTD_Marka  AS Type_NTD_MarkaM, &#13;&#10;        		T_Predm_M.Name_NTD_Marka  AS Name_NTD_MarkaM,&#13;&#10;          T_Predm_M.Name_K_Ed Name_K_Ed,&#13;&#10;          dbo.T_GetKoefTo(T_Predm_M.Key_Predm, '166', T_Predm_M.Cod_Ed_Izm) K,                                                                                                      &#13;&#10;         &#13;&#10;       T_Zagotov.Name_Zagotov, &#13;&#10;       Pr_SPDetail.ProfRazm, &#13;&#10;       Pr_SPDetail.M_Zag, &#13;&#10;       Pr_SPDetail.Kolvo,&#13;&#10;         &#13;&#10;       Gr_VxodPredm.Cod_Gr,&#13;&#10;       Gr_Groups.FullNameGr,&#13;&#10;       Pr_Pasp_M.Cod_Pasp   Cod_PaspM                                    &#13;&#10;           &#13;&#10;FROM   &#13;&#10;       Pr_Tabel &#13;&#10;       INNER JOIN Pr_Tabel_MK &#13;&#10;               ON Pr_Tabel.Key_Tabel = Pr_Tabel_MK.Key_Tabel &#13;&#10;       INNER JOIN Pr_Pasp_Rows &#13;&#10;               ON Pr_Tabel_MK.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows&#13;&#10;    --           AND ((Pr_Pasp_Rows.Cod_Oper in ('2151','4280','4281','4282','4284','4285','4286','4287','9700','9711','9730','9733','9751')) or ( :PrintAll = 1))                                                                                                     &#13;&#10;       INNER JOIN Pr_Pasp &#13;&#10;               ON Pr_Pasp_Rows.Key_Pasport = Pr_Pasp.Key_Pasport &#13;&#10;       INNER JOIN T_VPredm AS DT &#13;&#10;               ON Pr_Pasp.Key_Predm = DT.Key_Predm &#13;&#10;       INNER JOIN Pr_SP &#13;&#10;               ON Pr_Pasp.Key_Pasport = Pr_SP.Key_Pasport &#13;&#10;       INNER JOIN Pr_SPDetail &#13;&#10;               ON Pr_SP.Key_PrSP = Pr_SPDetail.Key_PrSp &#13;&#10;       INNER JOIN A_VID_Mat &#13;&#10;               ON Pr_SP.Key_ID_Mat = A_VID_Mat.Key_ID_Mat&#13;&#10;       LEFT JOIN Pr_Pasp Pr_Pasp_M&#13;&#10;               ON Pr_Pasp_M.Key_Pasport = A_VID_Mat.Key_Pasport                                                                             &#13;&#10;&#13;&#10;                 &#13;&#10;       INNER JOIN T_VPredm  T_Predm_M   &#13;&#10;               ON A_VID_Mat.Key_Predm = T_Predm_M.Key_Predm &#13;&#10;       LEFT JOIN T_Zagotov &#13;&#10;               ON Pr_SPDetail.Cod_Zagotov = T_Zagotov.Cod_Zagotov&#13;&#10;&#13;&#10;		LEFT JOIN Gr_VxodPredm &#13;&#10;			ON T_Predm_M.Key_Predm = Gr_VxodPredm.Key_Predm &#13;&#10;		AND (Gr_VxodPredm.Key_VidGr = 1)    &#13;&#10;		LEFT JOIN Gr_Groups &#13;&#10;			ON Gr_VxodPredm.Key_VidGr = Gr_Groups.Key_VidGr &#13;&#10;			AND Gr_VxodPredm.Cod_Gr = Gr_Groups.Cod_Gr &#13;&#10;  &#13;&#10;                 &#13;&#10;WHERE     (Pr_Tabel.Key_Tabel = :Key_Tabel)&#13;&#10;ORDER BY &#13;&#10;       Gr_VxodPredm.Cod_Gr, &#13;&#10;       T_Predm_M.Name_Predm, &#13;&#10;&#13;&#10;        		T_Predm_M.Marka, &#13;&#10;        		T_Predm_M.Type_NTD_Marka, &#13;&#10;        		T_Predm_M.Name_NTD_Marka,&#13;&#10;          &#13;&#10; &#13;&#10;        		T_Predm_M.OrderSortam,&#13;&#10;        		T_Predm_M.Type_NTD_Sortam,&#13;&#10;        		T_Predm_M.Name_NTD_Sortam,  &#13;&#10;          T_Predm_M.Key_Predm,&#13;&#10;            &#13;&#10;       A_VID_Mat.VidSert, &#13;&#10;       A_VID_Mat.N_Sert,&#13;&#10;       A_VID_Mat.Date_Sert, &#13;&#10;       A_VID_Mat.N_SertPos &#13;&#10;        &#13;&#10;                &#13;&#10;             " PropData="05704C65667402580470546F7002240A506172616D657465727301010C43000000204E616D653D225072696E74416C6C222044617461547970653D226674496E7465676572222045787072657373696F6E3D225072696E74416C6C2E436865636B65642200010C26000000204E616D653D224B65795F546162656C222044617461547970653D226674556E6B6E6F776E220000"/>
