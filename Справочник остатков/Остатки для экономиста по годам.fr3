<TfrxReport Version="6.9.6" DotMatrixReport="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="Ïî óìîë÷àíèþ" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38932,0200043403" ReportOptions.Description.Text="" ReportOptions.Name="Îñòàòêè äëÿ ýêîíîìèñòà ïî ãîäàì (ver. 10).fr3 Ñïðàâî÷íèê îñòàòêîâ" ReportOptions.LastChange="44336,6100117361" ScriptLanguage="PascalScript" ScriptText.Text="uses 'COmmonRu.pas';&#13;&#10;&#13;&#10;var&#13;&#10;  IniFile: TIniFile;&#13;&#10;&#13;&#10;function FnPredm:string;&#13;&#10;begin&#13;&#10;  Result:=PrintPredm(&#60;Îñòàòêè.&#34;PrintFlag&#34;&#62;,&#60;Îñòàòêè.&#34;Cod_Predm&#34;&#62;, &#60;Îñòàòêè.&#34;Name_Predm&#34;&#62;,&#13;&#10;                       &#60;Îñòàòêè.&#34;Sortam&#34;&#62;, &#60;Îñòàòêè.&#34;Type_NTD_Sortam&#34;&#62;, &#60;Îñòàòêè.&#34;Name_NTD_Sortam&#34;&#62;,&#13;&#10;                       &#60;Îñòàòêè.&#34;Marka&#34;&#62;,  &#60;Îñòàòêè.&#34;Type_NTD_Marka&#34;&#62;,  &#60;Îñòàòêè.&#34;Name_NTD_Marka&#34;&#62;);&#13;&#10;&#13;&#10;//  Result:=Result+'('+VarToStr(&#60;ÂÑÈ.&#34;PrintFlag&#34;&#62;)+')';&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo6OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  PriceMemo.Color:=clNone;&#13;&#10;  if &#60;Îñòàòêè.&#34;PriceAvg&#34;&#62; &#60;=0   then  PriceMemo.Color:=$006060FD;&#13;&#10;end;&#13;&#10;&#13;&#10;function MyYY :string;&#13;&#10;begin&#13;&#10;  Result:= IntToStr(&#60;Îñòàòêè.&#34;YY&#34;&#62;);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MainDialogPageOnActivate(Sender: TfrxComponent);&#13;&#10;var i: Integer;&#13;&#10;begin&#13;&#10;  edCodZakFilter.Text:=Trim(TIniFile(IniFile).ReadString('Main', 'TextFilter', ''));&#13;&#10;  edCodZakFilter.Text:=TIniFile(IniFile).ReadString('Main', 'TextFilter', '');&#13;&#10;  rbLike.Checked:=       TIniFile(IniFile).ReadBool('Main', 'rbLike', True);&#13;&#10;  rbRecord.Checked:= not TIniFile(IniFile).ReadBool('Main', 'rbLike', True);&#13;&#10;  qZakaz.Close;&#13;&#10;  qZakaz.Open;&#13;&#10;  for i:=2016 to YearOf(Date)&#13;&#10;    do begin&#13;&#10;      cbBeginDate.Items.Add(IntToStr(i));&#13;&#10;      cbEndDate.Items.Add(IntToStr(i));&#13;&#10;    end;&#13;&#10;  cbBeginDate.ItemIndex:=TIniFile(IniFile).ReadInteger('Main', 'cbBeginDate', 0);&#13;&#10;  cbEndDate.ItemIndex:=TIniFile(IniFile).ReadInteger('Main', 'cbEndDate', cbEndDate.Items.Count-1);&#13;&#10;  lbxCodZak.Enabled:=rbLike.Checked;&#13;&#10;  dblCodZak.Enabled:= not rbLike.Checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MainDialogPageOnDeactivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if MainDialogPage.ModalResult = mrOk&#13;&#10;    then begin&#13;&#10;      TIniFile(IniFile).WriteString('Main', 'TextFilter', Trim(edCodZakFilter.Text));&#13;&#10;      TIniFile(IniFile).WriteBool('Main', 'rbLike', rbLike.Checked);&#13;&#10;      TIniFile(IniFile).WriteInteger('Main', 'cbBeginDate', cbBeginDate.ItemIndex);&#13;&#10;      TIniFile(IniFile).WriteInteger('Main', 'cbEndDate', cbEndDate.ItemIndex);&#13;&#10;      if lbxCodZak.Enabled&#13;&#10;        then begin OstatokQRY.SQL.Strings[85]:='AND (A_LZK.Cod_Zak = @CodZak)';    OstatokQRY.ParamByName('CodZak').Value:= qZakaz.FieldByName('Cod_Zak').AsString end&#13;&#10;        else begin OstatokQRY.SQL.Strings[85]:='AND (A_LZK.Cod_Zak Like @CodZak)'; OstatokQRY.ParamByName('CodZak').Value:= Trim(edCodZakFilter.Text+'%') end;&#13;&#10;    end;&#13;&#10;{  OstatokQRY.ParamByName('BeginYear').Value:=StrToInt(Trim(cbBeginDate.Text));&#13;&#10;  OstatokQRY.ParamByName('EndYear').Value:=StrToInt(Trim(cbEndDate.Text));  }&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbBeginDateOnChange(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if cbBeginDate.ItemIndex &#62; cbEndDate.ItemIndex then cbEndDate.ItemIndex:=cbBeginDate.ItemIndex;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbEndDateOnChange(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if cbBeginDate.ItemIndex &#62; cbEndDate.ItemIndex then cbBeginDate.ItemIndex:=cbEndDate.ItemIndex;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbLikeOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  lbxCodZak.Enabled:=TfrxRadioButtonControl(Sender).Checked;&#13;&#10;  dblCodZak.Enabled:= not TfrxRadioButtonControl(Sender).Checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbRecordOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  lbxCodZak.Enabled:= not TfrxRadioButtonControl(Sender).Checked;&#13;&#10;  dblCodZak.Enabled:= TfrxRadioButtonControl(Sender).Checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure edCodZakFilterOnChange(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  qZakaz.Close;&#13;&#10;  qZakaz.Open;&#13;&#10;  lbxCodZak.Items.Clear;&#13;&#10;  dblCodZak.DataSet.First;&#13;&#10;  while (not dblCodZak.DataSet.Eof) and (dblCodZak.DataSet.RecNo &#60;= 100)&#13;&#10;    do begin&#13;&#10;      lbxCodZak.Items.Add(qZakaz.FieldByName(dblCodZak.ListField).AsString);&#13;&#10;      dblCodZak.DataSet.Next&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  IniFile:=TIniFile.Create('.\Îò÷åòû\Ñïðàâî÷íèê îñòàòêîâ\Îñòàòêè äëÿ ýêîíîìèñòà ïî ãîäàì.ini');&#13;&#10;end." StoreInDFM="False">
