    <TfrxERQuery Name="RepData" UserName="RepData" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT     S_SubAtrib.Key_Rekv, S_SubAtrib.Val_Str, S_Subjects.Name_Sub, S_Subjects.cod_sub, Pr_TabelDetail.Cod_Zak, SUM(Pr_TabelDetail.Kolvo) &#13;&#10;                      AS SmTrud, T_Oper.Name_Oper, T_VPredm.Cod_Predm, T_VPredm.Name_Predm, T_VPredm.OrderCod_Predm&#13;&#10;FROM         Pr_Tabel INNER JOIN&#13;&#10;                      Pr_TabelDetail ON Pr_Tabel.Key_Tabel = Pr_TabelDetail.Key_Tabel&#13;&#10;                      and Pr_Tabel.Date_Tabel &#62;=:DateFrom AND  Pr_Tabel.Date_Tabel&#60; :DateTo                        &#13;&#10;                       INNER JOIN&#13;&#10;                      S_SubVer ON Pr_Tabel.Key_Sub_Ver = S_SubVer.Key_Sub_Ver INNER JOIN&#13;&#10;                      S_Subjects ON S_SubVer.Key_Sub = S_Subjects.Key_Sub INNER JOIN&#13;&#10;                      S_SubAtrib ON S_SubVer.Key_Sub = S_SubAtrib.Key_Sub AND S_SubVer.Sub_Version = S_SubAtrib.Sub_Version INNER JOIN&#13;&#10;                      T_VPredm ON Pr_TabelDetail.Key_Predm = T_VPredm.Key_Predm LEFT OUTER JOIN&#13;&#10;                      T_Oper ON Pr_TabelDetail.Cod_Oper = T_Oper.Cod_Oper&#13;&#10;GROUP BY S_SubAtrib.Key_Rekv, S_SubAtrib.Val_Str, S_Subjects.Name_Sub, S_Subjects.cod_sub, Pr_TabelDetail.Cod_Zak, T_Oper.Name_Oper, &#13;&#10;                      T_VPredm.Cod_Predm, T_VPredm.Name_Predm, T_VPredm.OrderCod_Predm&#13;&#10;HAVING      (S_SubAtrib.Key_Rekv = 14)&#13;&#10;                                                                                                                  &#13;&#10;ORDER BY S_SubAtrib.Val_Str, S_Subjects.Name_Sub, Pr_TabelDetail.Cod_Zak, T_VPredm.OrderCod_Predm  " PropData=""/>
