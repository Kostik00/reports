    <TfrxERQuery Name="TrudQRY" UserName="TrudQRY" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT     &#13;&#10;       D_Zakaz.Cod_Zak, &#13;&#10;       D_Zakaz.Name_Zak,&#13;&#10;       P_VKZ_KOLVO.Key_VKZ_Izdel,                                                             &#13;&#10;	      T_Predm.Cod_Predm,&#13;&#10;	      T_Predm.OrderCod_Predm,&#13;&#10;	      T_Predm.Name_Predm,&#13;&#10;       T_Oper_Gr.Name_GrGr,&#13;&#10;       T_Oper.Name_Oper,                            &#13;&#10;       SUM(Pr_TabelDetail.Kolvo*P_VKZ_KOLVO.K1/P_VKZ_KOLVO.K2) AS Kolvo&#13;&#10;FROM   &#13;&#10;      D_Zakaz&#13;&#10;      INNER JOIN Pr_TabelDetail&#13;&#10;             ON D_Zakaz.Cod_Zak = Pr_TabelDetail.Cod_Zak &#13;&#10;      INNER JOIN Pr_Tabel &#13;&#10;             ON Pr_TabelDetail.Key_Tabel = Pr_Tabel.Key_Tabel&#13;&#10;      INNER JOIN P_VKZ_KOLVO&#13;&#10;             ON   P_VKZ_KOLVO.Cod_Zak=Pr_TabelDetail.Cod_Zak&#13;&#10;             AND  P_VKZ_KOLVO.Key_Predm= Pr_TabelDetail.Key_Predm&#13;&#10;          &#13;&#10;      INNER JOIN P_VKZ&#13;&#10;                 ON P_VKZ.Key_VKZ=P_VKZ_KOLVO.Key_VKZ_Izdel&#13;&#10;      INNER JOIN T_VPredm T_Predm&#13;&#10;                 ON T_Predm.Key_Predm=P_VKZ.Key_Predm&#13;&#10;          &#13;&#10;      LEFT JOIN T_Oper&#13;&#10;              ON T_Oper.Cod_Oper = Pr_TabelDetail.Cod_Oper&#13;&#10;      LEFT JOIN  T_Oper_Gr&#13;&#10;             ON  T_Oper_Gr.Cod_Oper_Gr=T_Oper.Cod_Oper_Gr                                                                 &#13;&#10;&#13;&#10;  &#13;&#10;WHERE Pr_Tabel.Date_Tabel &#62;=:Date_From AND Pr_Tabel.Date_Tabel &#60;=:Date_To                                                                                                                                                &#13;&#10;                           &#13;&#10;GROUP BY        D_Zakaz.Cod_Zak, &#13;&#10;       D_Zakaz.Name_Zak,&#13;&#10;       P_VKZ_KOLVO.Key_VKZ_Izdel,                                                             &#13;&#10;	   T_Predm.Cod_Predm,&#13;&#10;	      T_Predm.OrderCod_Predm,&#13;&#10;	   T_Predm.Name_Predm,&#13;&#10;       T_Oper_Gr.Name_GrGr,    &#13;&#10;       T_Oper.Name_Oper                            &#13;&#10;  &#13;&#10;ORDER BY D_Zakaz.Cod_Zak, T_Predm.OrderCod_Predm,  T_Oper.Name_Oper      &#13;&#10;&#13;&#10;  &#13;&#10;&#13;&#10;    " PropData=""/>
