r = T_Oper.Cod_Oper &#13;&#10;		INNER JOIN T_Oper_Gr &#13;&#10;			ON T_Oper.Cod_Oper_Gr = T_Oper_Gr.Cod_Oper_Gr&#13;&#10;&#13;&#10;WHERE     (MasterDSE.Key_VKZ = :Key_VKZ  )&#13;&#10;GROUP BY &#13;&#10;	MasterDSE.Key_VKZ, &#13;&#10;	T_Oper_Gr.Name_Oper_Gr&#13;&#10;) T2&#13;&#10;ON T1.Name_Oper_Gr = T2.Name_Oper_Gr&#13;&#10;&#13;&#10;ORDER BY T1.Name_Oper_Gr                                                 " PropData="05704C656674024C0470546F7002500A506172616D657465727301010C24000000204E616D653D224B65795F564B5A222044617461547970653D226674556E6B6E6F776E220000"/>
