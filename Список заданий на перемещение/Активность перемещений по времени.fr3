    <TfrxERQuery Name="Subj" UserName="ФИО" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="		SELECT DISTINCT Pr_MTask_Detail.Key_Sub, S_Subjects.Name_Sub&#13;&#10;		FROM            &#13;&#10;				Pr_MTask_Detail &#13;&#10;				INNER JOIN Pr_MTask&#13;&#10;					ON Pr_MTask.Key_MTask = Pr_MTask_Detail.Key_MTask&#13;&#10;				INNER JOIN S_Subjects ON Pr_MTask_Detail.Key_Sub = S_Subjects.Key_Sub&#13;&#10;&#13;&#10;		WHERE &#13;&#10;			Pr_MTask.Date_Task &#62;= '20170615'&#13;&#10;  " PropData="05704C65667402680470546F70024C0A506172616D65746572730100"/>
    <TfrxERQuery Name="ERQuery1" UserName="ERQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="Subj" IgnoreDupParams="False" SQL.Text="SELECT  Subj.*, TmpTimeTable.H, TblCnt.Cnt&#13;&#10;FROM&#13;&#10;	(&#13;&#10;		SELECT   NumRec H&#13;&#10;		FROM [dbo].[SYS_Days] &#13;&#10;		WHERE NumRec &#60;=24&#13;&#10;&#13;&#10;	) TmpTimeTable&#13;&#10;	CROSS JOIN &#13;&#10;	(&#13;&#10;		SELECT DISTINCT Pr_MTask_Detail.Key_Sub, S_Subjects.Name_Sub&#13;&#10;		FROM            &#13;&#10;				Pr_MTask_Detail &#13;&#10;				INNER JOIN Pr_MTask&#13;&#10;					ON Pr_MTask.Key_MTask = Pr_MTask_Detail.Key_MTask&#13;&#10;				INNER JOIN S_Subjects ON Pr_MTask_Detail.Key_Sub = S_Subjects.Key_Sub&#13;&#10;&#13;&#10;		WHERE &#13;&#10;			Pr_MTask.Date_Task &#62;= '20170615'&#13;&#10;	) Subj&#13;&#10;	LEFT JOIN &#13;&#10;	(&#13;&#10;&#13;&#10;		SELECT        &#13;&#10;			Pr_MTask_Detail.Key_Sub, &#13;&#10;			DATEPART(hh, Pr_MTask_Detail.DtCompl) AS H, &#13;&#10;			COUNT(Pr_MTask_Detail.Key_MTask) AS Cnt&#13;&#10;		FROM&#13;&#10;			Pr_MTask_Detail &#13;&#10;			INNER JOIN Pr_MTask&#13;&#10;				ON Pr_MTask.Key_MTask = Pr_MTask_Detail.Key_MTask&#13;&#10;		WHERE &#13;&#10;			Pr_MTask.Date_Task &#62;= '20170615'&#13;&#10;   AND Pr_MTask_Detail.DtCompl BETWEEN :DateFrom AND :DateTo                                                                                              &#13;&#10;			AND Pr_MTask_Detail.DtCompl is not null&#13;&#10;		GROUP BY &#13;&#10;			Pr_MTask_Detail.Key_Sub, &#13;&#10;			DATEPART(hh, Pr_MTask_Detail.DtCompl)&#13;&#10;	) TblCnt&#13;&#10;	ON &#13;&#10;			TblCnt.H=TmpTimeTable.H&#13;&#10;		AND TblCnt.Key_Sub = Subj.Key_Sub&#13;&#10;WHERE Subj.Key_Sub= :Key_Sub                                                              &#13;&#10;ORDER BY Subj.Name_Sub, TmpTimeTable.H  " PropData=""/>
    <TfrxERQuery Name="ERQuery2" UserName="ERQuery2" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT    TmpTimeTable.H, TblCnt.Cnt&#13;&#10;FROM&#13;&#10;	(&#13;&#10;		SELECT   NumRec H&#13;&#10;		FROM [dbo].[SYS_Days] &#13;&#10;		WHERE NumRec &#60;=24&#13;&#10;&#13;&#10;	) TmpTimeTable&#13;&#10;	LEFT JOIN &#13;&#10;	(&#13;&#10;&#13;&#10;		SELECT        &#13;&#10;			DATEPART(hh, Pr_MTask_Detail.DtZayavka) AS H, &#13;&#10;			COUNT(Pr_MTask_Detail.Key_MTask) AS Cnt&#13;&#10;		FROM&#13;&#10;			Pr_MTask_Detail &#13;&#10;			INNER JOIN Pr_MTask&#13;&#10;				ON Pr_MTask.Key_MTask = Pr_MTask_Detail.Key_MTask&#13;&#10;		WHERE &#13;&#10;			      Pr_MTask_Detail.DtZayavka  BETWEEN :DateFrom AND :DateTo&#13;&#10;   	AND 	Pr_MTask_Detail.DtZayavka is not null&#13;&#10;		GROUP BY &#13;&#10;			DATEPART(hh, Pr_MTask_Detail.DtZayavka)&#13;&#10;	) TblCnt&#13;&#10;	ON &#13;&#10;			TblCnt.H=TmpTimeTable.H&#13;&#10;                                                            &#13;&#10;ORDER BY  TmpTimeTable.H  " PropData=""/>
