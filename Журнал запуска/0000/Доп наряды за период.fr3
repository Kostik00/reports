    <TfrxERQuery Name="SmQRY" UserName="SmQRY" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="DECLARE @DateFrom datetime&#13;&#10;DECLARE @DateTo datetime&#13;&#10;&#13;&#10;SET @DateFrom = :DateFrom  &#13;&#10;SET @DateTo   = :DateTo   &#13;&#10;&#13;&#10;SELECT     &#13;&#10;       Pr_Pasp.Cod_Zak, &#13;&#10;	      P_VKZ_1.Key_VKZ AS Key_VKZ_Izdel,&#13;&#10;       T_VPredm_1.Cod_Predm AS Izdel,&#13;&#10;       P_VKZ_1.Kolvo KolvoIzdel,                                                           &#13;&#10;         &#13;&#10;       T_VPredm.Cod_Predm, &#13;&#10;       T_VPredm.Name_Predm, &#13;&#10;	      Pr_Pasp_Rows.N_Oper_Sm,&#13;&#10;&#13;&#10;       Pr_Pasp_Rows.NumDopN,&#13;&#10;       Pr_Pasp_Rows.DateDopN,                 &#13;&#10;  &#13;&#10;       Pr_Pasp_Rows.Key_Pasp_Rows Key_Pasp_Rows,                                           &#13;&#10;       T_Oper.Name_K_Oper, &#13;&#10;       T_Oper.Name_Oper,&#13;&#10;       S_SubVer.F_Name_Sub,                                                           &#13;&#10;       SUM(Pr_Pasp_Ac.AcKolvo*Pr_Pasp_Rows.TSm*Pr_PaspDet.KolvoZap/Pr_Pasp.PaspKolvo) AS TSm,&#13;&#10;       T_VPredm.OrderCod_Predm                &#13;&#10;FROM&#13;&#10;&#13;&#10;       Pr_Pasp_Ac&#13;&#10;       INNER JOIN S_SubVer &#13;&#10;               ON Pr_Pasp_Ac.Key_Sub_Ver = S_SubVer.Key_Sub_Ver&#13;&#10;                 &#13;&#10;&#13;&#10;                  &#13;&#10;       INNER JOIN Pr_Pasp_Rows &#13;&#10;               ON Pr_Pasp_Ac.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows&#13;&#10;&#13;&#10;                 &#13;&#10;       INNER JOIN T_Oper&#13;&#10;               ON Pr_Pasp_Rows.Cod_Oper = T_Oper.Cod_Oper &#13;&#10;       INNER JOIN Pr_Pasp &#13;&#10;               ON Pr_Pasp_Rows.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;       INNER JOIN T_VPredm&#13;&#10;               ON Pr_Pasp.Key_Predm = T_VPredm.Key_Predm &#13;&#10;&#13;&#10;       INNER JOIN   Pr_PaspDet &#13;&#10;				           ON Pr_PaspDet.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;&#13;&#10;&#13;&#10;       INNER JOIN   P_VKZ    &#13;&#10;               ON P_VKZ.Key_VKZ = Pr_PaspDet.Key_VKZ &#13;&#10;               &#13;&#10;    &#13;&#10;  &#13;&#10;       &#13;&#10;&#13;&#10;       INNER JOIN P_VKZ AS P_VKZ_1&#13;&#10;               ON P_VKZ.Key_VKZ_Izdel = P_VKZ_1.Key_VKZ &#13;&#10;         &#13;&#10;       INNER JOIN T_VPredm AS T_VPredm_1 &#13;&#10;               ON P_VKZ_1.Key_Predm = T_VPredm_1.Key_Predm&#13;&#10;&#13;&#10;&#13;&#10;                 &#13;&#10;WHERE     &#13;&#10;       (Pr_Pasp_Rows.Key_TPRows IS NULL)&#13;&#10;       AND Pr_Pasp_Ac.Ac_Date &#62;= @DateFrom&#13;&#10;       AND Pr_Pasp_Ac.Ac_Date &#60;= @DateTo&#13;&#10;       AND Pr_Pasp_Rows.DateDopN is not NULL&#13;&#10;       AND Pr_Pasp_Rows.TSm&#62;0                                &#13;&#10;           &#13;&#10;  &#13;&#10;GROUP BY &#13;&#10;       Pr_Pasp.Cod_Zak, &#13;&#10;	   P_VKZ_1.Key_VKZ,&#13;&#10;       T_VPredm_1.Cod_Predm,&#13;&#10;        P_VKZ_1.Kolvo,             &#13;&#10;       T_VPredm.Cod_Predm, &#13;&#10;       T_VPredm.Name_Predm, &#13;&#10;	   Pr_Pasp_Rows.N_Oper_Sm,&#13;&#10;       Pr_Pasp_Rows.NumDopN,&#13;&#10;       Pr_Pasp_Rows.DateDopN,                 &#13;&#10;  &#13;&#10;       Pr_Pasp_Rows.Key_Pasp_Rows,               &#13;&#10;       T_Oper.Name_K_Oper, &#13;&#10;       T_Oper.Name_Oper,&#13;&#10;       S_SubVer.F_Name_Sub,             &#13;&#10;       T_VPredm.OrderCod_Predm&#13;&#10;         &#13;&#10;ORDER BY Pr_Pasp.Cod_Zak, T_VPredm.OrderCod_Predm, Pr_Pasp_Rows.N_Oper_Sm             " PropData=""/>
