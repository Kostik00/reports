tion].DD_DatePostav, -- Delivery Date                                &#13;&#10;    [rComposition].DD_Key_Predm_BySP,            &#13;&#10;    [rComposition].DD_VMZ_Notes,&#13;&#10;    [T_SPRows].[SpNotes] AS [SpNotes2]                  &#13;&#10;FROM          &#13;&#10;(SELECT DISTINCT&#13;&#10;    [P_VKZ1].[Key_VKZ]                  AS [Key_VKZ1],          -- Composition                                    &#13;&#10;    [P_VKZ1].[Key_VKZ_Izdel]            AS [Key_VKZ_Izdel1],            &#13;&#10;    [P_VKZ1].[CodStrSv]                 AS [CodStrSv1],&#13;&#10;    &#13;&#10;    /*&#13;&#10;    [P_VKZ1].[DataZapK]                 AS [DataZapK],&#13;&#10;    [P_VKZ1].[DataZapA]                 AS [DataZapA],        &#13;&#10;    */&#13;&#10;    &#13;&#10;    [P_VKZ_PLN].DTZCRMIN                AS [DataZapK],&#13;&#10;    [P_VKZ_PLN].DTZPlnMIN               AS [DataZapA],    &#13;&#10;        &#13;&#10;    [P_VKZ1].[Kolvo]                    AS [Kolvo1],  &#13;&#10;    [P_VKZ1].[Kolvo_On_Izdel]           AS [Kolvo_On_Izdel1],&#13;&#10;    [T_Edizm1].[Name_K_Ed]              AS [SPEdizmName1],      &#13;&#10;    [P_VKZ1].[Lvl]                      AS [Lvl1],&#13;&#10;    [MASTER_VKZ].[CodStrSv]             AS [MASTER_CodStrSv1],          &#13;&#10;    [T_VPredm].[Key_Predm]              AS [Key_Predm1],&#13;&#10;    [T_VPredm].[Cod_Predm]              AS [Cod_Predm1],&#13;&#10;    [T_VPredm].[Name_Predm]             AS [Name_Predm1],  &#13;&#10;    [T_VPredm].[Cod_Postav]             AS [Cod_Postav1],&#13;&#10;    [T_VPredm].[Name_Postav]            AS [Name_Postav1],&#13;&#10;    [T_VPredm].[Name_Prin]              AS [Name_Prin1],&#13;&#10;  &#13;&#10;    [T_SPRows].[SP_Position]            AS [SP_Position1],                &#13;&#10;    [P_VKZ_Zap_Det].[Key_VKZ]           AS [ZD_Key_VKZ1],          &#13;&#10;    [P_VKZ_Zap_Det].[Key_VKZ_Zap]       AS [ZD_Key_VKZ_Zap1],&#13;&#10;    &#13;&#10;    [P_VMZ2].[Key_VMZ]                  AS [Key_VMZ2],          -- Purechase                            &#13;&#10;    [P_VMZ2].[Key_VKZ_Zap]              AS [Key_VKZ_Zap2],&#13;&#10;             &#13;&#10;    [P_VMZ2].[VMZ_Notes]                AS [VMZ_Notes2],            &#13;&#10;    [T_SPRows2].[Key_SPRow]             AS [Key_SPRow2],&#13;&#10;    [T_SPRows2].[Kolvo]                 AS [Kolvo2],                 &#13;&#10;    [T_Edizm2].[Name_K_Ed]              AS [SPEdizmName2],                                                                    &#13;&#10;    [T_VPredm2].[Key_Predm]             AS [Key_Predm2],&#13;&#10;-- function PrintPredm BEGIN&#13;&#10;    [T_VPredm2].[PrintFlag]             AS [PrintFlag2],             &#13;&#10;    [T_VPredm2].[Cod_Predm]             AS [Cod_Predm2],&#13;&#10;    [T_VPredm2].[Name_Predm]            AS [Name_Predm2],  &#13;&#10;      &#13;&#10;    [T_VPredm2].[Marka]                 AS [Marka2],&#13;&#10;    [T_VPredm2].[Type_NTD_Marka]        AS [Type_NTD_Marka2],  &#13;&#10;    [T_VPredm2].[Name_NTD_Marka]        AS [Name_NTD_Marka2],          &#13;&#10;&#13;&#10;    [T_VPredm2].[Sortam]                AS [Sortam2],      &#13;&#10;    [T_VPredm2].[Key_NTD_Sortam]        AS [Key_NTD_Sortam2],  &#13;&#10;    [T_VPredm2].[Type_NTD_Sortam]       AS [Type_NTD_Sortam2],  &#13;&#10;    [T_VPredm2].[Name_NTD_Sortam]       AS [Name_NTD_Sortam2],    &#13;&#10;-- function PrintPredm END          &#13;&#10;    [T_SPRows2].[SP_position]           AS [SP_position2],&#13;&#10;    CONCAT(CAST([T_SPRows2].[SP_position] AS NVARCHAR(4)), ': ',[T_VPredm2].[Cod_Predm])&#13;&#10;                                        AS [OutlineText2],&#13;&#10;    [Pr_Pasp2].[Key_Pasport]            AS [Key_Pasport2],&#13;&#10;    [T_SP2].[Key_SP]                    AS [Key_SP2],&#13;&#10;    [T_SP2].[SP_Version]                AS [SP_Version2],                             &#13;&#10;&#13;&#10;    Store_Pr_CZ.Cod_Pr_CZ               AS Store_Cod_Pr_CZ,&#13;&#10;    Store_Pr_CZ.Status_CZ               AS Store_Status_CZ,        &#13;&#10;    Store_Pr_CZ_Status.Name_Status_CZ   AS Store_Name_Status_CZ,        &#13;&#10;    Store_Pr_CZ_DT.Key_Pr_CZ_DT         AS Store_Key_Pr_CZ_DT,&#13;&#10;    Store_Pr_CZ_DT.Kolvo                AS Store_Kolvo,         -- Store&#13;&#10;    Store_A_Rezerv.KolvoInBaseEd        AS Store_KolvoInBaseEd, -- Кол-во со склада&#13;&#10;    Store_A_VID_Mat.RegN                AS Store_RegN,          &#13;&#10;    Store_A_VID_Mat.RegN_Sert           AS Store_RegN_Sert,  &#13;&#10;    Store_T_SertSost.Key_SostSert       AS Store_Key_SostSert,&#13;&#10;    Store_T_SertSost.Name_SostSert      AS Store_Name_SostSert,    &#13;&#10;    Store_Pr_CZ_DT.Cod_DocumZ           AS Store_PCodDocumZ,&#13;&#10;    Store_A_Rezerv.Cod_DocumZ           AS Store_RCodDocumZ,&#13;&#10;/* ***** BEGIN ***** */                        &#13;&#10;    Store_T_VPredm.PrintFlag            AS Store_PrintFlag, &#13;&#10;    Store_T_VPredm.Key_Predm            AS Store_Key_Predm,     &#13;&#10;    Store_T_VPredm.Cod_Predm            AS Store_Cod_Predm, &#13;&#10;    Store_T_VPredm.Name_Predm           AS Store_Name_Predm,  &#13;&#10;    Store_T_Edizm.Name_K_Ed             AS Store_SPEdizmName,&#13;&#10;    Store_EDZS.Name_K_Ed                AS Store_StoreEdizmName,    &#13;&#10;    Store_T_SertPos.SrokGodn            AS Store_SrokGodn,    &#13;&#10;    &#13;&#10;/* ***** END ***** */&#13;&#10;  &#13;&#10;    OMTS_A_Rezerv.Key_Rezerv            AS OMTS_Key_Rezerv,     -- OMTS                                                            &#13;&#10;    OMTS_A_Rezerv.Kolvo                 AS OMTS_Kolvo,&#13;&#10;    OMTS_A_Rezerv.KolvoInBaseEd         AS OMTS_KolvoInBaseEd,&#13;&#10;    OMTS_A_Rezerv.Cod_DocumZ            AS OMTS_CodDocumZ,        &#13;&#10;    OMTS_A_VID_Mat.RegN                 AS OMTS_RegN,  &#13;&#10;    OMTS_A_VID_Mat.RegN_Sert            AS OMTS_RegN_Sert,&#13;&#10;    OMTS_T_SertSost.Key_SostSert        AS OMTS_Key_SostSert,&#13;&#10;    OMTS_T_SertSost.Name_SostSert       AS OMTS_Name_SostSert,    &#13;&#10;    OMTS_T_SertPos.SrokGodn             AS OMTS_SrokGodn,    &#13;&#10;    OMTS_T_VPredm.Key_Predm             AS OMTS_Key_Predm,&#13;&#10;    OMTS_T_VPredm.Name_Predm            AS OMTS_Name_Predm,&#13;&#10;/* ***** BEGIN ***** */        &#13;&#10;    OMTS_EDZR.Name_K_Ed                 AS OMTS_SPEdizmName,&#13;&#10;    OMTS_EDZS.Name_K_Ed                 AS OMTS_StoreEdizmName,          &#13;&#10;/* ***** END ***** */        &#13;&#10;    [P_VMZ2].[DatePostav]               AS DD_DatePostav, -- Delivery Date                                &#13;&#10;    [P_VMZ2].[Key_Predm_BySP]           AS DD_Key_Predm_BySP,            &#13;&#10;    [P_VMZ2].[VMZ_Notes]                AS DD_VMZ_Notes      &#13;&#10;  FROM&#13;&#10;-- Список входящих ВКЗ  &#13;&#10;    [P_VKZ] AS [P_VKZ1]&#13;&#10;    LEFT OUTER JOIN [P_VKZ_PLN]&#13;&#10;            ON [P_VKZ_PLN].[Key_VKZ] = [P_VKZ1].[Key_VKZ] AND [P_VKZ_PLN].[Key_PlnV] = @Key_PlnV&#13;&#10;    INNER JOIN [P_VKZ_Zap_Det]&#13;&#10;            ON [P_VKZ_Zap_Det].[Key_VKZ] = [P_VKZ1].[Key_VKZ]                                           &#13;&#10;    INNER JOIN [T_VPredm]&#13;&#10;            ON [T_VPredm].[Key_Predm] = [P_VKZ1].[Key_Predm]&#13;&#10;    LEFT OUTER JOIN [P_VKZ] AS [MASTER_VKZ]                   &#13;&#10;            ON [MASTER_VKZ].[Key_VKZ] = [P_VKZ1].[Key_VKZ_Master]                                      &#13;&#10;    LEFT OUTER JOIN [T_SPRows]&#13;&#10;            ON [T_SPRows].[Key_SPRow] = [P_VKZ1].[Key_SPRow]&#13;&#10;    LEFT OUTER JOIN T_Edizm AS T_Edizm1 -- Еденица измерения по спецификации                                           &#13;&#10;            ON T_Edizm1.Cod_Ed_Izm = [T_SPRows].Cod_Ed_Izm&#13;&#10;-- Ведомость заказа материалов            &#13;&#10;    LEFT OUTER JOIN [P_VMZ] AS [P_VMZ2]&#13;&#10;            ON [P_VMZ2].[Key_VKZ_Zap] = [P_VKZ_Zap_Det].[Key_VKZ_Zap]    &#13;&#10;    INNER JOIN [T_SPRows] AS [T_SPRows2]&#13;&#10;            ON [T_SPRows2].[Key_SPRow] = [P_VMZ2].[Key_SPRow] AND&#13;&#10;               ((([T_SPRows2].[SP_Position] &#62; 0     AND [T_SPRows2].[SP_Position] &#60; 1000) AND (@iBelow1000 = 1)) OR&#13;&#10;                (([T_SPRows2].[SP_Position] &#62;= 1000 AND [T_SPRows2].[SP_Position] &#60; 2000) AND (@iAbove1000 = 1)))&#13;&#10;    LEFT OUTER JOIN T_Edizm AS T_Edizm2 -- Еденица измерения по спецификации                                           &#13;&#10;            ON T_Edizm2.Cod_Ed_Izm = [T_SPRows2].Cod_Ed_Izm                    &#13;&#10;    INNER JOIN [T_SP] AS [T_SP2]&#13;&#10;            ON [T_SP2].[Key_SP] = [T_SPRows2].[Key_SP]                                                                                                                     &#13;&#10;    INNER JOIN [T_VPredm] AS [T_VPredm2]&#13;&#10;            ON [T_VPredm2].[Key_Predm] = [T_SPRows2].[Key_Predm] AND [T_VPredm2].[Cod_Postav] = '8'                 &#13;&#10;    LEFT OUTER JOIN [Pr_Pasp] AS [Pr_Pasp2]&#13;&#10;            ON [Pr_Pasp2].[Key_VKZ_Zap] = [P_VMZ2].[Key_VKZ_Zap] AND [Pr_Pasp2].[Key_SP] = [P_VMZ2].[Key_SP] AND [Pr_Pasp2].[Key_Pasp_Status] &#62; 0      &#13;&#10;-- Резервирование со склада           &#13;&#10;    LEFT OUTER JOIN Pr_CZ_DT AS Store_Pr_CZ_DT           &#13;&#10;            ON Store_Pr_CZ_DT.Key_SPRow = [T_SPRows2].[Key_SPRow] AND Store_Pr_CZ_DT.Key_Pasport = [Pr_Pasp2].[Key_Pasport]&#13;&#10;    LEFT OUTER JOIN A_Rezerv AS Store_A_Rezerv                                &#13;&#10;            ON Store_A_Rezerv.Key_Rezerv = Store_Pr_CZ_DT.Key_Rezerv        &#13;&#10;    LEFT OUTER JOIN Pr_CZ AS Store_Pr_CZ                    &#13;&#10;            ON Store_Pr_CZ.Key_Pr_CZ = Store_Pr_CZ_DT.Key_Pr_CZ -- AND Store_Pr_CZ.Status_CZ IN (4)&#13;&#10;    LEFT OUTER JOIN Pr_CZ_Status AS Store_Pr_CZ_Status                    &#13;&#10;            ON Store_Pr_CZ_Status.Status_CZ = Store_Pr_CZ.Status_CZ                &#13;&#10;    LEFT OUTER JOIN A_VID_Mat AS Store_A_VID_Mat           &#13;&#10;            ON Store_A_VID_Mat.Key_ID_Mat = Store_Pr_CZ_DT.Key_ID_Mat            &#13;&#10;    LEFT OUTER JOIN T_Sert AS Store_T_Sert&#13;&#10;       			  ON Store_T_Sert.Key_Sert = Store_A_VID_Mat.Key_Sert&#13;&#10;       			  &#13;&#10;    LEFT OUTER JOIN T_SertPos AS Store_T_SertPos ON Store_T_SertPos.Key_Sert = Store_T_Sert.Key_Sert       			  &#13;&#10;       			  &#13;&#10;    LEFT OUTER JOIN T_SertSost AS Store_T_SertSost&#13;&#10;       			  ON Store_T_SertSost.Key_SostSert = Store_T_Sert.Key_SostSert                        &#13;&#10;    LEFT OUTER JOIN T_VPredm AS Store_T_VPredm            &#13;&#10;            ON Store_T_VPredm.Key_Predm = Store_A_VID_Mat.Key_Predm                 &#13;&#10;    LEFT OUTER JOIN T_SPRows AS Store_T_SPRows          &#13;&#10;            ON Store_T_SPRows.Key_SPRow = Store_Pr_CZ_DT.Key_SPRow  &#13;&#10;    LEFT OUTER JOIN T_Edizm AS Store_T_Edizm -- Еденица измерения по спецификации                                           &#13;&#10;            ON Store_T_Edizm.Cod_Ed_Izm = Store_T_SPRows.Cod_Ed_Izm&#13;&#10;    LEFT OUTER JOIN S_SubVer AS Store_S_SubVer                                  &#13;&#10;            ON Store_S_SubVer.Key_Sub_Ver = Store_Pr_CZ_DT.Key_Sub_Ver&#13;&#10;    LEFT OUTER JOIN T_Edizm AS Store_EDZS -- Еденица измерения для склада                              &#13;&#10;       			  ON Store_EDZS.Cod_Ed_Izm = Store_T_VPredm.Sklad_Ed_Izm&#13;&#10;-- Резервирование от ОМТС                     &#13;&#10;    LEFT OUTER JOIN A_Rezerv AS OMTS_A_Rezerv            &#13;&#10;            ON OMTS_A_Rezerv.Key_VMZ IN (SELECT P_VMZ3.Key_VMZ FROM P_VMZ AS P_VMZ3 WHERE P_VMZ3.Key_SPRow = [T_SPRows2].[Key_SPRow] AND P_VMZ3.Key_VKZ_Zap = [P_VMZ2].[Key_VKZ_Zap])                                                                                                   &#13;&#10;    LEFT OUTER JOIN A_VID_Mat AS OMTS_A_VID_Mat                      &#13;&#10;       			  ON OMTS_A_VID_Mat.Key_ID_Mat = OMTS_A_Rezerv.Key_ID_Mat&#13;&#10;    LEFT OUTER JOIN T_Sert AS OMTS_T_Sert&#13;&#10;       			  ON OMTS_T_Sert.Key_Sert = OMTS_A_VID_Mat.Key_Sert&#13;&#10;       			  &#13;&#10;    LEFT OUTER JOIN T_SertPos  AS OMTS_T_SertPos ON OMTS_T_SertPos.Key_Sert =  OMTS_A_VID_Mat.Key_Sert       			  &#13;&#10;       			  &#13;&#10;    LEFT OUTER JOIN T_SertSost AS OMTS_T_SertSost&#13;&#10;       			  ON OMTS_T_SertSost.Key_SostSert = OMTS_T_Sert.Key_SostSert&#13;&#10;    LEFT OUTER JOIN T_VPredm AS OMTS_T_VPredm              &#13;&#10;       			  ON OMTS_T_VPredm.Key_Predm = OMTS_A_VID_Mat.Key_Predm&#13;&#10;    LEFT OUTER JOIN T_Edizm AS OMTS_EDZR -- Еденица измерения для резервирования                                                                                                                                           &#13;&#10;       			  ON OMTS_EDZR.Cod_Ed_Izm = OMTS_A_Rezerv.Cod_Ed_Izm&#13;&#10;    LEFT OUTER JOIN T_Edizm AS OMTS_EDZS -- Еденица измерения для склада                              &#13;&#10;       			  ON OMTS_EDZS.Cod_Ed_Izm = OMTS_T_VPredm.Sklad_Ed_Izm             &#13;&#10;    LEFT OUTER JOIN S_SubVer AS OMTS_S_SubVer              &#13;&#10;       			  ON OMTS_S_SubVer.Key_Sub_Ver = OMTS_A_Rezerv.Key_Sub_Ver    &#13;&#10;  WHERE&#13;&#10;    [P_VKZ1].[Key_VKZ_Izdel] =    (SELECT [Key_VKZ_Izdel] FROM [P_VKZ] AS [A1VKZ] WHERE [A1VKZ].[Key_VKZ] = @Key_VKZ) AND&#13;&#10;    [P_VKZ1].[CodStrSv]      LIKE (SELECT [CodStrSv]      FROM [P_VKZ] AS [A2VKZ] WHERE [A2VKZ].[Key_VKZ] = @Key_VKZ)+'%'&#13;&#10;    ) AS [rComposition]&#13;&#10;    LEFT OUTER JOIN T_SPRows&#13;&#10;            ON T_SPRows.Key_SPRow = [rComposition].[Key_SPRow2]&#13;&#10;  ORDER BY&#13;&#10;    [rComposition].[CodStrSv1], [rComposition].[SP_Position1], [rComposition].[SP_position2];        " PropData=""/>
  </TfrxDataPage>
