Visible:=False;&#13;&#10;//   DetailData1.Visible:=false;                                 &#13;&#10;  end&#13;&#10;  else&#13;&#10;  &#13;&#10;  begin&#13;&#10;//   Child1.Visible:=True;                                                     &#13;&#10;//   DetailData1.Visible:=False;                                 &#13;&#10;  end;&#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="">
