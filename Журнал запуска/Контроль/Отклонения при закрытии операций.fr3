    <TfrxERQuery Name="SmQRY" UserName="SmQRY" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT     &#13;&#10;	Pr_Pasp.Cod_Pasp,&#13;&#10; Pr_Pasp.Date_Vip,         &#13;&#10;	Pr_Pasp_Rows.Key_Pasport, &#13;&#10;	Pr_Pasp_Rows.AcDate, &#13;&#10;	SUM(Pr_Pasp_Ac.AcKolvo) AS AcKolvo, &#13;&#10;	Pr_Pasp.PaspKolvo, &#13;&#10;	T_VPredm.Cod_Predm, &#13;&#10;	T_VPredm.Name_Predm,&#13;&#10;  Pr_Pasp_Rows.Key_Pasp_Rows,    &#13;&#10;  Pr_Pasp_Rows.N_Oper_Sm,&#13;&#10;  Pr_Pasp_Rows.TSm,                          &#13;&#10;  T_Oper.Name_K_Oper,        &#13;&#10;  T_Oper.Name_Oper,&#13;&#10;&#13;&#10;  Pr_Pasp_Status.Name_Pasp_Status,&#13;&#10;  Pr_Status_Oper.Name_Status_Oper          &#13;&#10;                                &#13;&#10;&#13;&#10;FROM         &#13;&#10;	Pr_Pasp &#13;&#10;	INNER JOIN Pr_Pasp_Rows &#13;&#10;		ON Pr_Pasp.Key_Pasport = Pr_Pasp_Rows.Key_Pasport &#13;&#10;&#13;&#10;	INNER JOIN T_VPredm&#13;&#10;		ON Pr_Pasp.Key_Predm = T_VPredm.Key_Predm&#13;&#10;  &#13;&#10;	INNER JOIN T_Oper&#13;&#10;	    ON T_Oper.Cod_Oper = Pr_Pasp_Rows.Cod_Oper&#13;&#10;     AND T_Oper.Cod_Oper not like '02%'&#13;&#10;     AND T_Oper.Cod_Oper not like '03%'  &#13;&#10;  &#13;&#10;&#13;&#10;	LEFT OUTER JOIN Pr_Pasp_Ac &#13;&#10;		ON Pr_Pasp_Rows.Key_Pasp_Rows = Pr_Pasp_Ac.Key_Pasp_Rows&#13;&#10;&#13;&#10;&#13;&#10;  &#13;&#10; INNER JOIN Pr_Pasp_Status&#13;&#10;     ON Pr_Pasp_Status.Key_Pasp_Status = Pr_Pasp.Key_Pasp_Status&#13;&#10; LEFT JOIN Pr_Status_Oper&#13;&#10;     ON Pr_Status_Oper.Status_Oper = Pr_Pasp_Rows.Status_Oper  &#13;&#10;  &#13;&#10;&#13;&#10;WHERE &#13;&#10;	(Pr_Pasp_Rows.AcDate between :DateFrom and :DateTo)&#13;&#10;  AND Pr_Pasp_Rows.Status_Oper=2                 &#13;&#10;GROUP BY &#13;&#10;	Pr_Pasp.Cod_Pasp, &#13;&#10; Pr_Pasp.Date_Vip,&#13;&#10;	Pr_Pasp_Rows.Key_Pasport,&#13;&#10;	Pr_Pasp_Rows.AcDate, &#13;&#10;	Pr_Pasp.PaspKolvo, &#13;&#10;	T_VPredm.Cod_Predm, &#13;&#10;	T_VPredm.Name_Predm,&#13;&#10;&#13;&#10;  Pr_Pasp_Rows.Key_Pasp_Rows,    &#13;&#10;  Pr_Pasp_Rows.N_Oper_Sm,  &#13;&#10;  Pr_Pasp_Rows.TSm,                          &#13;&#10;  T_Oper.Name_K_Oper,        &#13;&#10;  T_Oper.Name_Oper,&#13;&#10;&#13;&#10;  Pr_Pasp_Status.Name_Pasp_Status,&#13;&#10;  Pr_Status_Oper.Name_Status_Oper          &#13;&#10;                                &#13;&#10;  &#13;&#10;HAVING&#13;&#10;	SUM(Isnull(Pr_Pasp_Ac.AcKolvo,0))  &#60;&#62;	Pr_Pasp.PaspKolvo&#13;&#10; AND Pr_Pasp_Rows.TSm &#62;0                  &#13;&#10;  &#13;&#10;                               " PropData=""/>
