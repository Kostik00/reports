    <TfrxERQuery Name="OperQRY" UserName="Îïåðàöèè" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="DECLARE @DateFrom datetime&#13;&#10;DECLARE @DateTo datetime&#13;&#10;DECLARE @DayKolvo int&#13;&#10;&#13;&#10;SET @DayKolvo=7&#13;&#10;&#13;&#10;SET @DateFrom='20200228'&#13;&#10;SET @DateTo = '20200302'&#13;&#10;&#13;&#10;&#13;&#10;SELECT &#13;&#10;	S_ALL_Sub.F_Name_Sub,&#13;&#10;	P_VKZ_Zap.Key_VKZ_Izdel,&#13;&#10;	P_VKZ_Oper.Key_Vkz_Oper,&#13;&#10;	Pr_Pln_Res.Name_Pln_Res,&#13;&#10;	Pr_Pasp.Cod_Pasp,&#13;&#10;&#13;&#10;	Pr_Pasp.Cod_Zak,&#13;&#10;	DSE.Cod_PRedm,&#13;&#10;	Pr_Pln_Res.Key_Pln_Res,&#13;&#10;	&#13;&#10;	&#13;&#10;	pr_pasp_rows.Key_Pasp_Rows,&#13;&#10;	pr_pasp_rows.N_Oper_Sm,&#13;&#10;	T_Oper.Name_Oper,&#13;&#10;	TDT.MaxDT AcDT,&#13;&#10;	pr_pasp_rows.DateVipUtv,&#13;&#10;	P_VKZ_Zap.Date_Vip Date_Vip_DSE,&#13;&#10;	PlnCr.DataVip DataVipIzdel,&#13;&#10;	Convert(Float,pr_pasp_rows.DateVipUtv - TDT.MaxDT) Dif,&#13;&#10;	pr_pasp_rows.TSm,&#13;&#10; &#13;&#10;	DSE.Name_Predm&#13;&#10;&#13;&#10;FROM &#13;&#10;	pr_pasp_rows&#13;&#10;	INNER JOIN T_Oper&#13;&#10;		ON T_Oper.Cod_Oper = pr_pasp_rows.Cod_Oper&#13;&#10;	LEFT JOIN P_VKZ_Oper&#13;&#10;		ON P_VKZ_Oper.Key_Pasp_Rows=pr_pasp_rows.Key_Pasp_Rows&#13;&#10;	LEFT JOIN P_VKZ_Zap	&#13;&#10;		ON P_VKZ_Zap.Key_VKZ_Zap=P_VKZ_Oper.Key_VKZ_Zap&#13;&#10;	&#13;&#10;	LEFT JOIN Pr_Pln_Res&#13;&#10;		ON Pr_Pln_Res.Key_Pln_Res= ISNULL(pr_pasp_rows.Key_Pln_ResUtv, P_VKZ_Oper.Key_Pln_Res)&#13;&#10;&#13;&#10;	INNER JOIN Pr_Pasp	&#13;&#10;		ON Pr_Pasp.Key_Pasport=pr_pasp_rows.Key_Pasport&#13;&#10;	INNER JOIN T_VPredm DSE&#13;&#10;		ON DSE.Key_Predm=Pr_Pasp.Key_Predm&#13;&#10;	LEFT JOIN&#13;&#10;	(&#13;&#10;		SELECT        Key_Pasp_Rows, Key_Sub_Ver, MIN(AC_Date) AS MinDT, MAX(AC_Date) AS MaxDT&#13;&#10;		FROM            Pr_Pasp_Ac&#13;&#10;		GROUP BY Key_Pasp_Rows, Key_Sub_Ver&#13;&#10;	) TDT&#13;&#10;	ON TDT.Key_Pasp_Rows=pr_pasp_rows.Key_Pasp_Rows&#13;&#10;&#13;&#10;	LEFT J