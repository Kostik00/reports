;&#10;&#13;&#10;				Pr_Pasp_Status.Name_Pasp_Status,&#13;&#10;				Pr_Pasp.Date_Pasp_Status,&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;				Pr_Pasp_Rows.Key_Pasp_Rows,&#13;&#10;				Pr_Pasp_Rows.N_Oper_Sm,&#13;&#10;&#13;&#10;				T_Oper.Name_Oper Name_K_Oper,&#13;&#10;				Pr_Status_Oper.Name_Status_Oper,&#13;&#10;				Pr_Pasp_Rows.AcDate,&#13;&#10;&#13;&#10;				Pr_Pasp_Rows.PrevCnt, &#13;&#10;				Pr_Pasp_Rows.PrevTrud&#13;&#10;&#13;&#10;&#13;&#10;			FROM &#13;&#10;				Pr_PK&#13;&#10;				INNER JOIN Pr_Pk_Izdel&#13;&#10;					ON Pr_Pk_Izdel.Key_Pr_Pk = Pr_PK.Key_Pr_PK&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;				LEFT JOIN Pr_Pk_Str&#13;&#10;					ON Pr_Pk_Str.Key_Pr_PK = Pr_PK.Key_Pr_PK&#13;&#10;&#13;&#10;				LEFT JOIN &#13;&#10;				(&#13;&#10;					SELECT &#13;&#10; 						Pr_Pk_Str.Key_Pk_Str,&#13;&#10;						T_SP.Key_Predm&#13;&#10;					FROM &#13;&#10;						Pr_Pk_Str&#13;&#10;						INNER JOIN  Pr_Pk_Str_TP&#13;&#10;							ON Pr_Pk_Str_TP.Key_Pk_Str = Pr_Pk_Str.Key_Pk_Str&#13;&#10;						INNER JOIN T_TP&#13;&#10;							ON T_TP.Key_TP = Pr_Pk_Str_TP.Key_TP &#13;&#10;						INNER JOIN T_SP	&#13;&#10;							ON T_Sp.Key_SP=T_TP.Key_SP&#13;&#10;					WHERE Pr_Pk_Str.Key_Pr_PK = @Key_Pr_Pk&#13;&#10;					UNION &#13;&#10;					SELECT &#13;&#10;						Pr_Pk_Str.Key_Pk_Str,&#13;&#10;						Pr_Pk_Str_Predm.Key_Predm&#13;&#10;					FROM &#13;&#10;						Pr_Pk_Str&#13;&#10;						INNER JOIN  Pr_Pk_Str_Predm&#13;&#10;							ON Pr_Pk_Str_Predm.Key_Pk_Str = Pr_Pk_Str.Key_Pk_Str	&#13;&#10;					WHERE Pr_Pk_Str.Key_Pr_PK = @Key_Pr_Pk&#13;&#10;				) Pr_Pk_Str_Predm&#13;&#10;					ON Pr_Pk_Str_Predm.Key_Pk_Str = Pr_Pk_Str.Key_Pk_Str&#13;&#10;&#13;&#10;				LEFT JOIN T_VPredm&#13;&#10;					ON T_VPredm.Key_Predm = Pr_Pk_Str_Predm.Key_Predm&#13;&#10;&#13;&#10;&#13;&#10;				INNER JOIN &#13;&#10;				(&#13;&#10;&#13;&#10;					SELECT     &#13;&#10;						Pr_Pk_Izdel.Key_Pr_Pk,&#13;&#10;						Pr_Pk_Izdel.Key_VKZ, &#13;&#10;						P_VKZ.Key_Predm, &#13;&#10;						SUM(P_VKZ.Kolvo) AS CntByVKZ&#13;&#10;					FROM         &#13