TPRows.Cod_Oper&#13;&#10;&#13;&#10;				LEFT JOIN P_VKZ_Zap&#13;&#10;					ON P_Vkz_Zap.Key_VKZ_Izdel = Pr_Pk_Izdel.Key_VKZ&#13;&#10;					AND P_VKZ_Zap.Key_Predm = Pr_Pk_Str_Predm.Key_Predm&#13;&#10;&#13;&#10;				LEFT JOIN Pr_Pasp&#13;&#10;					ON Pr_Pasp.Key_VKZ_Zap = P_VKZ_Zap.Key_VKZ_Zap&#13;&#10;&#13;&#10;				LEFT JOIN Pr_Pasp_Status&#13;&#10;					ON Pr_Pasp_Status.Key_Pasp_Status = Pr_Pasp.Key_Pasp_Status&#13;&#10;&#13;&#10;				LEFT JOIN Pr_Pasp_Rows&#13;&#10;					ON Pr_Pasp_Rows.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;					AND Pr_Pasp_Rows.Key_TPRows = Pr_Pk_Str_TProws.Key_TPRows&#13;&#10;&#13;&#10;				LEFT JOIN Pr_Status_Oper&#13;&#10;					ON Pr_Status_Oper.Status_Oper = Pr_Pasp_Rows.Status_Oper&#13;&#10;				LEFT JOIN T_Oper&#13;&#10;					ON T_Oper.Cod_Oper = Pr_Pasp_Rows.Cod_Oper	   &#13;&#10;	   )&#13;&#10;	   Pr_Pk_SmData&#13;&#10;	INNER JOIN&#13;&#10;       (&#13;&#10;            SELECT Pr_Pk_Str.Key_Pk_Str, COUNT( Pr_Pk_Str_Sub.Key_Pk_Str) CntAll&#13;&#10;            FROM Pr_Pk_Str&#13;&#10;            	LEFT JOIN Pr_Pk_Str_Sub&#13;&#10;            		ON Pr_Pk_Str_Sub.Key_Pk_Str = Pr_Pk_Str.Key_Pk_Str&#13;&#10;            WHERE Pr_Pk_Str.Key_Pr_PK = @Key_Pr_Pk&#13;&#10;            GROUP BY Pr_Pk_Str.Key_Pk_Str  &#13;&#10;         &#13;&#10;       ) TCntKontr&#13;&#10;       ON TCntKontr.Key_Pk_Str = Pr_Pk_SmData.Key_Pk_Str&#13;&#10;	INNER JOIN&#13;&#10;       (&#13;&#10;            SELECT Pr_Pk_Str.Key_Pk_Str, COUNT( Pr_Pk_Str_Sub.Key_Pk_Str) CntAc  &#13;&#10;            FROM Pr_Pk_Str&#13;&#10;            	LEFT JOIN Pr_Pk_Str_Sub&#13;&#10;            		 ON Pr_Pk_Str_Sub.Key_Pk_Str = Pr_Pk_Str.Key_Pk_Str&#13;&#10;               AND Pr_Pk_Str_Sub.IsAc=1                &#13;&#10;            WHERE Pr_Pk_Str.Key_Pr_PK = @Key_Pr_Pk&#13;&#10;            GROUP BY Pr_Pk_Str.Key_Pk_Str  &#13;&#10;         &#13;&#10;       ) TCntKontrAc    &#13;&#10;       ON TCntKontrAc.Key_Pk_Str = Pr_Pk_SmData.Key_Pk_Str&#13;&#10;&#13;&#10;  LEFT JOIN Pr_Pk_Str_PaspRows&#13;&#10