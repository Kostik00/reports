    <TfrxERQuery Name="OperSMQry" UserName="OperSMQry" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT &#13;&#10;    T_Oper.Cod_Oper_Gr,&#13;&#10;	T_Oper_Gr.Name_Oper_Gr,    &#13;&#10;	T_Oper.Name_K_Oper, &#13;&#10;	T_Oper.Name_Oper,&#13;&#10;	T_Oper.Cod_Oper,                       &#13;&#10;	SUM(Pr_Pasp_Rows.Tsm) AS SM&#13;&#10;FROM         &#13;&#10;	Pr_Pasp_Rows &#13;&#10;	INNER JOIN T_Oper &#13;&#10;		ON Pr_Pasp_Rows.Cod_Oper = T_Oper.Cod_Oper&#13;&#10;	INNER JOIN T_Oper_Gr&#13;&#10;		ON T_Oper_Gr.Cod_Oper_Gr = T_Oper.Cod_Oper_Gr&#13;&#10;WHERE      &#13;&#10;	(Pr_Pasp_Rows.Status_Oper = 2)&#13;&#10;	 AND (Pr_Pasp_Rows.AcDate &#62;= :DateFrom)&#13;&#10;  AND (Pr_Pasp_Rows.AcDate &#60;= :DateTo)                 &#13;&#10;	AND NOT exists(SELECT * FROM Pr_Tabel_MK where Pr_Tabel_MK.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows)&#13;&#10;&#13;&#10;GROUP BY &#13;&#10; T_Oper.Cod_Oper_Gr,&#13;&#10;	T_Oper_Gr.Name_Oper_Gr,    &#13;&#10;	T_Oper.Name_K_Oper, &#13;&#10;	T_Oper.Name_Oper,&#13;&#10; T_Oper.Cod_Oper    &#13;&#10;ORDER BY     &#13;&#10;	T_Oper.Cod_Oper_Gr,&#13;&#10;	T_Oper_Gr.Name_Oper_Gr,    &#13;&#10;	T_Oper.Name_K_Oper, &#13;&#10;	T_Oper.Name_Oper  " PropData=""/>
