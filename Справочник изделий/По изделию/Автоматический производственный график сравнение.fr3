    <TfrxERQuery Name="MinMaxQRY" UserName="MinMaxQRY" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="MainFrxDS" IgnoreDupParams="False" SQL.Text="DECLARE @Key_VKZ_Izdel int&#13;&#10;DECLARE @Key_PlnV1 int&#13;&#10;DECLARE @Key_PlnV2 int&#13;&#10;  &#13;&#10;&#13;&#10;SET @Key_VKZ_Izdel = :Key_VKZ_Izdel  &#13;&#10;SET @Key_PlnV1 = :Key_PlnV1    &#13;&#10;SET @Key_PlnV2 = :Key_PlnV2    &#13;&#10;  &#13;&#10;&#13;&#10;SELECT  &#13;&#10;       @Key_VKZ_Izdel as Key_VKZ_Izdel,  &#13;&#10;       MIN(MinDate) MinDate, &#13;&#10;       MAX(MaxDate) MaxDate&#13;&#10;FROM          &#13;&#10;(       &#13;&#10;      SELECT MIN(P_VKZ_PLN.DTZPlnMIN) MinDate, MAX(P_VKZ_PLN.DTVPlnMAX) MaxDate &#13;&#10;      FROM &#13;&#10;		dbo.P_VKZ P_VKZ&#13;&#10;		INNER JOIN P_VKZ_PLN&#13;&#10;			ON  P_VKZ_PLN.Key_VKZ =  P_VKZ.Key_VKZ&#13;&#10;			AND P_VKZ_PLN.Key_PlnV in ( @Key_PlnV1, @Key_PlnV2 )        &#13;&#10;&#13;&#10;      WHERE &#13;&#10;      		(P_VKZ.Key_VKZ_Izdel = @Key_VKZ_Izdel)&#13;&#10;&#13;&#10;/*    &#13;&#10;      UNION&#13;&#10;         &#13;&#10;      SELECT MIN(P_VKZ_PLN.DTZCRMIN) MinDate, MAX(P_VKZ_PLN.DTVCRMAX) MaxDate &#13;&#10;      FROM &#13;&#10;		dbo.P_VKZ P_VKZ&#13;&#10;		INNER JOIN P_VKZ_PLN&#13;&#10;			ON  P_VKZ_PLN.Key_VKZ =  P_VKZ.Key_VKZ&#13;&#10;			AND P_VKZ_PLN.Key_PlnV  in ( @Key_PlnV1, @Key_PlnV2 )&#13;&#10;      WHERE &#13;&#10;      		(P_VKZ.Key_VKZ_Izdel = @Key_VKZ_Izdel)&#13;&#10;&#13;&#10; */          &#13;&#10;  &#13;&#10;        &#13;&#10;) T1&#13;&#10; WHERE T1.MinDate is not null&#13;&#10; AND T1.MaxDate is not null&#13;&#10;&#13;&#10;   " PropData=""/>
    <TfrxERQuery Name="OperQRY" UserName="Îïåðàöèè" CloseDataSource="True" FieldAliases.Text="Key_Vkz_Oper=Key_Vkz_Oper&#13;&#10;Key_VKZ_Zap=Key_VKZ_Zap&#13;&#10;N_Oper=N_Oper&#13;&#10;N_Oper_SM=N_Oper_SM&#13;&#10;Lvl=Lvl&#13;&#10;DTZPLN1=DTZPLN1&#13;&#10;DTVPLN1=DTVPLN1&#13;&#10;DTZPLN2=DTZPLN2&#13;&#10;DTVPLN2=DTVPLN2&#13;&#10;IsActive=IsActive&#13;&#10;Cod_Oper=Cod_Oper&#13;&#10;Name_K_Oper=Name_K_Oper&#13;&#10;Name_Oper=Name_Oper&#13;&#10;Cod_Prof=Cod_Prof" BCDToCurrency="False" Master="PartZap" IgnoreDupParams="False" SQL.Text="SELECT &#13;&#10;       P_Vkz_Oper.Key_Vkz_Oper, &#13;&#10;	   &#13;&#10;	   Key_VKZ_Zap, &#13;&#10;	   N_Oper, &#13;&#10;	   N_Oper_SM, &#13;&#10;	   Lvl, &#13;&#10;	   &#13;&#10;	   &#13;&#10;	   P_VKZ_Oper_Pln1.DTZPLN DTZPLN1,&#13;&#10;	   P_VKZ_Oper_Pln1.DTVPLN DTVPLN1,&#13;&#10;	   P_VKZ_Oper_Pln2.DTZPLN DTZPLN2,&#13;&#10;	   P_VKZ_Oper_Pln2.DTVPLN DTVPLN2,&#13;&#10;&#13;&#10;&#13;&#10;	   IsActive, &#13;&#10;&#13;&#10;	   &#13;&#10;	   &#13;&#10;&#13;&#10;	   T_Oper.Cod_Oper, &#13;&#10;	   T_Oper.Name_K_Oper, &#13;&#10;	   T_Oper.Name_Oper, &#13;&#10;	   T_Oper.Cod_Prof&#13;&#10;&#13;&#10;	   &#13;&#10;	   &#13;&#10;&#13;&#10;FROM&#13;&#10;       P_Vkz_Oper&#13;&#10;       INNER JOIN T_Oper&#13;&#10;			ON T_Oper.Cod_Oper = P_Vkz_Oper.Cod_Oper&#13;&#10;       LEFT JOIN P_VKZ_Oper_Pln P_VKZ_Oper_Pln1&#13;&#10;			ON P_VKZ_Oper_Pln1.Key_Vkz_Oper = P_Vkz_Oper.Key_Vkz_Oper&#13;&#10;			AND P_VKZ_Oper_Pln1.Key_PlnV= :Key_PlnV1     &#13;&#10;       LEFT JOIN P_VKZ_Oper_Pln P_VKZ_Oper_Pln2&#13;&#10;			ON P_VKZ_Oper_Pln2.Key_Vkz_Oper = P_Vkz_Oper.Key_Vkz_Oper&#13;&#10;			AND P_VKZ_Oper_Pln2.Key_PlnV= :Key_PlnV2                                                                                              &#13;&#10;                  &#13;&#10;WHERE Key_VKZ_Zap = :Key_VKZ_Zap&#13;&#10;AND TSmSm &#62; 0                                    &#13;&#10;ORDER BY P_Vkz_Oper.N_Oper_SM DESC&#13;&#10;  " PropData=""/>
