<TfrxReport Version="5.5.2" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="41183,6439494097" ReportOptions.Description.Text="" ReportOptions.Name="Операции закрытые за период по подразделениям(05-04).fr3 Справочник изделий" ReportOptions.LastChange="43326,4438480324" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  vYear, vMonth, vDay: Word;&#13;&#10;&#13;&#10;function MySmPercent(P, F: double): double;&#13;&#10;begin&#13;&#10;  Result := 0;&#13;&#10;  if  P &#62; 0 then&#13;&#10;    Result := F * 100 / P;&#13;&#10;end;&#13;&#10;&#13;&#10;function FnPercent(F, FSm: double): double;&#13;&#10;begin&#13;&#10;  Result := 0;&#13;&#10;  if (FSm&#62;0) then&#13;&#10;       Result:=100*F/FSm;&#13;&#10;end;&#13;&#10;&#13;&#10;function Capitalize(AString: String): String;&#13;&#10;begin&#13;&#10;  Result:=Uppercase(Copy(AString, 1, 1))+Copy(AString, 2, Length(AString)-1);&#13;&#10;end;&#13;&#10;&#13;&#10;function CapitalizeLowercase(AString: String): String;&#13;&#10;var sTemp: String;&#13;&#10;begin&#13;&#10;  sTemp:=Lowercase(Trim(AString));&#13;&#10;  Result:=Uppercase(Copy(sTemp, 1, 1))+Copy(sTemp, 2, Length(sTemp)-1);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if TrudQRY.RecNo mod 2 = 1&#13;&#10;    then begin&#13;&#10;      ERQuery1SMTrudByPaspF.Color:=$F0F0F0;&#13;&#10;      Memo2.Color:= $F0F0F0;&#13;&#10;      Memo6.Color:= $F0F0F0;&#13;&#10;      Memo7.Color:= $F0F0F0;&#13;&#10;      Memo11.Color:=$F0F0F0;&#13;&#10;      Memo12.Color:=$F0F0F0;&#13;&#10;      Memo23.Color:=$F0F0F0;&#13;&#10;      Memo25.Color:=$F0F0F0;&#13;&#10;      Memo32.Color:=$F0F0F0;&#13;&#10;      Memo33.Color:=$F0F0F0&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      ERQuery1SMTrudByPaspF.Color:=clNone;&#13;&#10;      Memo2.Color:= clNone;&#13;&#10;      Memo6.Color:= clNone;&#13;&#10;      Memo7.Color:= clNone;&#13;&#10;      Memo11.Color:=clNone;&#13;&#10;      Memo12.Color:=clNone;&#13;&#10;      Memo23.Color:=clNone;&#13;&#10;      Memo25.Color:=clNone;&#13;&#10;      Memo32.Color:=clNone;&#13;&#10;      Memo33.Color:=clNone&#13;&#10;    end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure FillList;&#13;&#10;var&#13;&#10;  sTemp: String;&#13;&#10;begin&#13;&#10;  qTOperGr.Open;&#13;&#10;  qTOperGr.First;&#13;&#10;  while not qTOperGr.Eof&#13;&#10;    do begin&#13;&#10;      sTemp:=qTOperGr.FieldByName('Cod_Oper_Gr').AsString;&#13;&#10;      Case Length(sTemp) of&#13;&#10;        1: sTemp:='   '+sTemp;&#13;&#10;        2: sTemp:='  '+sTemp;&#13;&#10;        3: sTemp:=' '+sTemp;&#13;&#10;        end;&#13;&#10;      cblOpGrList.Items.Add('('+sTemp+') '+CapitalizeLowercase(qTOperGr.FieldByName('Name_Oper_Gr').AsString));&#13;&#10;      qTOperGr.Next;&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SetCheckedInList;&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;begin&#13;&#10;  for i:=0 to cblOpGrList.Items.Count-1&#13;&#10;    do if Trim(Copy(cblOpGrList.Items.Strings(i), 2, 4)) in ['02', '03', '06К', '07К', '51']&#13;&#10;         then cblOpGrList.Checked(i):=True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ShowOperGr;&#13;&#10;begin&#13;&#10;  ShowMessage('Begin procedure ShowOperGr!');&#13;&#10;  qTOperGrR2.Close;&#13;&#10;  qTOperGrR2.Open;&#13;&#10;  qTOperGrR2.First;&#13;&#10;  ShowMessage('Begin While! RecordCount = '+IntToStr(qTOperGrR2.RecordCount));&#13;&#10;  while not qTOperGrR2.Eof&#13;&#10;    do begin&#13;&#10;      ShowMessage('('+qTOperGrR2.FieldByName('Cod_Oper_Gr').AsString+') '+qTOperGrR2.FieldByName('Name_GrGr').AsString);&#13;&#10;      qTOperGrR2.Next;&#13;&#10;    end;&#13;&#10;  ShowMessage('End While');&#13;&#10;  ShowMessage('End procedure ShowOperGr!');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure SetCheckedInOperGr;&#13;&#10;var&#13;&#10;  i: Integer;&#13;&#10;begin&#13;&#10;