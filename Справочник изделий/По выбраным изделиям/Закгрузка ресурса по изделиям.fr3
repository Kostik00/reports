    <TfrxERQuery Name="TrudQRY" UserName="TrudQRY" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="MainFrxDS" IgnoreDupParams="False" SQL.Text="DECLARE @ReportNum int &#13;&#10;DECLARE @PlnResGr varchar(150)                  &#13;&#10;DECLARE @VPln int                                      &#13;&#10;DECLARE @Key_Pln_Res_VC int    &#13;&#10;DECLARE @M int&#13;&#10;DECLARE @Dt00 datetime&#13;&#10;DECLARE @Key_Vkz_Izdel int&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;     &#13;&#10;SET @ReportNum = :ReportNum    &#13;&#10;SET @PlnResGr = :PlnResGr  &#13;&#10;SET @VPln = :VPln  &#13;&#10;SET @M= :MM              &#13;&#10;SET @Key_Vkz_Izdel = :Key_Vkz_Izdel&#13;&#10;  &#13;&#10;SET @Key_Pln_Res_VC =  (SELECT Key_Pln_Res_VC FROM Pr_PlnV WHERE Key_PlnV=@VPln)  &#13;&#10;&#13;&#10;SELECT &#13;&#10;	T1.* ,&#13;&#10;	TAUTO.TZapA&#13;&#10;FROM&#13;&#10;&#13;&#10;(&#13;&#10; SELECT        &#13;&#10;    Usr_PlnStr.N_Str, &#13;&#10;    Usr_PlnStr.StrName, &#13;&#10;    Usr_PlnStr.DateFrom, &#13;&#10;    Usr_PlnStr.DateTo&#13;&#10;FROM            &#13;&#10;    Usr_PlnStr &#13;&#10;&#13;&#10;WHERE &#13;&#10;    Usr_PlnStr.ReportNum=@ReportNum&#13;&#10;    AND Usr_PlnStr.Usr = USER&#13;&#10;) T1&#13;&#10;LEFT JOIN                        &#13;&#10;(&#13;&#10;  &#13;&#10;  SELECT     &#13;&#10;	Usr_PlnStr.N_Str,&#13;&#10;	Usr_PlnStr.StrName,&#13;&#10;	SUM(Pr_Pln_Oper_Res.TZap) TZapA&#13;&#10;  &#13;&#10;  &#13;&#10;  FROM &#13;&#10;  	Usr_PlnStr&#13;&#10;  	INNER JOIN Pr_Pln_Oper_Res&#13;&#10;  		ON Pr_Pln_Oper_Res.Key_PlnV=@VPln&#13;&#10;  		AND Pr_Pln_Oper_Res.DayDate BETWEEN Usr_PlnStr.DateFrom AND Usr_PlnStr.DateTo&#13;&#10;  	INNER JOIN Pr_Pln_Res&#13;&#10;  		ON Pr_Pln_Res.Key_Pln_Res = Pr_Pln_Oper_Res.Key_Pln_Res&#13;&#10;  		AND Pr_Pln_Res.PlnResGr = @PlnResGr&#13;&#10;	INNER JOIN P_VKZ_Oper&#13;&#10;		ON P_VKZ_Oper.Key_Vkz_Oper = Pr_Pln_Oper_Res.Key_Vkz_Oper&#13;&#10;	INNER JOIN P_VKZ_Zap&#13;&#10;		ON P_VKZ_Zap.Key_VKZ_Zap = P_VKZ_Oper.Key_VKZ_Zap&#13;&#10;		AND P_VKZ_Zap.Key_VKZ_Izdel = @Key_Vkz_Izdel&#13;&#10;    &#13;&#10;   &#13;&#10;  &#13;&#10;  &#13;&#10;  WHERE &#13;&#10;  	Usr_PlnStr.ReportNum = @ReportNum         &#13;&#10;  	AND Usr_PlnStr.Usr = User&#13;&#10;  &#13;&#10;                           &#13;&#10;  GROUP BY 	&#13;&#10;      Usr_PlnStr.N_Str,&#13;&#10;      Usr_PlnStr.StrName       &#13;&#10;    &#13;&#10;  &#13;&#10;) TAUTO      &#13;&#10;ON TAUTO.N_Str  = T1.N_Str    &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;ORDER BY T1.N_Str" PropData=""/>
