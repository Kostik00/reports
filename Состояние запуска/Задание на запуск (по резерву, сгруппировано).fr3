    <TfrxERQuery Name="qMaster" UserName="dsMaster" CloseDataSource="False" FieldAliases.Text="Key_Id_Mat=Key_Id_Mat&#13;&#10;Key_VKZ_Zap=Key_VKZ_Zap&#13;&#10;VidSert=VidSert&#13;&#10;N_Sert=N_Sert&#13;&#10;Date_Sert=Date_Sert&#13;&#10;Cod_Plavka=Cod_Plavka&#13;&#10;Party=Party&#13;&#10;RegN_Sert=RegN_Sert&#13;&#10;PrintFlag=PrintFlag&#13;&#10;Cod_Predm=Cod_Predm&#13;&#10;Name_Predm=Name_Predm&#13;&#10;Sortam=Sortam&#13;&#10;OrderSortam=OrderSortam&#13;&#10;Marka=Marka&#13;&#10;Type_NTD_Sortam=Type_NTD_Sortam&#13;&#10;Name_NTD_Sortam=Name_NTD_Sortam&#13;&#10;Type_NTD_Marka=Type_NTD_Marka&#13;&#10;Name_NTD_Marka=Name_NTD_Marka&#13;&#10;TOInfo=TOInfo" BCDToCurrency="False" Master="MainFrxDS" IgnoreDupParams="False" SQL.Text="SELECT DISTINCT &#13;&#10;    A_VID_Mat.Key_Id_Mat,&#13;&#10;    P_VKZ_Zap.Key_VKZ_Zap,          &#13;&#10;    A_VID_Mat.VidSert, &#13;&#10;    A_VID_Mat.N_Sert, &#13;&#10;    A_VID_Mat.Date_Sert,&#13;&#10;    A_VID_Mat.Cod_Plavka,&#13;&#10;    A_VID_Mat.Party,&#13;&#10;    A_VID_Mat.RegN_Sert,&#13;&#10;    T_VPredm.PrintFlag,                           &#13;&#10;    T_VPredm.Cod_Predm, &#13;&#10;    T_VPredm.Name_Predm, &#13;&#10;    T_VPredm.Sortam, &#13;&#10;    T_VPredm.OrderSortam, &#13;&#10;    T_VPredm.Marka, &#13;&#10;    T_VPredm.Type_NTD_Sortam, &#13;&#10;    T_VPredm.Name_NTD_Sortam, &#13;&#10;    T_VPredm.Type_NTD_Marka, &#13;&#10;    T_VPredm.Name_NTD_Marka,  &#13;&#10;    dbo.A_IdMat_TOInfo(A_VID_Mat.Key_Id_Mat) AS TOInfo   &#13;&#10;  FROM            &#13;&#10;    P_VKZ&#13;&#10;      INNER JOIN P_VKZ_Zap_Det &#13;&#10;              ON P_VKZ_Zap_Det.Key_VKZ = P_VKZ.Key_VKZ &#13;&#10;      INNER JOIN P_VKZ_Zap &#13;&#10;              ON P_VKZ_Zap.Key_VKZ_Zap = P_VKZ_Zap_Det.Key_VKZ_Zap&#13;&#10;      INNER JOIN P_VMZ&#13;&#10;              ON P_VMZ.Key_VKZ_Zap = P_VKZ_Zap.Key_VKZ_Zap &#13;&#10;      INNER JOIN T_SPRows &#13;&#10;              ON T_SPRows.Key_SPRow = P_VMZ.Key_SPRow       &#13;&#10;      INNER JOIN A_Rezerv &#13;&#10; 