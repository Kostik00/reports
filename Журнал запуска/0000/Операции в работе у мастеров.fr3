    <TfrxERQuery Name="OperQuery" UserName="Îïåðàöèè" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="&#13;&#10;SELECT&#13;&#10;  Pr_Pasp.Key_Pasport,&#13;&#10;  Pr_Pasp.Cod_Pasp,&#13;&#10;  Pr_Pasp.Cod_Zak,&#13;&#10;  Pr_Pasp.Key_GrZap,&#13;&#10;&#13;&#10;&#13;&#10;  Pr_Pasp.Key_Predm,&#13;&#10;  T_Predm.Cod_Predm Cod_Predm,&#13;&#10;  T_Predm.Name_Predm Name_Predm,&#13;&#10;&#13;&#10;  T_Predm_Mat.Name_Predm Name_PredmMat,&#13;&#10;  T_Predm_Mat.Sortam Sortam_Mat,&#13;&#10;  T_Predm_Mat.Marka  Marka_Mat,&#13;&#10;  &#13;&#10;&#13;&#10;  T_Predm.Cod_Prin Cod_Prin,&#13;&#10;  T_Predm.Name_K_Prin Name_K_Prin,&#13;&#10;  Pr_Pasp.Key_Pasp_Status Key_Pasp_Status,&#13;&#10;  Pr_Pasp_Status.Name_Pasp_Status AS  Name_Pasp_Status,&#13;&#10;&#13;&#10;  Pr_Pasp_Rows.N_Oper_SM N_Oper_SM,&#13;&#10;  T_Oper.Name_Oper,&#13;&#10;  T_Oper.Name_K_Oper,&#13;&#10;  Pr_Pasp.PaspKolvo as PaspKolvo,&#13;&#10;  T_Oper_Gr.Name_GrGr,&#13;&#10;&#13;&#10;  S_Subjects.Name_Sub   as PaspMaster,&#13;&#10;&#13;&#10;&#13;&#10;  Pr_Pasp_Rows.Ad_Info,&#13;&#10;  Pr_Pasp_Rows.Key_Pasp_Rows,&#13;&#10;  Pr_Pasp_Rows.Tpz,&#13;&#10;  Pr_Pasp_Rows.Tsht,&#13;&#10;  Pr_Pasp_Rows.Tsm,&#13;&#10;  dbo.Pr_Get_Mk_TZap(Pr_Pasp_Rows.Key_Pasp_Rows) TZap,&#13;&#10;&#13;&#10;  Pr_Pasp_Rows.Tsm - isnull(dbo.Pr_Get_Mk_TZap(Pr_Pasp_Rows.Key_Pasp_Rows),0) TOst,&#13;&#10;&#13;&#10;  dbo.Pr_Get_Mk_KolvoZap(Pr_Pasp_Rows.Key_Pasp_Rows) KolvoZap,&#13;&#10; (Pr_Pasp.PaspKolvo - isnull(dbo.Pr_Get_Mk_KolvoZap(Pr_Pasp_Rows.Key_Pasp_Rows),0)) KolvoOst&#13;&#10;  ,TDT.DataZap,&#13;&#10;  A_ID_Mat.VidSert,&#13;&#10;  A_ID_Mat.N_Sert,&#13;&#10;  A_ID_Mat.Date_Sert,&#13;&#10;&#13;&#10;  Pr_Pasp_Rows.PlnDate,&#13;&#10;  SubOperMaster.Name_Sub OperMaster,&#13;&#10;&#13;&#10;  Pr_Pasp_Rows.DateZapUtv,&#13;&#10;	Pr_Pasp_Rows.DateVipUtv,&#13;&#10;	Pr_Pasp_Rows.Key_Sub_Ver_Utv,&#13;&#10;&#13;&#10;&#13;&#10;  Pr_Pasp_Rows.PrevCnt,&#13;&#10;  Pr_Pasp_Rows.PrevTrud&#13;&#10;&#13;&#10;FROM&#13;&#10;  Pr_Pasp&#13;&#10;&#13;&#10;  INNER JOIN&#13;&#10;  (&#13;&#10;&#13;&#10;      SELECT   DISTINCT     Pr_Pasp.Key_Pasport&#13;&#10;      FROM            Pr_Pasp INNER JOIN&#13;&#10;                               Pr_PaspDet ON Pr_Pasp.Key_Pasport = Pr_PaspDet.Key_Pasport INNER JOIN&#13;&#10;                               P_VKZ ON Pr_PaspDet.Key_VKZ = P_VKZ.Key_VKZ INNER JOIN&#13;&#10;                               P_VKZ AS Izdel ON P_VKZ.Key_VKZ_Izdel = Izdel.Key_VKZ&#13;&#10;      WHERE        (Izdel.Cod_SosZ = 'Ä')&#13;&#10;  )  DP                     &#13;&#10;  ON   Pr_Pasp.Key_Pasport = DP.Key_Pasport                                &#13;&#10;    &#13;&#10;&#13;&#10;  INNER JOIN T_VPredm T_Predm&#13;&#10;    ON  Pr_Pasp.Key_Predm = T_Predm.Key_Predm&#13;&#10;&#13;&#10;  INNER JOIN dbo.Pr_Pasp_Status   Pr_Pasp_Status&#13;&#10;    ON  Pr_Pasp.Key_Pasp_Status=Pr_Pasp_Status.Key_Pasp_Status&#13;&#10;&#13;&#10;&#13;&#10;  INNER JOIN dbo.Pr_Pasp_Rows Pr_Pasp_Rows&#13;&#10;      ON Pr_Pasp_Rows.Key_Pasport=Pr_Pasp.Key_Pasport&#13;&#10;      AND   &#13;&#10;		(&#13;&#10;			(Pr_Pasp_Rows.Status_Oper = 1 )&#13;&#10;			OR&#13;&#10;			((Pr_Pasp_Rows.PrevTrud &#60; 0.0001) AND (Pr_Pasp_Rows.Status_Oper = 0 ))&#13;&#10;		)&#13;&#10;&#13;&#10;&#13;&#10;  LEFT JOIN dbo.T_Oper  T_Oper&#13;&#10;     ON T_Oper.Cod_Oper=Pr_Pasp_Rows.Cod_Oper&#13;&#10;  LEFT JOIN dbo.T_Oper_Gr&#13;&#10;     ON T_Oper_Gr.Cod_Oper_Gr=T_Oper.Cod_Oper_Gr&#13;&#10;&#13;&#10;  LEFT JOIN S_Subjects&#13;&#10;		ON S_Subjects.Key_Sub = Pr_Pasp.Key_Sub_Master&#13;&#10;&#13;&#10;  LEFT JOIN S_Subjects  SubOperMaster&#13;&#10;		ON SubOperMaster.Key_Sub = Pr_Pasp_Rows.Key_Sub_Otv&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;  INNER JOIN&#13;&#10;  (&#13;&#10;  SELECT     Pr_Pasp.Key_Pasport, MIN(P_VKZ.DataZap) AS DataZap&#13;&#10;  FROM         Pr_Pasp INNER JOIN&#13;&#10;                        Pr_PaspDet ON Pr_Pasp.Key_Pasport = Pr_PaspDet.Key_Pasport INNER JOIN&#13;&#10;                        P_VKZ ON Pr_PaspDet.Key_VKZ = P_VKZ.Key_VKZ&#13;&#10;  GROUP BY Pr_Pasp.Key_Pasport&#13;&#10;  ) TDT&#13;&#10;  ON TDT.Key_Pasport =  Pr_Pasp.Key_Pasport&#13;&#10;&#13;&#10;  LEFT JOIN&#13;&#10;  (&#13;&#10;    SELECT     Pr_Pasp.Key_Pasport, MIN(Pr_SP.Key_ID_Mat) AS Key_ID_Mat&#13;&#10;    FROM         Pr_SP INNER JOIN&#13;&#10;                          Pr_Pasp ON Pr_SP.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;    GROUP BY Pr_Pasp.Key_Pasport&#13;&#10;   ) TM&#13;&#10;   ON TM.Key_Pasport =  Pr_Pasp.Key_Pasport&#13;&#10;&#13;&#10;   LEFT JOIN A_VID_Mat   A_ID_Mat&#13;&#10;   ON A_ID_Mat.Key_ID_Mat = TM.Key_ID_Mat&#13;&#10;&#13;&#10;  LEFT JOIN T_VPredm T_Predm_Mat&#13;&#10;    ON  T_Predm_Mat.Key_Predm = A_ID_Mat.Key_Predm&#13;&#10;&#13;&#10;  INNER JOIN&#13;&#10;  (&#13;&#10;    SELECT     Pr_Pasp.Key_Pasport, P_VKZ_1.Cod_SosZ&#13;&#10;    FROM&#13;&#10;            Pr_Pasp&#13;&#10;            INNER JOIN Pr_PaspDet&#13;&#10;              ON Pr_Pasp.Key_Pasport = Pr_PaspDet.Key_Pasport&#13;&#10;            INNER JOIN P_VKZ&#13;&#10;              ON Pr_PaspDet.Key_VKZ = P_VKZ.Key_VKZ&#13;&#10;            INNER JOIN P_VKZ AS P_VKZ_1&#13;&#10;              ON P_VKZ.Key_VKZ_Izdel = P_VKZ_1.Key_VKZ&#13;&#10;&#13;&#10;    WHERE  (P_VKZ_1.Cod_SosZ = 'Ä')&#13;&#10;    GROUP BY Pr_Pasp.Key_Pasport, P_VKZ_1.Cod_SosZ&#13;&#10;  ) TDI&#13;&#10;  ON TDI.Key_Pasport =   Pr_Pasp.Key_Pasport&#13;&#10;&#13;&#10;&#13;&#10;WHERE&#13;&#10;  (Pr_Pasp.Key_Pasp_Status in&#13;&#10;    (select DISTINCT Key_Pasp_Status from dbo.Pr_Pasp_StatusR WHERE Is_Member(RoleName) = 1))&#13;&#10;  AND Pr_Pasp.Key_Pasp_Status between 1 and 5&#13;&#10;  AND (T_Oper.Cod_Oper not like '02%')                         &#13;&#10;  AND (T_Oper.Cod_Oper not like '03%')                         &#13;&#10;  AND Pr_Pasp_Rows.Tsm &#62; 0                        &#13;&#10;&#13;&#10;ORDER BY &#13;&#10;       PaspMaster, &#13;&#10;       T_Oper_Gr.Name_GrGr,&#13;&#10;       T_Oper.Name_K_Oper,                                 