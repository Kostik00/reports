    <TfrxERQuery Name="TaskQRY" UserName="Задания" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT        &#13;&#10;	Pr_Tabel.Key_Tabel, &#13;&#10;	Pr_Tabel.Date_Tabel, &#13;&#10;	T_VPredm.Cod_Predm,&#13;&#10;	T_VPredm.Name_Predm,&#13;&#10;	Pr_Pasp.Cod_Pasp,&#13;&#10; Pr_Pasp.Cod_Zak,                         &#13;&#10;	Pr_Pasp_Rows.N_Oper_Sm, &#13;&#10;	T_Oper.Name_K_Oper,&#13;&#10; S_SubVer_Tabel.F_Name_Sub F_Name_Sub_Tabel,                                                &#13;&#10;	S_Subjects_Tabel.Name_Sub  Name_Sub_Tabel,&#13;&#10;	S_Subjects_RazrabTP.Name_Sub Name_Sub_RazrabTP,&#13;&#10;	S_Subjects_RazrabPasp.Name_Sub Name_Sub_RazrabPasp&#13;&#10;FROM            &#13;&#10;	Pr_Tabel &#13;&#10;	INNER JOIN Pr_Tabel_MK &#13;&#10;		ON Pr_Tabel.Key_Tabel = Pr_Tabel_MK.Key_Tabel &#13;&#10;	INNER JOIN Pr_Pasp_Rows &#13;&#10;		ON Pr_Tabel_MK.Key_Pasp_Rows = Pr_Pasp_Rows.Key_Pasp_Rows &#13;&#10;	INNER JOIN Pr_Pasp &#13;&#10;		ON Pr_Pasp_Rows.Key_Pasport = Pr_Pasp.Key_Pasport &#13;&#10;	INNER JOIN T_VPredm&#13;&#10;		ON T_VPredm.Key_Predm=Pr_Pasp.Key_Predm&#13;&#10;&#13;&#10;	INNER JOIN T_TPRows &#13;&#10;		ON Pr_Pasp_Rows.Key_TPRows = T_TPRows.Key_TPRows &#13;&#10;	INNER JOIN T_TP &#13;&#10;		ON T_TPRows.Key_TP = T_TP.Key_TP &#13;&#10;	INNER JOIN S_SubVer  AS S_SubVer_RazrabPasp &#13;&#10;		ON Pr_Pasp.Key_Sub_Ver_Pasp = S_SubVer_RazrabPasp.Key_Sub_Ver &#13;&#10;	INNER JOIN S_Subjects AS S_Subjects_RazrabPasp &#13;&#10;		ON S_SubVer_RazrabPasp.Key_Sub = S_Subjects_RazrabPasp.Key_Sub &#13;&#10;	INNER JOIN T_Oper &#13;&#10;		ON Pr_Pasp_Rows.Cod_Oper = T_Oper.Cod_Oper &#13;&#10;	INNER JOIN S_SubVer AS S_SubVer_RazrabTP &#13;&#10;		ON T_TP.Key_Sub_Ver_Razrab = S_SubVer_RazrabTP.Key_Sub_Ver &#13;&#10;	INNER JOIN S_Subjects  AS S_Subjects_RazrabTP &#13;&#10;		ON S_SubVer_RazrabTP.Key_Sub = S_Subjects_RazrabTP.Key_Sub &#13;&#10;	INNER JOIN S_SubVer AS S_SubVer_Tabel &#13;&#10;		ON Pr_Tabel.Key_Sub_Ver = S_SubVer_Tabel.Key_Sub_Ver &#13;&#10;	INNER JOIN S_Subjects  AS S_Subjects_Tabel &#13;&#10;		ON S_SubVer_Tabel.Key_Sub = S_Subjects_Tabel.Key_Sub&#13;&#10;WHERE&#13;&#10;      Pr_Tabel.Date_Tabel &#62;= :DateFrom&#13;&#10;  AND Pr_Tabel.Date_Tabel &#60;= :DateTo                                              &#13;&#10;AND&#13;&#10;(    &#13;&#10;	S_Subjects_RazrabTP.Name_Sub like :FIO&#13;&#10; OR         &#13;&#10;	S_Subjects_RazrabPasp.Name_Sub like :FIO                    &#13;&#10;)      &#13;&#10;  &#13;&#10;ORDER BY&#13;&#10;S_Subjects_Tabel.Name_Sub, S_SubVer_Tabel.F_Name_Sub,           &#13;&#10;Pr_Pasp.Cod_Zak, T_VPredm.Cod_Predm, Pr_Pasp.Cod_Pasp, Pr_Pasp_Rows.N_Oper_Sm                                          " PropData=""/>
