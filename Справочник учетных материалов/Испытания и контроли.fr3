    <TfrxERQuery Name="PrimenQRY" UserName="Применяемость" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="MQRY" IgnoreDupParams="False" SQL.Text="SELECT     Pr_Pasp.Key_Pasport, Pr_Pasp.Cod_Pasp, Pr_Pasp.Cod_Zak, Pr_SP.Key_ID_Mat, T_VPredm.Cod_Predm, T_VPredm.Name_Predm, T_VPredm.Sortam, &#13;&#10;                      Pr_SPDetail.ProfRazm, Pr_SPDetail.Kolvo, Pr_SPDetail.M_Zag&#13;&#10;FROM         Pr_SP INNER JOIN&#13;&#10;                      Pr_Pasp ON Pr_SP.Key_Pasport = Pr_Pasp.Key_Pasport INNER JOIN&#13;&#10;                      T_VPredm ON Pr_Pasp.Key_Predm = T_VPredm.Key_Predm INNER JOIN&#13;&#10;                      Pr_SPDetail ON Pr_SP.Key_PrSP = Pr_SPDetail.Key_PrSp&#13;&#10;WHERE     (Pr_SP.Key_ID_Mat = :Key_ID_Mat )  " PropData="05704C65667403B8000470546F7002680A506172616D657465727301010C27000000204E616D653D224B65795F49445F4D6174222044617461547970653D226674556E6B6E6F776E220000"/>
    <TfrxERQuery Name="TTQry" UserName="TTQRY" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="MQRY" IgnoreDupParams="False" SQL.Text="  SELECT &#13;&#10;&#13;&#10;      A_ID_Mat.Key_Id_Mat,&#13;&#10;      A_ID_Mat.VidSert    VidSert,&#13;&#10;      A_ID_Mat.N_Sert     N_Sert,&#13;&#10;      A_ID_Mat.Date_Sert  Date_Sert,&#13;&#10;&#13;&#10;      Pr_Zakl.Vid_Zakl  Vid_Zakl,&#13;&#10;      Pr_Zakl.Num_Zakl  Num_Zakl,&#13;&#10;      Pr_Zakl.Data_Zakl Data_Zakl,&#13;&#10;&#13;&#10;      T_Sert_Isp.Cod_TT Cod_TT,    &#13;&#10;      dbo.T_Sert_Isp.Key_Isp,&#13;&#10;      T_TechTreb.Name_TT              &#13;&#10;&#13;&#10;&#13;&#10;  FROM&#13;&#10;    A_VID_Mat A_ID_Mat&#13;&#10;    INNER JOIN T_Sert_Isp&#13;&#10;       ON T_Sert_Isp.Key_Sert_Pos = A_ID_Mat.Key_Sert_Pos&#13;&#10;&#13;&#10;    INNER JOIN T_TechTreb&#13;&#10;       ON T_TechTreb.Cod_TT = T_Sert_Isp.Cod_TT                                                &#13;&#10;&#13;&#10;&#13;&#10;    LEFT JOIN Pr_VNaprZakl  Pr_NaprZakl&#13;&#10;       ON Pr_NaprZakl.Key_NaprZakl=T_Sert_Isp.Key_NaprZakl&#13;&#10;&#13;&#10;    LEFT JOIN dbo.Pr_Zakl Pr_Zakl&#13;&#10;       ON Pr_NaprZakl.Key_Zakl=Pr_Zakl.Key_Zakl&#13;&#10;&#13;&#10;  WHERE&#13;&#10;        (&#13;&#10;          (Pr_NaprZakl.Key_Zakl IS NOT NULL)&#13;&#10;          OR&#13;&#10;          (T_Sert_Isp.Key_NaprZakl IS NULL)&#13;&#10;        )  -- если есть заключение&#13;&#10;    AND&#13;&#10;    (&#13;&#10;       (&#13;&#10;        (T_Sert_Isp.Key_NaprZakl in (SELECT Key_NaprZakl FROM Pr_NaprZakl WHERE Key_Pasport=A_ID_Mat.Key_Pasport ))&#13;&#10;        and Pr_Zakl.Key_Zakl is not NULL&#13;&#10;       ) -- если мы проводили доп испытания&#13;&#10;    )&#13;&#10;    AND                   &#13;&#10;      Key_Id_Mat=:Key_Id_Mat  " PropData="05704C65667403B8000470546F7003B4000A506172616D657465727301010C27000000204E616D653D224B65795F49645F4D6174222044617461547970653D226674556E6B6E6F776E220000"/>
