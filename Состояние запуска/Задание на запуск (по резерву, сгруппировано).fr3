    <TfrxERQuery Name="qDetail" UserName="dsDetail" CloseDataSource="False" FieldAliases.Text="Cod_Predm=Cod_Predm&#13;&#10;Name_Predm=Name_Predm&#13;&#10;Sortam=Sortam&#13;&#10;PrintFlag_Mat=PrintFlag_Mat&#13;&#10;Cod_Predm_Mat=Cod_Predm_Mat&#13;&#10;Name_Predm_Mat=Name_Predm_Mat&#13;&#10;Sortam_Mat=Sortam_Mat&#13;&#10;Type_NTD_Sortam_Mat=Type_NTD_Sortam_Mat&#13;&#10;Name_NTD_Sortam_Mat=Name_NTD_Sortam_Mat&#13;&#10;Marka_Mat=Marka_Mat&#13;&#10;Type_NTD_Marka_Mat=Type_NTD_Marka_Mat&#13;&#10;Name_NTD_Marka_Mat=Name_NTD_Marka_Mat&#13;&#10;Prof_Razm=Prof_Razm&#13;&#10;Key_VMZ=Key_VMZ&#13;&#10;Key_VKZ_Zap=Key_VKZ_Zap&#13;&#10;KolvoZapPln=KolvoZapPln&#13;&#10;KolvoZapPlnCount=KolvoZapPlnCount&#13;&#10;KolDT=KolDT&#13;&#10;KolDTCount=KolDTCount" BCDToCurrency="False" Master="qMaster" IgnoreDupParams="False" SQL.Text="SELECT       &#13;&#10;    T_VPredm.Cod_Predm, &#13;&#10;    T_VPredm.Name_Predm, &#13;&#10;    T_VPredm.Sortam,&#13;&#10;    T_Predm_Mat.PrintFlag PrintFlag_Mat,                                      &#13;&#10;    T_Predm_Mat.Cod_Predm Cod_Predm_Mat,&#13;&#10;    T_Predm_Mat.Name_Predm Name_Predm_Mat,&#13;&#10;    T_Predm_Mat.Sortam Sortam_Mat,&#13;&#10;    T_Predm_Mat.Type_NTD_Sortam Type_NTD_Sortam_Mat,&#13;&#10;    T_Predm_Mat.Name_NTD_Sortam Name_NTD_Sortam_Mat,&#13;&#10;    T_Predm_Mat.Marka  Marka_Mat,&#13;&#10;    T_Predm_Mat.Type_NTD_Marka Type_NTD_Marka_Mat,&#13;&#10;    T_Predm_Mat.Name_NTD_Marka Name_NTD_Marka_Mat,&#13;&#10;    T_SPRows.Prof_Razm,&#13;&#10;    P_VMZ.Key_VMZ,&#13;&#10;    P_VKZ_Zap_Det.Key_VKZ_Zap,          &#13;&#10;    MAX(A_Rezerv.KolDT)  AS  KolvoZapPln,&#13;&#10;    COUNT(A_Rezerv.KolDT) AS KolvoZapPlnCount,                                    &#13;&#10;    MAX(P_VKZ_Zap.KolvoZap)  AS  KolDT,&#13;&#10;    COUNT(P_VKZ_Zap.KolvoZap) AS KolDTCount        &#13;&#10;  FROM  &#13;&#10;    P_VKZ&#13;&#10;      INNER JOIN P_VKZ_Zap_Det &#13;&#10;             	ON P_VKZ_Zap_Det.Key_VKZ = P_VKZ.Key_VKZ &#13;&#10;      INNER JOIN P_VKZ_Zap 