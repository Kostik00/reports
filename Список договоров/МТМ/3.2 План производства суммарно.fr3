    <TfrxERQuery Name="ERQuery1" UserName="ERQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="DECLARE @DT1 DateTime&#13;&#10;&#13;&#10;SET @DT1 = :DateFrom&#13;&#10;&#13;&#10;DECLARE @PrintDolg int&#13;&#10;SET @PrintDolg = :PrintDolg&#13;&#10;  &#13;&#10;  &#13;&#10;&#13;&#10;SELECT&#13;&#10;	DatePart(yy,DateP) Y,&#13;&#10;	DatePart(q,DateP) Q,&#13;&#10;	DatePart(m,DateP) M,&#13;&#10;	SUM(Ost) Ost,&#13;&#10;	SUM(SMMassa) SMMassa                               &#13;&#10;FROM&#13;&#10;	( &#13;&#10;		select&#13;&#10;			T001.DateP,&#13;&#10;			T001.Ost*dbo.D_FnGetKurs(D_Dogovor.Cod_Val,@DT1 ) Ost ,&#13;&#10;			T_VPredm.Massa * D_Plan.Kolvo SMMassa                                                          &#13;&#10;		FROM  &#13;&#10;			dbo.D_FnTblPlnOst( @DT1 ) T001&#13;&#10;			INNER JOIN D_Product&#13;&#10;				ON  D_Product.Key_Product =  T001.Key_Product&#13;&#10;   INNER JOIN D_Dogovor&#13;&#10;    ON D_Dogovor.Cod_Dogov= D_Product.Cod_Dogov&#13;&#10;    AND D_Dogovor.Cod_SosD in ('Ä','È')        &#13;&#10;			INNER JOIN D_Plan&#13;&#10;				ON D_Plan.Key_Plan = T001.Key_Plan                                                                 &#13;&#10;			LEFT JOIN T_VPredm&#13;&#10;				ON T_VPredm.Key_Predm =  IsNULL( D_Plan.Key_Predm, D_Product.Key_Izdel)                                                                                                             &#13;&#10;         &#13;&#10;	WHERE                                                &#13;&#10;					T001.Key_DocType = 1     &#13;&#10;				AND T001.Key_VidPlan =0&#13;&#10;    AND T001.Ost &#62; 0    &#13;&#10;    AND ((T001.DateP &#62;= @DT1) OR ( @PrintDolg = 1 ))                        &#13;&#10;	       &#13;&#10;	 &#13;&#10; &#13;&#10;	) TOstA001&#13;&#10;GROUP BY &#13;&#10;	DatePart(yy,DateP),&#13;&#10;	DatePart(q,DateP) ,&#13;&#10;	DatePart(m,DateP)&#13;&#10;&#13;&#10;ORDER BY Y,Q,M  " PropData=""/>
