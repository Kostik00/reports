    <TfrxERQuery Name="ERQuery1" UserName="ERQuery1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT     &#13;&#10;       Pr_Pasp.Key_Pasport, &#13;&#10;       Pr_Pasp.Cod_Pasp, &#13;&#10;       Pr_Pasp_Status.Name_Pasp_Status, &#13;&#10;       Pr_Pasp_Status.Key_Pasp_Status, &#13;&#10;       Pr_Pasp.Key_Predm, &#13;&#10;       T_VPredm.Cod_Predm,&#13;&#10;       T_VPredm.Name_Predm, &#13;&#10;       T_Oper_Gr.Name_GrGr, &#13;&#10;        Pr_Pasp.Cod_Zak,&#13;&#10;       T_VPredm.OrderCod_Predm,&#13;&#10;       Pr_Pasp_Rows.N_Oper_SM,  &#13;&#10;       Isnull(T_TpRows.TPz,0)+Isnull( T_TpRows.TSht,0)*Isnull(Pr_Pasp.PaspKolvo,1)   NormByTP                                              &#13;&#10;       ,T_Status.Name_T_Status&#13;&#10;	      ,T_TP.Cod_TP&#13;&#10;       ,Pr_Pasp_Rows.AcDate                 &#13;&#10;FROM         &#13;&#10;       Pr_Pasp &#13;&#10;       INNER JOIN  Pr_Pasp_Status &#13;&#10;               ON Pr_Pasp.Key_Pasp_Status = Pr_Pasp_Status.Key_Pasp_Status &#13;&#10;       LEFT JOIN  T_VPredm &#13;&#10;               ON Pr_Pasp.Key_Predm = T_VPredm.Key_Predm &#13;&#10;       LEFT JOIN Pr_Pasp_Rows &#13;&#10;               ON Pr_Pasp.Key_Pasport = Pr_Pasp_Rows.Key_Pasport &#13;&#10;       LEFT JOIN T_Oper &#13;&#10;               ON Pr_Pasp_Rows.Cod_Oper = T_Oper.Cod_Oper &#13;&#10;       LEFT JOIN T_Oper_Gr &#13;&#10;               ON T_Oper.Cod_Oper_Gr = T_Oper_Gr.Cod_Oper_Gr&#13;&#10;       LEFT JOIN T_TpRows &#13;&#10;               ON T_TpRows.Key_TpRows  =  Pr_Pasp_Rows.Key_TpRows&#13;&#10;&#13;&#10;       LEFT JOIN T_TP&#13;&#10;				ON T_TP.Key_TP = Pr_Pasp.Key_TP&#13;&#10;	   LEFT JOIN T_Status&#13;&#10;				ON T_Status.Key_T_Status = T_TP.Key_T_Status                                         &#13;&#10;WHERE     &#13;&#10;       ((Pr_Pasp_Status.Key_Pasp_Status in (-1,1,2,3,4,5)) OR (0 = :ShowInWork))                 &#13;&#10;       AND (ABS(isnull(Pr_Pasp_Rows.Tsm,0)) &#60;= 0.00001)&#13;&#10;       AND (Pr_Pasp_Rows.Cod_Oper not in ('0000'))                                     &#13;&#10;       AND (T_Oper_Gr.Name_GrGr not like 'Контр%')&#13;&#10;       AND ((Pr_Pasp_Rows.Status_Oper = 1) OR (0 = :ShowOperInWork))                &#13;&#10;       AND ((T_TpRows.Key_V_TP is NULL) OR (0 = :ShowVTp))&#13;&#10;       AND (( Isnull(T_TpRows.TPz,0)+Isnull( T_TpRows.TSht,0) &#60;=0.000000001 )OR( 0 = :SkipTPNorm ))                                     &#13;&#10;         &#13;&#10;/*                                            &#13;&#10;GROUP BY &#13;&#10;       Pr_Pasp.Key_Pasport, &#13;&#10;       Pr_Pasp.Cod_Pasp, &#13;&#10;       Pr_Pasp_Status.Name_Pasp_Status, &#13;&#10;       Pr_Pasp_Status.Key_Pasp_Status, &#13;&#10;       Pr_Pasp.Key_Predm, &#13;&#10;       T_VPredm.Cod_Predm,&#13;&#10;       T_VPredm.Name_Predm, &#13;&#10;       T_Oper_Gr.Name_GrGr, &#13;&#10;       Pr_Pasp.Cod_Zak, &#13;&#10;       T_VPredm.OrderCod_Predm,&#13;&#10;       Pr_Pasp_Rows.N_Oper_SM&#13;&#10;--       ,&#13;&#10;--       T_TpRows.TPz,&#13;&#10;--       T_TpRows.TSht           &#13;&#10;                          */          &#13;&#10;ORDER BY Pr_Pasp_Status.Name_Pasp_Status, Pr_Pasp.Cod_Zak, T_VPredm.OrderCod_Predm  " PropData=""/>
