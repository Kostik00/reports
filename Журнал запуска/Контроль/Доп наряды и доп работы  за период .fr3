r = T_Oper.Cod_Oper &#13;&#10;       INNER JOIN Pr_Pasp &#13;&#10;               ON Pr_Pasp_Rows.Key_Pasport = Pr_Pasp.Key_Pasport&#13;&#10;       INNER JOIN T_VPredm&#13;&#10;               ON Pr_Pasp.Key_Predm = T_VPredm.Key_Predm &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;                 &#13;&#10;WHERE     &#13;&#10;      -- ((Pr_Pasp_Rows.Key_TPRows IS NULL) OR (Pr_Pasp_Rows.Row_Src &#60;&#62;1))                &#13;&#10;       ((Pr_Pasp_Rows.Row_Src &#60;&#62;1)OR(Pr_Pasp_Rows.Key_TPRows IS NULL))                       &#13;&#10;       AND &#13;&#10;           Pr_Pasp_Ac.Ac_Date &#62;= @DateFrom&#13;&#10;       AND Pr_Pasp_Ac.Ac_Date &#60;= @DateTo&#13;&#10;                                &#13;&#10;           &#13;&#10;  &#13;&#10;GROUP BY &#13;&#10;       Pr_Pasp.Cod_Zak,             &#13;&#10;       T_VPredm.Cod_Predm, &#13;&#10;       T_VPredm.Name_Predm, &#13;&#10;	      Pr_Pasp_Rows.N_Oper_Sm,&#13;&#10;       Pr_Pasp_Rows.NumDopN,&#13;&#10;       Pr_Pasp_Rows.DateDopN,                 &#13;&#10;  &#13;&#10;       Pr_Pasp_Rows.Key_Pasp_Rows,               &#13;&#10;       T_Oper.Name_K_Oper, &#13;&#10;       T_Oper.Name_Oper,&#13;&#10;       S_SubVer.F_Name_Sub,             &#13;&#10;       T_VPredm.OrderCod_Predm&#13;&#10;         &#13;&#10;ORDER BY Pr_Pasp.Cod_Zak, T_VPredm.OrderCod_Predm, Pr_Pasp_Rows.N_Oper_Sm             " PropData=""/>
